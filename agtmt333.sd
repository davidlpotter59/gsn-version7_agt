    while agqendorse:app_no   = g_app_no and
          agqendorse:prem_no  = l_prem_no and
          agqendorse:build_no = l_build_no and
          agqendorse:code     = l_code
        begin
        delete agqendorse

        next agqendorse
        end
    end
end

