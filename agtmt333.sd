    while agqaddin:app_no   = g_app_no and
          agqaddin:prem_no  = l_prem_no and
          agqaddin:build_no = l_build_no and
          agqaddin:code     = l_code
        begin
        delete agqaddin

        next agqaddin
        end
    end

