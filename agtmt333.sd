procedure check_for_Red
begin
l_sub = 1
while l_sub < 29
    begin
    if l_value[l_sub] = 1 then
        begin
        l_code = l_policywide_code[l_sub]
        if l_location_specific[l_sub] = 0 then
            begin
            l_prem_no  = 0
            l_build_no = 0
            g_prem_no  = 0
            g_build_no = 0
            end
        else
            begin
            l_prem_no  = agqgeneral:prem_no
            l_build_no = agqgeneral:build_no
            g_prem_no  = agqgeneral:prem_no
            g_build_no = agqgeneral:build_no
            end
        do check_popup
        if l_popup_complete = 1 then
            do remove_red(l_the_tag[l_sub],"black",l_value[l_sub],l_popup_screen[l_sub])
        end

    l_sub = l_sub + 1
    end

end

procedure check_popup
begin
l_popup_complete = 0
l_code = l_policywide_code[l_sub]
if l_location_specific[l_sub] = 0 then
    begin
    l_prem_no  = 0
    l_build_no = 0
    g_prem_no  = 0
    g_build_no = 0
    end
else
    begin
    l_prem_no  = agqgeneral:prem_no
    l_build_no = agqgeneral:build_no
    g_prem_no  = agqgeneral:prem_no
    g_build_no = agqgeneral:build_no
    end

if l_code one of "BU0609", "BU0309" then
    begin
    access agqaddin, set agqaddin:app_no   = g_app_no,
                         agqaddin:prem_no  = l_prem_no,
                         agqaddin:build_no = l_build_no,
                         agqaddin:code     = l_code,  generic

    while agqaddin:app_no   = g_app_no and
          agqaddin:prem_no  = l_prem_no and
          agqaddin:build_no = l_build_no and
          agqaddin:code     = l_code
        begin
        if agqaddin:name[1] <> "" then
            begin
            l_popup_complete = 1
            end
        else
            begin
            l_popup_complete = 0
            end

        next agqaddin
        end

    end

if l_code one of "BU0315" then
    begin
    access agqendorse, set agqendorse:app_no   = g_app_no,
                           agqendorse:prem_no  = l_prem_no,
                           agqendorse:build_no = l_build_no,
                           agqendorse:code     = l_code,  generic

    while agqendorse:app_no   = g_app_no and
          agqendorse:prem_no  = l_prem_no and
          agqendorse:build_no = l_build_no and
          agqendorse:code     = l_code
        begin
        if agqendorse:description[1] <> "" then
            begin
            l_popup_complete = 1
            end
        else
            begin
            l_popup_complete = 0
            end

        next agqendorse
        end

    end

if l_code one of "BU0601", "BU0603", "BU0604", "BU0605", "BU0606",
                 "BU0607", "BU0608", "BU0610", "BU0616" then
    begin
    access agqaddin, set agqaddin:app_no   = g_app_no,
                         agqaddin:prem_no  = l_prem_no,
                         agqaddin:build_no = l_build_no,
                         agqaddin:code     = l_code,  generic

    while agqaddin:app_no   = g_app_no and
          agqaddin:prem_no  = l_prem_no and
          agqaddin:build_no = l_build_no and
          agqaddin:code     = l_code
        begin
        if agqaddin:name[1] <> "" and
           agqaddin:address[1] <> "" and
           agqaddin:city <> "" and
           agqaddin:str_state <> "" and
           agqaddin:zipcode <> 0 then
            l_popup_complete = 1
        else
            l_popup_complete = 0

        next agqaddin
        end
    end
end

