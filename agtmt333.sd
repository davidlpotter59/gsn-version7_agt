procedure delete_popup_files
begin
l_code = l_policywide_code[l_sub]
if l_code one of "BU0601", "BU0603", "BU0604", "BU0605", "BU0606",
                 "BU0607", "BU0608", "BU0609", "BU0610", "BU0616", "BU0309"  then
    begin
    access agqaddin, set agqaddin:app_no   = g_app_no,
                         agqaddin:prem_no  = l_prem_no,
