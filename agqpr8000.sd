access agqmaster, set agqmaster:app_no = agqprint1:app_no, generic

access agqamt, set agqamt:app_no = agqprint1:app_no, generic

l_lob = agqamt:line_of_business[2]
access agqmastsupp, set agqmastsupp:app_no         = agqprint1:app_no,
                        agqmastsupp:line_of_business = l_lob, generic

access sfscompany, set sfscompany:company_id = agqname:company_id, generic

access sfsagent, set sfsagent:company_id = agqname:company_id,
                     sfsagent:agent_no   = agqname:agent_no, generic

access sfsline, set sfsline:company_id       = agqname:company_id,
                    sfsline:line_of_business = agqname:line_of_business,
                    sfsline:lob_subline      = "00", generic

l_username = username
access sfsemail, set sfsemail:company_id = agqname:company_id,
                     sfsemail:user_id    = l_username, generic

if sfsline:lob_code one of "CPORTFOLIO" then
    l_state = agqmastsupp:state
else
    l_state    = agqmaster:state
l_lob_code = sfsline:lob_code
access scipscontrol, set scipscontrol:company_id = agqname:company_id,
                         scipscontrol:state      = l_state,
                         scipscontrol:lob_code   = l_lob_code, generic

L_CSEXEC_APPLICATION = "PRINT"/raw

Access SFSCSEXEC, Set SFSCSEXEC:APPLICATION = L_CSEXEC_APPLICATION, Exact

If SFSCSEXEC:APPLICATION_NUMBER = 0 Then
    Begin
    L_CSEXEC_NUMBER = 99999
    End
Else
    Begin
    L_CSEXEC_NUMBER = SFSCSEXEC:APPLICATION_NUMBER + 1

    Change SFSCSEXEC
        Begin
        SFSCSEXEC:APPLICATION_NUMBER = L_CSEXEC_NUMBER
        End
    End

l_dec = "agqpr8000.cq"
l_file_name = l_dec
deletefile(l_dec)

do create_header
access agqgeneral1,
   set agqgeneral1:app_no = agqprint1:app_no, generic

if agqgeneral1:app_no = agqprint1:app_no then
  begin
    include "agqiso100a.inc"
    writefile(l_dec,"%s","<033>&l0H")
    writefile(l_dec,"%s",l_reset)
  end

access agqliability,
   set agqliability:app_no = agqprint1:app_no, generic

if agqliability:app_no = agqprint1:app_no then
  begin
    include "agqiso100b.inc"
    writefile(l_dec,"%s","<033>&l0H")
    writefile(l_dec,"%s",l_reset)
  end

access agqartisan,
   set agqartisan:app_no = agqprint1:app_no, generic

if agqartisan:app_no = agqprint1:app_no then
  begin
    include "agqiso100c.inc"
    writefile(l_dec,"%s","<033>&l0H")
    writefile(l_dec,"%s",l_reset)
  end

access agqinland,
   set agqinland:app_no = agqprint1:app_no, generic

if agqinland:app_no = agqprint1:app_no then
  begin
    include "agqiso100d.inc"
    writefile(l_dec,"%s","<033>&l0H")
    writefile(l_dec,"%s",l_reset)
  end

access agqcrime1,
   set agqcrime1:app_no = agqprint1:app_no, generic

if agqcrime1:app_no = agqprint1:app_no then
  begin
    include "agqiso100e.inc"
    writefile(l_dec,"%s","<033>&l0H")
    writefile(l_dec,"%s",l_reset)
  end

access agqend,
   set agqend:app_no = agqprint1:app_no,
       agqend:prem_no  = 0,
       agqend:build_no = 0, generic

if agqend:app_no = agqprint1:app_no and
   agqend:prem_no  = 0 and
   agqend:build_no = 0 then
    begin
    writefile(l_dec,"%s",l_arial)
    writefile(l_dec,"%s","<NL>")
    writefile(l_dec,"%s","<NL>")
    writefile(l_dec,"%s","<033>&a300h+0V")
    writefile(l_dec,"%s","POLICY WIDE ENDORSEMENTS")
    writefile(l_dec,"%s","<NL>")
    writefile(l_dec,"%s","<NL>")
    writefile(l_dec,"%s","<033>&a300h+0V")
    writefile(l_dec,"%s","FORM EDITION")
    writefile(l_dec,"%s","<033>&a1500h+0V")
    writefile(l_dec,"%s","DESCRIPTION")
    writefile(l_dec,"%s","<033>&a4000h+0V")
    writefile(l_dec,"%s","PREMIUM")
    writefile(l_dec,"%s","<NL>")
    end

while agqend:app_no = agqprint1:app_no and
      agqend:prem_no  = 0 and
      agqend:build_no = 0
        begin
        writefile(l_dec,"%s","<033>&a300h+0V")
        writefile(l_dec,"%s",trun(l_arial))
        writefile(l_dec,"%s",trun(agqend:form_edition))
        writefile(l_dec,"%s","<033>&a1500h+0V")
        writefile(l_dec,"%s",trun(agqend:description))
        if agqend:premium <> 0 then
            begin
            writefile(l_dec,"%s","<033>&a4000h+0V")
            writefile(l_dec,"%s",str(agqend:premium,"ZZZ,ZZZ.99-"))
            end
        else
        if agqend:manual_premium <> 0 then
            begin
            writefile(l_dec,"%s","<033>&a4000h+0V")
            writefile(l_dec,"%s",str(agqend:manual_premium,"ZZZ,ZZZ.99-"))
            end
        writefile(l_dec,"%s","<NL>")

        next agqend
        end

closefile(l_macro)

