        iconline = 0 ;


        Global Unsigned Ascii Number G_app_NO[11] ,
                                     G_LINE_OF_BUSINESS[4],
                                     g_prem_no[4],
                                     g_build_no[4],
                                     G_STATE[2],
                                     g_sub_code[4] ,
                                     g_form[1];


        Global String G_COMPANY_ID[10] ,
                      g_inquiry[1] ,
                      g_article[20],
                      g_answer[1],
                      g_code[7] ;

        local array[13] of tag l_the_tag[15] ;

        local unsigned ascii number l_sub_code[4],
                                    l_form[2],
                                    l_sub_code_1[4],
                                    l_popup_complete[1],
                                    l_remove_form[1],
                                    L_STATE[2],
                                    l_agent_no[4],
                                    l_liability_code[2] ,
                                    l_limit[8],
                                    l_option_1,
                                    l_option_2,
                                    l_option_3,
                                    l_option_4,
                                    l_option_5,
                                    l_option_6,
                                    l_option_7,
                                    l_option_8,
                                    l_option_9,
                                    l_option_10,
                                    l_agqend_sub_code[4] ;

        local string l_code[7],
                     l_code_1[7],
                     l_code_2[7],
                     l_code_3[7],
                     l_code_4[7],
                     l_system_id[15],
                     l_st_type[6],
                     l_st_direction[2],
                     l_username[15],
                     l_add_endorsement[1],
                     l_Form_edition[18],
                     L_COMPANY_ID[10] ,
                     l_blank[10],
                     l_screen[10],
                     l_valid[1]="N",
                     l_description[55] ;

        Access agqendorse1,
           Set agqendorse1:app_NO   = G_app_NO ,
               agqendorse1:prem_no  = g_prem_no,
               agqendorse1:build_no = g_build_no,
               agqendorse1:code     = g_code ;

        Include "stdkeys7.inc"

