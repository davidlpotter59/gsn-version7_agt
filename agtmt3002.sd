Update
If UPDATEOK = "YES" And
   Abort = "NO" Then
    Begin
    access agqpersonal, set agqpersonal:app_no = g_app_no, generic

    Access PLSPREPRINTED, Set PLSPREPRINTED:COMPANY_ID       = agqpersonal:COMPANY_ID,
                              PLSPREPRINTED:STATE            = agqpersonal:state,
                              PLSPREPRINTED:LINE_OF_BUSINESS = agqpersonal:LINE_OF_BUSINESS, Generic

    while PLSPREPRINTED:COMPANY_ID       = agqpersonal:COMPANY_ID and
          PLSPREPRINTED:STATE            = agqpersonal:state and
          PLSPREPRINTED:LINE_OF_BUSINESS = agqpersonal:LINE_OF_BUSINESS
        begin
        If agqpersonal:EFF_DATE >= PLSPREPRINTED:REFF_DATE Then
            Begin
            L_CODE_1 = PLSPREPRINTED:CODE[13] -- mpl91
            L_CODE_2 = PLSPREPRINTED:CODE[14] -- mpl92
            L_CODE_3 = PLSPREPRINTED:CODE[11] -- mpl93
            L_CODE_4 = PLSPREPRINTED:CODE[12] -- mpl90
            End

        Next PLSPREPRINTED
        End

    If L_CODE_1 <> "" Then
        Begin
        L_CODE = L_CODE_1
        Do ACCESS_agqend
        If L_ADD_ENDORSEMENT = "Y" And
           agqendorse1:PROFESSIONAL_USE = 1 Then
            Do ADD_agqend
        If L_ADD_ENDORSEMENT = "N" And
           agqendorse1:PROFESSIONAL_USE = 0 Then
            Do DELETE_agqend
        End

    If L_CODE_2 <> "" Then
        Begin
        L_CODE = L_CODE_2
        Do ACCESS_agqend
        If L_ADD_ENDORSEMENT = "Y" And
           agqendorse1:ELECTRONIC_DAMAGE = 1 Then
            Do ADD_agqend
        If L_ADD_ENDORSEMENT = "N" And
           agqendorse1:ELECTRONIC_DAMAGE = 0 Then
            Do DELETE_agqend
        End

    If L_CODE_3 <> "" Then
        Begin
        L_CODE = L_CODE_3
        Do ACCESS_agqend
        If ((L_ADD_ENDORSEMENT = "Y" And
           agqendorse1:STAMP_THEFT = 1) Or
           (L_ADD_ENDORSEMENT = "Y" And
           agqendorse1:COIN_THEFT = 1)) Then
            Do ADD_agqend
        If L_ADD_ENDORSEMENT = "N" And
           agqendorse1:STAMP_THEFT = 0 And
           agqendorse1:COIN_THEFT = 0 Then
            Do DELETE_agqend
        End

    If L_CODE_4 <> "" Then
        Begin
        L_CODE = L_CODE_4
        Do ACCESS_agqend
        If L_ADD_ENDORSEMENT = "N" And
           agqendorse1:OPTION[10] = 0 Then
            Do DELETE_agqend
        End

    function = "redisplay"
    End

procedure definition

procedure check_popup(number l_sub_code_1)
begin
l_popup_complete = 0
access agqdesc1, set agqdesc1:app_no   = g_app_no,
                     agqdesc1:prem_no  = g_prem_no,
                     agqdesc1:build_no = g_build_no,
                     agqdesc1:code     = g_code,
                     agqdesc1:article  = g_article, generic

if agqdesc1:article <> "" and
   agqdesc1:limit <> 0 and
   agqdesc1:description <> "" then
    l_popup_complete = 1

end

procedure display_blue(tag l_tag, string l_color, string l_display_screen)
begin
    setproperty(l_tag,foregroundcolor,l_color)
end

procedure display_red(tag l_tag, string l_color, string l_display_screen)
begin
    setproperty(l_tag,foregroundcolor,l_color)
end

procedure display_view (number l_sub_code_1, string l_screen_name)
begin
if agqendorse1:option[l_sub_code_1] = 1 then
    begin
    l_screen = l_screen_name
    display modal screen l_screen
    function = "redisplay"
    abort()
    end

end

Procedure ACCESS_agqend
Begin
Access agqend, Set agqend:app_no   = G_app_no ,
                   agqend:PREM_NO  = G_PREM_NO,
                   agqend:BUILD_NO = G_BUILD_NO, Generic

L_ADD_ENDORSEMENT = "Y"
while agqend:app_no   = G_app_no and
      agqend:PREM_NO  = G_PREM_NO and
      agqend:BUILD_NO = G_BUILD_NO
        begin
        If agqend:CODE = L_CODE Then
            L_ADD_ENDORSEMENT = "N"

        Next agqend
        End

End

Procedure ADD_agqend
Begin
Access agqend, Set agqend:app_no   = G_app_no ,
                   agqend:PREM_NO  = G_PREM_NO,
                   agqend:BUILD_NO = G_BUILD_NO, Generic

L_agqend_SUB_CODE = 0
while agqend:app_no   = G_app_no and
      agqend:PREM_NO  = G_PREM_NO and
      agqend:BUILD_NO = G_BUILD_NO
        begin
        L_agqend_SUB_CODE = agqend:SUB_CODE

        Next agqend
        End

If L_agqend_SUB_CODE <= G_SUB_CODE Then
    L_agqend_SUB_CODE = L_agqend_SUB_CODE + 20
Else
    L_agqend_SUB_CODE = L_agqend_SUB_CODE + 10

Access SFSOPTEND, Set SFSOPTEND:COMPANY_ID       = agqpersonal:COMPANY_ID,
                      SFSOPTEND:STATE            = agqpersonal:state,
                      SFSOPTEND:LINE_OF_BUSINESS = agqpersonal:LINE_OF_BUSINESS,
                      SFSOPTEND:CODE             = L_CODE, Generic

while SFSOPTEND:COMPANY_ID       = agqpersonal:COMPANY_ID and
      SFSOPTEND:STATE            = agqpersonal:state and
      SFSOPTEND:LINE_OF_BUSINESS = agqpersonal:LINE_OF_BUSINESS and
      SFSOPTEND:CODE             = L_CODE
        begin
        If agqpersonal:EFF_DATE >= SFSOPTEND:EFF_DATE Then
            Begin
            L_FORM_EDITION = SFSOPTEND:FORM_EDITION
            L_DESCRIPTION = SFSOPTEND:DESCRIPTION
            End

        Next SFSOPTEND
        End

Add agqend
    Begin
    agqend:app_no               =       G_app_no
    agqend:PREM_NO              =       G_PREM_NO
    agqend:BUILD_NO             =       G_BUILD_NO
    agqend:SUB_CODE             =       L_agqend_SUB_CODE
    agqend:CODE                 =       L_CODE
    agqend:FORM_EDITION         =       L_FORM_EDITION
    agqend:DESCRIPTION          =       L_DESCRIPTION
    agqend:PREMIUM              =       0
    End

End

Procedure DELETE_agqend
Begin
Access agqend, Set agqend:app_no   = G_app_no ,
                   agqend:PREM_NO  = G_PREM_NO,
                   agqend:BUILD_NO = G_BUILD_NO, Generic

while agqend:app_no   = G_app_no and
      agqend:PREM_NO  = G_PREM_NO and
      agqend:BUILD_NO = G_BUILD_NO
        begin
        If agqend:CODE = L_CODE Then
            Delete agqend

        Next agqend
        End

End

Procedure GET_LIMIT
Begin
Access agqdesc1, Set agqdesc1:app_no   = G_app_no,
                     agqdesc1:PREM_NO  = G_PREM_NO ,
                     agqdesc1:BUILD_NO = G_BUILD_NO ,
                    agqdesc1:CODE = G_CODE,
                    agqdesc1:ARTICLE = G_ARTICLE, Generic

L_LIMIT = 0
while agqdesc1:app_no   = G_app_no and
      agqdesc1:PREM_NO  = G_PREM_NO and
      agqdesc1:BUILD_NO = G_BUILD_NO and
      agqdesc1:CODE     = G_CODE and
      agqdesc1:ARTICLE  = G_ARTICLE
        begin
        L_LIMIT = L_LIMIT + agqdesc1:LIMIT

        Next agqdesc1
        End

End

Procedure DELETE_agqdesc1
Begin
Access agqdesc1, Set agqdesc1:app_no   = G_app_no,
                     agqdesc1:PREM_NO  = G_PREM_NO ,
                     agqdesc1:BUILD_NO = G_BUILD_NO ,
                    agqdesc1:CODE      = G_CODE,
                    agqdesc1:ARTICLE   = G_ARTICLE, Generic

while agqdesc1:app_no   = G_app_no and
      agqdesc1:PREM_NO  = G_PREM_NO and
      agqdesc1:BUILD_NO = G_BUILD_NO and
      agqdesc1:CODE     = G_CODE and
      agqdesc1:ARTICLE  = G_ARTICLE
        begin
        Delete agqdesc1

        Next agqdesc1
        End

End

Procedure ACCESS_agqdesc1
Begin
Access agqdesc1, Set agqdesc1:app_no   = G_app_no,
                     agqdesc1:PREM_NO  = G_PREM_NO ,
                     agqdesc1:BUILD_NO = G_BUILD_NO ,
                     agqdesc1:CODE     = G_CODE,
                    agqdesc1:ARTICLE = G_ARTICLE, Generic

If agqdesc1:app_no   = G_app_no And
   agqdesc1:PREM_NO  = G_PREM_NO And
   agqdesc1:BUILD_NO = G_BUILD_NO And
   agqdesc1:CODE     = G_CODE And
   agqdesc1:ARTICLE  = G_ARTICLE Then
    Begin
    If FUNCTION = "CHANGE" Then
        Begin
        G_ARTICLE = agqdesc1:ARTICLE
        If G_ANSWER = "Y" Then
           Begin
           Switch(G_ARTICLE)
             Case "CAMERAS",
                  "COIN COLLECTIONS",
                  "FURS",
                  "GOLFER'S EQUIPMENT",
                  "SILVERWARE",
                  "SPORTING EQUIPMENT",
                  "STAMP COLLECTION"     : L_SCREEN = "agtmt3002a"
             Case "JEWELRY"              : L_SCREEN = "agtmt3002b"
             Case "FINE ARTS"            : L_SCREEN = "agtmt3002c"
             Case "MUSICAL INSTRUMENTS"  : L_SCREEN = "agtmt3002d"
             End
          Display Modal Screen L_SCREEN
          End
        End
    End
Else
    Begin
    Switch(G_ARTICLE)
      Case "CAMERAS",
           "COIN COLLECTIONS",
           "FURS",
           "GOLFER'S EQUIPMENT",
           "SILVERWARE",
           "SPORTING EQUIPMENT",
           "STAMP COLLECTION"     : L_SCREEN = "agtmt3002a"
      Case "JEWELRY"              : L_SCREEN = "agtmt3002b"
      Case "FINE ARTS"            : L_SCREEN = "agtmt3002c"
      Case "MUSICAL INSTRUMENTS"  : L_SCREEN = "agtmt3002d"
      End
    Display Modal Screen L_SCREEN
    End

End

