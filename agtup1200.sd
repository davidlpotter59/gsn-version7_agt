If L_CODE = "MPL15" Then
    Begin
    L_RECORD_KEY = "SP"
    L_COMPANY_ID = agqPERSONAL:COMPANY_ID
    L_STATE = agqPERSONAL:STATE
    L_LINE_OF_BUSINESS = agqpersonal:LINE_OF_BUSINESS
