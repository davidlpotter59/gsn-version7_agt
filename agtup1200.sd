        End

        Change agqendorse1
            Begin
            agqendorse1:PREMIUM[10] = L_FINE_ARTS_PREMIUM
            End

        End

    L_UPDATE_SFPEND_PREMIUM = L_CAMERA_PREMIUM + L_COIN_PREMIUM + L_FURS_PREMIUM +
