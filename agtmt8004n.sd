        Local String L_CODE[7],
                     L_CODE_1[7],
                     l_code_2[7],
                     L_FORM_EDITION[18],
                     L_DESCRIPTION[55],
                     L_ADD_ENDORSEMENT[1]="N" ,
                     l_desc[50] ,
                     l_company_id[10],
                     l_username[15],
                     l_desc_1[50] ;

        Access agqendorse6,
           Set agqendorse6:app_no     = G_app_no,
