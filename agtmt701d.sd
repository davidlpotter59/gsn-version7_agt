    while sfspolicywideoptend:company_id       = agqcontractor_alias:company_id and
          sfspolicywideoptend:state            = agqcontractor_alias:state and
          sfspolicywideoptend:line_of_business = agqcontractor_alias:line_of_business and
          sfspolicywideoptend:class_code       = l_class_code
        {
        if (agqcontractor_alias:eff_date >= sfspolicywideoptend:eff_date and
           (agqcontractor_alias:eff_date <= sfspolicywideoptend:exp_date or
           sfspolicywideoptend:exp_date = 0)) then
            {
            if ((sfspolicywideoptend:code = "GIM121" and
               agqcontractor:form = 4) or
               (sfspolicywideoptend:code = "MSC715D" and
               l_ok_to_add = "Y") or
               (sfspolicywideoptend:code = "MSC713D" and
               agqcontractor:form <> 4) or
               sfspolicywideoptend:code not one of "GIM121", "MSC715D", "MSC713D") then
                begin
                l_tag_position = val(sfspolicywideoptend:tag_name[7,8])
                if l_tag_position <=  16 then
                    {
                    if l_left_side_no < l_tag_position then
                        {
                        l_left_side_no = l_tag_position
                        }
                    }
                else
                    {
                    if L_right_side_no < l_tag_position then
                        {
                        l_right_side_no = l_tag_position
                        }
                    }
                l_policywide_code[l_tag_position] = sfspolicywideoptend:code
                if sfspolicywideoptend:location_specific = 0 then
                    begin
                    l_prem_no_1  = 0
                    l_build_no_1 = 0
                    end
                else
                    begin
                    l_prem_no_1  = l_prem_no
                    l_build_no_1 = l_build_no
                    end
                l_value[l_tag_position] = 0
                do read_agqend
                l_view_desc[l_tag_position] = trun(sfspolicywideoptend:POLICY_WIDE_FORM_DESC)
                                              + sfspolicywideoptend:form_print
                l_popup_screen[l_tag_position] = sfspolicywideoptend:popup_screen

                l_the_tag[l_tag_position] = tag(trun(sfspolicywideoptend:tag_name))
                l_location_specific[l_tag_position] = sfspolicywideoptend:location_specific
                l_mandatory_form[l_tag_position] = sfspolicywideoptend:mandatory_form
                l_refer_to_co[l_tag_position] = sfspolicywideoptend:refer_to_company
                do printguidetext(l_the_tag[l_tag_position],trun(l_view_desc[l_tag_position]))
                end
            }

        next sfspolicywideoptend
        }

    end

if agqcontractor_alias:app_no   = g_app_no and
   agqcontractor_alias:prem_no  = l_prem_no and
   agqcontractor_alias:build_no = l_build_no then
    begin
    l_class_code = 99999
    access sfspolicywideoptend,
       set sfspolicywideoptend:company_id       = agqcontractor_alias:company_id,
           sfspolicywideoptend:state            = agqcontractor_alias:state,
           sfspolicywideoptend:line_of_business = agqcontractor_alias:line_of_business,
           sfspolicywideoptend:class_code       = l_class_code, generic

    while sfspolicywideoptend:company_id       = agqcontractor_alias:company_id and
          sfspolicywideoptend:state            = agqcontractor_alias:state and
          sfspolicywideoptend:line_of_business = agqcontractor_alias:line_of_business and
          sfspolicywideoptend:class_code       = l_class_code
        {
        if (agqcontractor_alias:eff_date >= sfspolicywideoptend:eff_date and
           (agqcontractor_alias:eff_date <= sfspolicywideoptend:exp_date or
           sfspolicywideoptend:exp_date = 0)) then
            {
            l_tag_position = val(sfspolicywideoptend:tag_name[7,8])
            if l_tag_position <=  16 then
                {
                if l_left_side_no < l_tag_position then
                    {
                    l_left_side_no = l_tag_position
                    }
                }
            else
                {
                if L_right_side_no < l_tag_position then
                    {
                    l_right_side_no = l_tag_position
                    }
                }
            l_policywide_code[l_tag_position] = sfspolicywideoptend:code
            if sfspolicywideoptend:location_specific = 0 then
                begin
                l_prem_no_1  = 0
                l_build_no_1 = 0
                end
            else
                begin
                l_prem_no_1  = l_prem_no
                l_build_no_1 = l_build_no
                end
            l_value[l_tag_position] = 0
            do read_agqend
            l_view_desc[l_tag_position] = trun(sfspolicywideoptend:POLICY_WIDE_FORM_DESC)
                                          + sfspolicywideoptend:form_print
            l_popup_screen[l_tag_position] = sfspolicywideoptend:popup_screen

            l_the_tag[l_tag_position] = tag(trun(sfspolicywideoptend:tag_name))
            l_location_specific[l_tag_position] = sfspolicywideoptend:location_specific
            l_mandatory_form[l_tag_position] = sfspolicywideoptend:mandatory_form
            do printguidetext(l_the_tag[l_tag_position],trun(l_view_desc[l_tag_position]))
            }

        next sfspolicywideoptend
        }

    end

    l_loop = 1
    while l_loop <= 32
    {
    if l_view_desc[l_loop] = "" then
        {
        switch(l_loop)
          case 1  : hide(t_remove_checkbox1)
          case 2  : hide(t_remove_checkbox2)
          case 3  : hide(t_remove_checkbox3)
          case 4  : hide(t_remove_checkbox4)
          case 5  : hide(t_remove_checkbox5)
          case 6  : hide(t_remove_checkbox6)
          case 7  : hide(t_remove_checkbox7)
          case 8  : hide(t_remove_checkbox8)
          case 9  : hide(t_remove_checkbox9)
          case 10 : hide(t_remove_checkbox10)
          case 11 : hide(t_remove_checkbox11)
          case 12 : hide(t_remove_checkbox12)
          case 13 : hide(t_remove_checkbox13)
          case 14 : hide(t_remove_checkbox14)
          case 15 : hide(t_remove_checkbox15)
          case 16 : hide(t_remove_checkbox16)
          end
        }
    else
    if l_view_desc[l_loop] <> "" then
        {
        switch(l_loop)
          case 1  : show(t_remove_checkbox1)
          case 2  : show(t_remove_checkbox2)
          case 3  : show(t_remove_checkbox3)
          case 4  : show(t_remove_checkbox4)
          case 5  : show(t_remove_checkbox5)
          case 6  : show(t_remove_checkbox6)
          case 7  : show(t_remove_checkbox7)
          case 8  : show(t_remove_checkbox8)
          case 9  : show(t_remove_checkbox9)
          case 10 : show(t_remove_checkbox10)
          case 11 : show(t_remove_checkbox11)
          case 12 : show(t_remove_checkbox12)
          case 13 : show(t_remove_checkbox13)
          case 14 : show(t_remove_checkbox14)
          case 15 : show(t_remove_checkbox15)
          case 16 : show(t_remove_checkbox16)
          end
        }

    l_loop = l_loop + 1
    }

END

procedure printguidetext(tag current_tag, string display_string)

BEGIN
    Setproperty(current_tag, text, display_string)
END

procedure read_agqend
begin
  access agqend,
     set agqend:app_no   = g_app_no,
         agqend:prem_no  = l_prem_no_1,
         agqend:build_no = l_build_no_1, generic

while agqend:app_no   = g_app_no and
      agqend:prem_no  = l_prem_no_1 and
      agqend:build_no = l_build_no_1
  begin
    if agqend:code     = l_policywide_code[l_tag_position] and
       agqend:prem_no  = l_prem_no_1 and
       agqend:build_no = l_build_no_1 then
        begin
        l_value[l_tag_position] = 1
        end
--    else
--        begin
--        l_value[l_tag_position] = 0
--        end
    next agqend
  end
end

