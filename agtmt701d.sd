procedure check_for_Red
begin
l_sub = 1
while l_sub < 32
    begin
    if l_value[l_sub] = 1 then
        begin
        l_code = l_policywide_code[l_sub]
        if l_location_specific[l_sub] = 0 then
            begin
            l_prem_no  = 0
            l_build_no = 0
            g_prem_no  = 0
            g_build_no = 0
            end
        else
            begin
            l_prem_no  = agqcontractor:prem_no
            l_build_no = agqcontractor:build_no
            g_prem_no  = agqcontractor:prem_no
            g_build_no = agqcontractor:build_no
            end
        do check_popup
        if l_popup_complete = 1 then
            do remove_red(l_the_tag[l_sub],"black",l_value[l_sub],l_popup_screen[l_sub])
        end

    l_sub = l_sub + 1
    end

end

procedure check_popup
begin
l_popup_complete = 0
l_code = l_policywide_code[l_sub]
if l_location_specific[l_sub] = 0 then
    begin
    l_prem_no  = 0
    l_build_no = 0
    g_prem_no  = 0
    g_build_no = 0
    end
else
    begin
    l_prem_no  = agqcontractor:prem_no
    l_build_no = agqcontractor:build_no
    g_prem_no  = agqcontractor:prem_no
    g_build_no = agqcontractor:build_no
    end

if l_code = "MSC125" then
    begin
    if agqcontractor:exterior_glass <> 0 then
        l_popup_complete = 1
    end

if l_code one of "MCL157", "MCL158" then
    begin
    access agqendorse3, set agqendorse3:app_no   = g_app_no,
