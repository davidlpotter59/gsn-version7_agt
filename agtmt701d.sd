procedure update_endorsements(number l_sub)
begin
if l_value[l_sub] = 1 then
    begin
    if l_location_specific[l_sub] = 0 then
        begin
        l_prem_no  = 0
        l_build_no = 0
        g_prem_no  = 0
        g_build_no = 0
        end
    else
        begin
        l_prem_no  = agqcontractor:prem_no
        l_build_no = agqcontractor:build_no
        g_prem_no  = agqcontractor:prem_no
        g_build_no = agqcontractor:build_no
        end
    l_code = l_policywide_code[l_sub]
    g_code = l_policywide_code[l_sub]
    do add_endorsements
    do check_endorsements
    if l_code one of "MCP415", "MCP416" then
        begin
        access agqcontractor_alias, set agqcontractor_alias:app_no   = g_app_no,
                                        agqcontractor_alias:prem_no  = l_prem_no,
                                        agqcontractor_alias:build_no = l_build_no, generic

        change agqcontractor_alias
            begin
            if l_code = "MCP415" then
                agqcontractor_alias:excluding_theft = 1
            if l_code = "MCP416" then
                agqcontractor_alias:limit_theft = 1
            end
        end

    if l_mcp416 = "Y" then
        begin
        l_value[l_sub] = 0
        l_continue_1 = dialog("EXITING","warning.bmp","You need to remove MCP416 to add MCP415","OK")
        end
    else
    if l_mcp415 = "Y" then
        begin
        l_value[l_sub] = 0
        l_continue_1 = dialog("EXITING","warning.bmp","You need to remove MCP415 to add MCP416","OK")
        end

    if l_mcl601 = "Y" then
        begin
        l_value[l_sub] = 0
        l_continue_1 = dialog("EXITING","warning.bmp","You need to remove MCL601 to add MCL602","OK")
        end
    else
    if l_mcl602 = "Y" then
        begin
        l_value[l_sub] = 0
        l_continue_1 = dialog("EXITING","warning.bmp","You need to remove MCL602 to add MCL601","OK")
        end

    if l_nonowned_exists = "Y" then
        begin
        l_value[l_sub] = 0
        l_continue_1 = dialog("EXITING","warning.bmp","Non-owned Auto already exists you need to delete these forms and use Hired/Non-Owned Auto Liability - MCL412","OK")
        end
    else
    if l_hired_exists = "Y" then
        begin
        l_value[l_sub] = 0
        l_continue_1 = dialog("EXITING","warning.bmp","Hired Auto already exists you need to delete these forms and use Hired/Non-Owned Auto Liability - MCL412","OK")
        end
    else
    if l_hirednonowned_exists = "Y" then
        begin
        l_value[l_sub] = 0
        l_continue_1 = dialog("EXITING","warning.bmp","Hired/Nonowned Auto already exists you need to delete this form","OK")
        end
    else
    if l_popup_screen[l_sub] <> "" then
        begin
        if l_location_specific[l_sub] = 0 then
            begin
            l_prem_no  = 0
            l_build_no = 0
            g_prem_no  = 0
            g_build_no = 0
            end
        else
            begin
            l_prem_no  = agqcontractor:prem_no
            l_build_no = agqcontractor:build_no
            g_prem_no  = agqcontractor:prem_no
            g_build_no = agqcontractor:build_no
            end
        l_screen = trun(l_popup_screen[l_sub])
        display modal screen l_screen
        end
    end

if l_value[l_sub] = 0 then
    begin
    do remove_blue(l_the_tag[l_sub],"black",l_value[l_sub],l_popup_screen[l_sub])
    do remove_red(l_the_tag[l_sub],"black",l_value[l_sub],l_popup_screen[l_sub])
    if l_location_specific[l_sub] = 0 then
        begin
        l_prem_no  = 0
        l_build_no = 0
        end
    else
        begin
        l_prem_no  = agqcontractor:prem_no
        l_build_no = agqcontractor:build_no
        end
    l_code = l_policywide_code[l_sub]
    if l_code one of "MCP415", "MCP416" then
        begin
        access agqcontractor_alias, set agqcontractor_alias:app_no   = g_app_no,
                                        agqcontractor_alias:prem_no  = l_prem_no,
                                        agqcontractor_alias:build_no = l_build_no, generic

        change agqcontractor_alias
            begin
            if l_code = "MCP415" then
                agqcontractor_alias:excluding_theft = 0
            if l_code = "MCP416" then
                agqcontractor_alias:limit_theft = 0
            end
        end

    do delete_endorsements
--    abort()
    end

end

procedure check_endorsements
begin
access agqend, set agqend:app_no   = g_app_no,
                   agqend:prem_no  = l_prem_no,
                   agqend:build_no = l_build_no, generic

l_nonowned_exists      = "N"
l_hired_exists         = "N"
l_hirednonowned_exists = "N"
l_mcp416               = "N"
l_mcp415               = "N"
l_mcl601               = "N"
l_mcl602               = "N"
while agqend:app_no   = g_app_no and
      agqend:prem_no  = l_prem_no and
      agqend:build_no = l_build_no
    begin
    if trun(l_code) = "MCP415" and
       trun(agqend:code) = "MCP416" then
        l_mcp416 = "Y"

    if trun(l_code) = "MCP416" and
       trun(agqend:code) = "MCP415" then
        l_mcp415 = "Y"

    if trun(l_code) = "MCL601" and
       trun(agqend:code) = "MCL602" then
        l_mcl602 = "Y"

    if trun(l_code) = "MCL602" and
       trun(agqend:code) = "MCL601" then
        l_mcl601 = "Y"

    if trun(l_code) = "MCL411" and
       trun(agqend:code) = "MCL410" then
        l_hired_exists = "Y"

    if trun(l_code) = "MCL410" and
       trun(agqend:code) = "MCL411" then
        begin
        l_nonowned_exists = "Y"
        end

    if trun(l_code) = "MCL412" and
       trun(agqend:code) one of "MCL411" then
        begin
        l_nonowned_exists = "Y"
        end

    if trun(l_code) = "MCL412" and
       trun(agqend:code) one of "MCL410" then
        begin
        l_hired_exists = "Y"
        end

    if trun(l_code) one of "MCL410" and
       trun(agqend:code) one of "MCL412" then
        begin
        l_hirednonowned_exists = "Y"
        end

    if trun(l_code) one of "MCL411" and
       trun(agqend:code) one of "MCL412" then
        begin
        l_hirednonowned_exists = "Y"
        end

    next agqend
    end

end

