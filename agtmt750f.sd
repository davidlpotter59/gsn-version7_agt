procedure check_endorsements
begin
access agqend, set agqend:app_no   = g_app_no,
                   agqend:prem_no  = l_prem_no,
                   agqend:build_no = l_build_no, generic

l_nonowned_exists      = "N"
l_hired_exists         = "N"
l_hirednonowned_exists = "N"
l_mcp416               = "N"
l_mcp415               = "N"
l_mcl601               = "N"
l_mcl602               = "N"
while agqend:app_no   = g_app_no and
      agqend:prem_no  = l_prem_no and
      agqend:build_no = l_build_no
    begin
    if trun(l_code) = "MCP415" and
       trun(agqend:code) = "MCP416" then
        l_mcp416 = "Y"

    if trun(l_code) = "MCP416" and
       trun(agqend:code) = "MCP415" then
        l_mcp415 = "Y"

    if trun(l_code) = "MCL601" and
       trun(agqend:code) = "MCL602" then
        l_mcl602 = "Y"

    if trun(l_code) = "MCL602" and
       trun(agqend:code) = "MCL601" then
        l_mcl601 = "Y"

    if trun(l_code) = "MCL411" and
       trun(agqend:code) = "MCL410" then
        l_hired_exists = "Y"

    if trun(l_code) = "MCL410" and
       trun(agqend:code) = "MCL411" then
        begin
        l_nonowned_exists = "Y"
        end

    if trun(l_code) = "MCL412" and
       trun(agqend:code) one of "MCL411" then
        begin
        l_nonowned_exists = "Y"
        end

    if trun(l_code) = "MCL412" and
       trun(agqend:code) one of "MCL410" then
        begin
        l_hired_exists = "Y"
        end

    if trun(l_code) one of "MCL410" and
       trun(agqend:code) one of "MCL412" then
        begin
        l_hirednonowned_exists = "Y"
        end

    if trun(l_code) one of "MCL411" and
       trun(agqend:code) one of "MCL412" then
        begin
        l_hirednonowned_exists = "Y"
        end

    next agqend
    end

end

