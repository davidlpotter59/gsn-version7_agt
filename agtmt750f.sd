                                    l_popup_complete[1],
                                    l_loop[2],
                                    l_remove_form[1],
                                    L_STATE[2],
                                    l_agent_no[4],
                                    l_count[4],
                                    l_prem_no[4],
                                    l_premium[8]=0,
                                    l_build_no[4],
                                    l_prem_no_1[4],
                                    l_build_no_1[4],
                                    l_liability_code[2] ,
                                    l_sub[2],
                                    l_tag_position[2] ;

        local string l_code[7],
                     l_code_1[7],
                     l_code_2[7],
                     l_mcp415[1],
                     l_mcp416[1],
                     l_mcl601[1],
                     l_mcl602[1],
                     l_last_str_location[8],
                     l_first_str_location[8],
                     l_str_location[8],
                     l_first_location[1]="Y",
                     l_system_id[15],
                     l_username[15],
                     l_hired_exists[1],
                     l_continue_2[3],
                     l_nonowned_exists[1],
