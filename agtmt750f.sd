components

Fields

101     begin
        if function one of "ADD", "NEW", "CHG", "CHANGE" then
            begin
            enable(accept_fn)
            disable(previousscreen_fn)
            disable(nextscreen_fn)
            disable(back_fn)
            end
        else
            begin
            disable(accept_fn)
            enable(previousscreen_fn)
            enable(nextscreen_fn)
            enable(back_fn)
            end

            l_sub = 1
            l_code = l_policywide_code[1]
            if l_value[1] = 1 then
                begin
                do check_popup
                l_sub = 1
                if l_popup_complete = 1 then
                    do display_blue(l_the_tag[l_sub],"blue",l_value[l_sub],l_popup_screen[l_sub])
                else
                    do display_red(l_the_tag[l_sub],"red",l_value[l_sub],l_popup_screen[l_sub])
                end
            else
            if l_value[1] = 0 then
                begin
                do remove_red(l_the_tag[l_sub],"black",l_value[l_sub],l_popup_screen[l_sub])
                do remove_blue(l_the_tag[l_sub],"black",l_value[l_sub],l_popup_screen[l_sub])
                end
            l_prior_value[1] = l_value[1]
        end
        l_value[1]/checkbox/values="0,1"/default/tabstop   tagged t_remove_checkbox1
        l_sub = 1
        if l_prior_value[1] <> l_value[1] then
            do update_endorsements(l_sub) ;
201     begin
            l_sub = 2
            l_code = l_policywide_code[2]
            if l_value[2] = 1 then
                begin
                do check_popup
                l_sub = 2
                if l_popup_complete = 1 then
                    do display_blue(l_the_tag[l_sub],"blue",l_value[l_sub],l_popup_screen[l_sub])
                else
                    do display_red(l_the_tag[l_sub],"red",l_value[l_sub],l_popup_screen[l_sub])
                end
            else
            if l_value[2] = 0 then
                begin
                do remove_red(l_the_tag[l_sub],"black",l_value[l_sub],l_popup_screen[l_sub])
                do remove_blue(l_the_tag[l_sub],"black",l_value[l_sub],l_popup_screen[l_sub])
                end
            l_prior_value[2] = l_value[2]
        end
