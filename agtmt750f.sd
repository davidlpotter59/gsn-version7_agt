Style Definition

        Include "styles.inc"
        Displayonly Field Style {
                                  backgroundcolor="#CCCCCC"
                                }


Functions
         reaccess;
         "redisplay" display/noreturn "agtmt750f" ;
        "change" sequence "chg|redisplay"   tagged change_fn ;
        "chg" change/hidden ;
        "func1" display l_screen ;

        "back" display/noreturn "agtmt001" tagged back_fn ;
        "nextscreen"     display/noreturn "agtmt750g"  tagged nextscreen_fn ;
        "func1" sequence "previousscreen" tagged previouscreen_fn ;
        "previousscreen" continue begin
                                  if agqcontractor:form <> 4 then
                                      l_screen = "agtmt750h"
                                  else
                                      l_screen = "agtmt750e"
                                  function = "popup"
                                  end;
        "popup" display/noreturn l_screen ;

        Include "stdfunc7.inc"

Menu Definition

Toolbar Definition

screen entry
disable(accept_fn)

access agqname, set agqname:app_no = g_app_no, generic

if agqname:quote_no <> 0 then
    begin
    disable(change_fn)
    disable(accept_fn)
    end

access sfsemail, set sfsemail:company_id = agqcontractor:company_id,
                     sfsemail:user_id    = username, generic

l_username  = username
l_company_id = sfsdefault:company_id

access sfsprofile,
   set sfsprofile:user_id  = l_username, exact

If SFSPROFILE:USER_ID <> L_USERNAME Then
  Begin
    L_USERNAME = "default"
    Access SFSPROFILE, Set SFSPROFILE:USER_ID = L_USERNAME, Exact
  End

if l_company_id = "FLEMINGTON" then
    begin
    l_server_name = sfsprofile:UNIX_SERVER
    l_current_web_page = "webhelp_scr_opt_endt.htm"
    do change_current_browser(l_server_name,l_current_web_page)
    end

l_system_id = "bop"
access sfssecurity_rating,
   set sfssecurity_rating:company_id = agqcontractor:company_id,
       sfssecurity_rating:user_id    = l_username,
       sfssecurity_rating:system_id  = l_system_id, generic

access sfsdefault, set sfsdefault:sfs_code = "SFS", generic

if sfsdefault:company_id one of "FLEMINGTON" then
    hide(farmers_fn)
else
if sfsdefault:company_id one of "FARMERS" then
    hide(ficof_fn)

Access Validation
If agqcontractor:app_NO <> G_app_NO Then
    Error 1000

access agqcontractor_alias, set agqcontractor_alias:app_no = agqcontractor:app_no, generic

l_no_locations   = 0
l_first_location = "Y"
l_first_prem_no  = 0
l_first_build_no = 0
l_last_prem_no   = 0
l_last_build_no  = 0
while agqcontractor_alias:app_no = agqcontractor:app_no
    begin
    if l_first_location = "Y" then
        begin
        l_first_str_location  = str(agqcontractor_alias:prem_no) +
                               str(agqcontractor_alias:build_no)
        l_first_location = "N"
        end
    l_last_str_location  = str(agqcontractor_alias:prem_no) +
                           str(agqcontractor_alias:build_no)
    l_no_locations  = l_no_locations + 1

    next agqcontractor_alias
    end

l_str_location = str(agqcontractor:prem_no) +
                 str(agqcontractor:build_no)

if l_no_locations = 1 then
    begin
    disable(next_fn)
    disable(previous_fn)
    end
else
if l_str_location = l_first_str_location and
   l_no_locations > 1 then
    begin
    disable(previous_fn)
    enable(next_fn)
    end
else
if l_str_location <> l_first_str_location and
   l_str_location <> l_last_str_location and
   l_no_locations > 1 then
    begin
    enable(next_fn)
    enable(previous_fn)
    end
else
if l_str_location = l_last_str_location and
   l_no_locations > 1 then
    begin
    disable(next_fn)
    enable(previous_fn)
    end

