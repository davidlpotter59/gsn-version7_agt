        array[40] of unsigned ascii number l_prior_value[1];
        array[40] of tag l_the_tag[15] ;

        Global String G_COMPANY_ID[10] ,
                      g_inquiry[1] ,
                      g_code[7] ;

        local unsigned ascii number l_sub_code[4],
                                    l_form[2],
                                    l_no_locations[2],
                                    l_class_code[5],
                                    l_first_prem_no[4],
                                    l_last_prem_no[4],
                                    l_first_build_no[4],
