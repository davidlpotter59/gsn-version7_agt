Program
  Name = "agq0282_012003";
  Type = "File";
  Domain = "flat";
  Padcharacter = Space;

 String L_FILE_NAME[17] = "agq0282_012003.cq";
 String L_COPY_NAME[20] ;
 String L_AGQLOCATION_ZIP;
 String L_COMPANY_ID[10];
 unsigned ascii number l_app_no[11] ;
 Unsigned ascii number l_max_lines;
 Unsigned Ascii Number L_YEAR[4];
 Unsigned Ascii number L_LINE_OF_BUSINESS[4];
 Unsigned ascii number l_found[1];
 Unsigned Ascii Number L_ITEM_COUNTER;
 Unsigned Ascii Number L_STATE[2]=0;
 Unsigned Ascii Number L_LOOP;
 wdate L_EFF_DATE;
 Unsigned Ascii Number L_LINE_COUNTER;
 Unsigned Ascii Number L_PAGE_NUMBER;

   Include "swsut100.var"
   Include "swsut200.var"
   Include "csfonts.inc"

Initialization
  l_app_no = val(commandline[1,11])
  Deletefile (L_FILE_NAME);

Access agqLOCATION,
   Set agqLOCATION:app_no    = l_app_no , generic

Access agqNAME,
   Set agqNAME:app_no    = l_app_no , generic

Access agqEND_ALIAS,
   Set agqEND_ALIAS:app_no    = l_app_no , generic

Access agqdishonesty,
   Set agqdishonesty:app_no    = l_app_no , generic

Access agqendorse,
   Set agqendorse:app_no    = l_app_no , generic

Access agqgeneral,
   Set agqgeneral:app_no    = l_app_no , generic

  Include "swsut100.inc"
  I_CODE = "BU0282"
  Include "swsut200.inc"
--Include "swsut300.inc"

While agqEND_ALIAS:app_no    = l_app_no
{
  If agqEND_ALIAS:CODE = I_CODE Then
    {
      L_STATE            = agqgeneral:STATE
      L_COMPANY_ID       = agqgeneral:COMPANY_ID
      L_LINE_OF_BUSINESS = agqgeneral:LINE_OF_BUSINESS

      Access SFSOPTEND,
         Set SFSOPTEND:COMPANY_ID       = L_COMPANY_ID,
             SFSOPTEND:STATE            = L_STATE,
             SFSOPTEND:LINE_OF_BUSINESS = L_LINE_OF_BUSINESS,
             SFSOPTEND:CODE             = I_CODE, generic

       While SFSOPTEND:COMPANY_ID       = L_COMPANY_ID and
             SFSOPTEND:STATE            = L_STATE and
             SFSOPTEND:LINE_OF_BUSINESS = l_LINE_OF_BUSINESS and
             SFSOPTEND:CODE             = I_CODE

         {  --start of sfsoptend check
           If (agqgeneral:eff_date >= sfsoptend:eff_date and
              (agqgeneral:eff_date < sfsoptend:exp_date or
               sfsoptend:exp_date = 0)) then
             {
               l_eff_date = sfsoptend:eff_date
             }
           next sfsoptend
         }
      Access SFSOPTEND,
         Set SFSOPTEND:COMPANY_ID       = L_COMPANY_ID,
             SFSOPTEND:STATE            = L_STATE,
             SFSOPTEND:LINE_OF_BUSINESS = L_LINE_OF_BUSINESS,
             SFSOPTEND:CODE             = I_CODE,
             SFSOPTEND:EFF_DATE         = L_EFF_DATE, exact

      If sfsoptend:edition <> 0 then
        {
          Writefile(L_FILE_NAME,"%s","<033>&a420h7080V")
          Writefile(L_FILE_NAME,"%s","Ed. ")
          Writefile(L_FILE_NAME,"%n",sfsoptend:edition)
         }

      Access agqDISHONESTY,
         Set agqDISHONESTY:app_no    = l_app_no,
             agqDISHONESTY:PREM_NO      = agqEND_ALIAS:PREM_NO,
             agqDISHONESTY:BUILD_NO     = agqEND_ALIAS:BUILD_NO, Generic

      Writefile(L_FILE_NAME,"%s",L_ARIAL_9)
      Writefile(L_FILE_NAME,"%s","<033>&a420h7200V")
      Writefile(L_FILE_NAME,"%s",l_copyright_6)
      Writefile(L_FILE_NAME,"%s"," ")
      Writefile(L_FILE_NAME,"%s",L_ARIAL_9)
      Writefile(L_FILE_NAME,"%s",sfsoptend:COPYRIGHT)
      Writefile(L_FILE_NAME,"%s","<033>&a200h+120V")

      Writefile(L_FILE_NAME,"%s",L_ARIAL_BOLD_12)
      Writefile(L_FILE_NAME,"%s","<033>&a4300h120V")
      Writefile(L_FILE_NAME,"%s",Trun(SFSOPTEND:FORM_PRINT))
      Writefile(L_FILE_NAME,"%s"," ")
      Writefile(L_FILE_NAME,"%s",L_ARIAL_BOLD_9)
      Writefile(L_FILE_NAME,"%s",SFSOPTEND:EDITION_PRINT)

      Writefile(L_FILE_NAME,"%s","<033>&a90h+120V")
      Writefile(L_FILE_NAME,"%s",L_ARIAL_ITALIC_BOLD_9)
      Writefile(L_FILE_NAME,"%s","EMPLOYEE ")
      Writefile(L_FILE_NAME,"%s",L_ARIAL_BOLD_9)
      Writefile(L_FILE_NAME,"%s","DISHONESTY COVERAGE ")

      Writefile(L_FILE_NAME,"%s","<033>&a+0h+20V")
      Writefile(L_FILE_NAME,"%s",L_BULLET_6)
      Writefile(L_FILE_NAME,"%s","<033>&a+0h-20V")

