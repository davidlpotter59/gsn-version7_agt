                               agqsetupmainscreen:state            = l_state,
                               agqsetupmainscreen:line_of_business = l_line_of_business, exact

                        l_screen_to_call = agqsetupmainscreen:screen_to_call[1]
                        function = "func1"
                        abort()
                        }
