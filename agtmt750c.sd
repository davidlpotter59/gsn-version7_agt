access agqname, set agqname:app_no = g_app_no,generic

if agqname:quote_no <> 0 then
    begin
    disable(add_fn)
    disable(change_fn)
    disable(delete_fn)
    disable(accept_fn)
    end

if agqcontractor:app_no = 0 then
    begin
    disable(previous_fn)
    disable(next_fn)
    disable(previousscreen_fn)
    disable(nextscreen_fn)
    disable(back_fn)
    end

access sfscompany, set sfscompany:company_id = l_company_id, generic

l_screen_name = "agtmt750c"
if g_app_no = 0 then
    l_field_no = 9997
else
    l_field_no = 9999

enable(previousscreen_fn)
enable(nextscreen_fn)
enable(back_fn)

if agqcontractor:prem_no = 0 then
    function = "CHANGE"

if g_form = 4 or
   agqcontractor:form = 4 then
    begin
    hide(property_fn)
    end
else
    begin
    show(property_fn)
    end

access validation
if agqcontractor:app_no <> g_app_no then
    error 1000

if function one of "NEXT", "PREVIOUS" then
    begin
    g_form = agqcontractor:form
    if g_form = 4 then
        begin
        hide(property_fn)
        end
    else
        begin
        show(property_fn)
        end
    end

access agqcontractor_alias, set agqcontractor_alias:app_no = agqcontractor:app_no, generic

l_no_locations   = 0
l_first_location = "Y"
l_first_prem_no  = 0
l_first_build_no = 0
l_last_prem_no   = 0
l_last_build_no  = 0
while agqcontractor_alias:app_no = agqcontractor:app_no
    begin
    if l_first_location = "Y" then
        begin
        l_first_str_location  = str(agqcontractor_alias:prem_no) +
                               str(agqcontractor_alias:build_no)
        l_first_location = "N"
        end
    l_last_str_location  = str(agqcontractor_alias:prem_no) +
                           str(agqcontractor_alias:build_no)
    l_no_locations  = l_no_locations + 1

    next agqcontractor_alias
    end

l_str_location = str(agqcontractor:prem_no) +
                 str(agqcontractor:build_no)

if l_no_locations one of 0, 1 then
    begin
    disable(next_fn)
    disable(previous_fn)
    end
else
if l_str_location = l_first_str_location and
   l_no_locations > 1 then
    begin
    disable(previous_fn)
    enable(next_fn)
    end
else
if l_str_location <> l_first_str_location and
   l_str_location <> l_last_str_location and
   l_no_locations > 1 then
    begin
    enable(next_fn)
    enable(previous_fn)
    end
else
if l_str_location = l_last_str_location and
   l_no_locations > 1 then
    begin
    disable(next_fn)
    enable(previous_fn)
    end

