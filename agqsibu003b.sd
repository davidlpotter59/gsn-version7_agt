            {
              Writefile(L_FILE_NAME,"%s%n%s%s%s","<033>&a",L_LIMIT_POS,"h+0V",
                                  l_letter_gothic_bold_10 ,Str(0,"ZZZ,ZZZ,ZZ9"))
              Writefile(L_FILE_NAME,"%s%s%s%n%s%s%s%n%s%s%s%s","<NL>",l_arial_9,"<033>&a",
