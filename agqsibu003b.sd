            Writefile(L_FILE_NAME,"%s%s%n%s%s%s",l_letter_gothic_bold_10,
                                  "<033>&a",L_LIMIT_POS,"h+0V",
                                 Str(L_REVISED_LIMIT,"ZZZ,ZZZ,ZZ9"),"<NL>")
            }
        }
    Else
        {
        If (L_BPSDEFAULT_LIMIT >= L_REVISED_LIMIT) Then
            {
            Writefile(L_FILE_NAME,"%s%s%n%s%s%s",l_letter_gothic_bold_10,
