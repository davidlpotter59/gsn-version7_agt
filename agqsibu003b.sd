                        L_REVISED_POS,"h+0V","Revised Limit:","<033>&a",
                        L_DOLLAR_POS,"h+0V",l_arial_bold_9,"$",
                        l_letter_gothic_bold_10)
              Writefile(L_FILE_NAME,"%s%s%n%s%s",l_letter_gothic_bold_10,
