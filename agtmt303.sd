components

Fields
101     begin
        If FUNCTION = "ADD" Then
            Begin
            agqendorse:app_no   = g_app_no
            agqendorse:PREM_NO  = G_PREM_NO
            agqendorse:BUILD_NO = G_BUILD_NO
            agqendorse:code     = g_code
            do subcode
            agqendorse:sub_code = l_sub_code + 10
            Access agqgeneral, Set agqgeneral:app_no = G_app_no, Generic

            agqENDORSE:EFF_DATE = agqgeneral:EFF_DATE
            End
        l_exp_date = dateadd(agqgeneral:eff_date,0,1)
        End
        agqENDORSE:EFF_DATE/default/tabstop
        If agqENDORSE:EFF_DATE < agqgeneral:EFF_DATE Then
            Error 1295

        If agqENDORSE:EFF_DATE >  L_EXP_DATE Then
            Error 1294 ;
201     Begin
        If FUNCTION = "ADD" Then
            agqENDORSE:EXP_DATE = L_EXP_DATE
        End
        agqENDORSE:EXP_DATE/default/tabstop
        If agqENDORSE:EXP_DATE < agqgeneral:EFF_DATE Then
            Error 1295
        If agqENDORSE:EXP_DATE > L_EXP_DATE Then
            Error 1294 ;
301     agqENDORSE:DESCRIPTION[1]/uppercase/tabstop ;

}


