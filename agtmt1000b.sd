procedure update_endorsements(number l_sub)
begin
if l_value[l_sub] = 1 then
    begin
    If l_code = "MHO45" and
       agqpersonal:company_id = "FARMERS" and
       agqpersonal:owner_occupied = "N" and
       agqpersonal:windstorm_loss_mitigation = 1 and
       agqpersonal:form = 6 then
        begin
        access agqpersonal_alias, set agqpersonal_alias:app_no = g_app_no, generic

        if agqpersonal_alias:app_no = g_app_no then
            begin
            change agqpersonal_alias
                begin
                agqpersonal_alias:windstorm_loss_mitigation = 0
                end
            end
        l_message = "You can not add MHO 45 and have the windstorm credit, windstorm credit is being removed"
        l_continue_1 = dialog("MHO45","warning.bmp",trun(l_message),"OK")
        end

    If l_code = "MPL12" and
       agqpersonal:company_id = "FARMERS" and
       l_value[l_sub] = 1 then
        begin
        access agqpersonal_alias, set agqpersonal_alias:app_no = g_app_no, generic

        if agqpersonal_alias:app_no = g_app_no then
            begin
            change agqpersonal_alias
                begin
                agqpersonal_alias:remove_mpl12 = 0
                end
            end
        end

    if l_location_specific[l_sub] = 0 then
        begin
        l_prem_no  = 0
        l_build_no = 0
        g_prem_no  = 0
        g_build_no = 0
        end
    else
        begin
        l_prem_no  = agqpersonal:prem_no
        l_build_no = agqpersonal:build_no
        g_prem_no  = agqpersonal:prem_no
        g_build_no = agqpersonal:build_no
        end
    l_code = l_policywide_code[l_sub]
    g_code = l_policywide_code[l_sub]
    do add_endorsements
    if l_popup_screen[l_sub] <> "" then
        begin
        if l_location_specific[l_sub] = 0 then
            begin
            l_prem_no  = 0
            l_build_no = 0
            g_prem_no  = 0
            g_build_no = 0
            end
        else
            begin
            l_prem_no  = agqpersonal:prem_no
            l_build_no = agqpersonal:build_no
            g_prem_no  = agqpersonal:prem_no
            g_build_no = agqpersonal:build_no
            end
        l_screen = trun(l_popup_screen[l_sub])
        display modal screen l_screen
--        function = "func1"
--        abort()
        end
--    else
--        begin
--        abort()
--        end
    end

if l_value[l_sub] = 0 then
    begin
    do remove_blue(l_the_tag[l_sub],"black",l_value[l_sub],l_popup_screen[l_sub])
    do remove_red(l_the_tag[l_sub],"black",l_value[l_sub],l_popup_screen[l_sub])
    if l_location_specific[l_sub] = 0 then
        begin
        l_prem_no  = 0
        l_build_no = 0
        end
    else
        begin
        l_prem_no  = agqpersonal:prem_no
        l_build_no = agqpersonal:build_no
        end
    l_code = l_policywide_code[l_sub]

    If l_code = "MPL12" and
       agqpersonal:company_id = "FARMERS" and
       l_value[l_sub] = 0 then
        begin
        access agqpersonal_alias, set agqpersonal_alias:app_no = g_app_no, generic

        if agqpersonal_alias:app_no = g_app_no then
            begin
            change agqpersonal_alias
                begin
                agqpersonal_alias:remove_mpl12 = 1
                end
            end
        end

    do delete_endorsements
--    abort()
    end

end

