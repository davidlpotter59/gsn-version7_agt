        l_the_tag[l_tag_position] = tag(trun(sfspolicywideoptend:tag_name))
        l_location_specific[l_tag_position] = sfspolicywideoptend:location_specific
        do printguidetext(l_the_tag[l_tag_position],trun(l_view_desc[l_tag_position]))
        }

    next sfspolicywideoptend
    }

l_loop = 1
while l_loop <= 36
    {
    if l_view_desc[l_loop] = "" then
        {
        switch(l_loop)
          case 1  : hide(t_remove_checkbox1)
          case 2  : hide(t_remove_checkbox2)
