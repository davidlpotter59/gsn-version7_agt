access sfspolicywideoptend, set sfspolicywideoptend:company_id       = agqpersonal:company_id,
                                sfspolicywideoptend:state            = agqpersonal:state,
                                sfspolicywideoptend:line_of_business = agqpersonal:line_of_business,
                                sfspolicywideoptend:class_code       = l_class_code, generic

while sfspolicywideoptend:company_id       = agqpersonal:company_id and
      sfspolicywideoptend:state            = agqpersonal:state and
      sfspolicywideoptend:line_of_business = agqpersonal:line_of_business and
      sfspolicywideoptend:class_code       = l_class_code
    {
    if (agqpersonal:eff_date >= sfspolicywideoptend:eff_date and
       (agqpersonal:eff_date <= sfspolicywideoptend:exp_date or
       sfspolicywideoptend:exp_date = 0)) then
        {
        l_tag_position = val(sfspolicywideoptend:tag_name[7,8])
        l_policywide_code[l_tag_position] = sfspolicywideoptend:code
        do read_agqend
        l_view_desc[l_tag_position] = trun(sfspolicywideoptend:POLICY_WIDE_FORM_DESC)
