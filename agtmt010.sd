EVENT DEFINITION
        default eventhandler {
                             "REMOVE_SCREEN" { function = "exit" abort () }
                             "reload" { function = "reaccess" abort () }
                             "display_message" { warning event.value }
                             "current_line_of_business" { g_line_of_business = val(event.value)
                                                        do display_rating_screen }
                             }

Menu Definition

Toolbar Definition

Screen Entry
if function = "POPUP" then
    function = "back"

access sfsdefault, set sfsdefault:sfs_code = "SFS", generic

if g_from_search_screen = 0 then
    {
    g_app_no     = 0
    }

l_username   = username
l_company_id = sfsdefault:company_id
access sfsemail, set sfsemail:company_id = l_company_id,
                     sfsemail:user_id    = l_username, generic

if sfsemail:agent_no = 0 then
    {
    l_agent_no = 9999
    }
else
    {
    l_agent_no = sfsemail:agent_no
    }

access sfsagent, set sfsagent:company_id = l_company_id,
                     sfsagent:agent_no   = l_agent_no, exact

l_state = sfsagent:state

--`do display_heading_1
do display_rating_screen

screen at 1,1 to 30,100  tagged main_screen
    Properties
        {
        Layouttype = column
        layoutconstraints = stretch
        bordercolor = "black"
        borderwidth=2
        transparent = False
        backgroundcolor = "#ADD8E6"
        }
    {
    panel/hidden
        {
        Fields
            function at 1,1 ;
        }

    panel at 1,1 to 30,100 tagged rating_panel
        Properties
            {
            transparent = False
            backgroundcolor = "#ADD8E6"
            }

        {

        }

}

Procedure Definition

procedure display_rating_screen

begin
  access agqsetupmainscreen,
     set agqsetupmainscreen:company_id       = l_company_id,
         agqsetupmainscreen:state            = l_state,
         agqsetupmainscreen:line_of_business = g_line_of_business, generic

if g_initial_screen <> "" then
    display screen trun(g_initial_screen) inside rating_panel  --this is from agqsetupaminscreen:screen_to_call[1] or [2] set in agtmt102
else
    display screen trun(agqsetupmainscreen:screen_to_call[1]) inside rating_panel

end

