access agqclasscode_alias, set agqclasscode_alias:app_no   = g_app_no,
                               agqclasscode_alias:prem_no  = g_prem_no,
                               agqclasscode_alias:build_no = g_build_no, generic

if agqclasscode_alias:app_no   <> g_app_no or
   agqclasscode_alias:prem_no  <> g_prem_no or
   agqclasscode_alias:build_no <> g_build_no then
    function = "ADD"

disable(accept_fn)
