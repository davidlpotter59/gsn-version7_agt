                error l_minimum_message
            end

        L_LIMIT = agqPERSONAL:DWELLING_LIMIT * 0.70
        if agqpersonal:company_id = "FARMERS" and
           sfsemail:agent_no <> 0 and
           agqpersonal:contents_limit > l_limit then
            begin
            l_continue_3 = dialog("CONTACT UNDERWRITER","warning.bmp","Coverage C more than 70% of Coverage A, Contact your underwriter","OK")
            end ;

1601    begin
        if function one of "ADD", "NEW", "CHANGE", "CHG" then
            begin
            if l_company_id = "FLEMINGTON" then
