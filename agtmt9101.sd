    Do DELETE_SFPPRINT4

    Do DELETE_SFPPRINT5

    Do UPDATE_SFPNAME

    Delete SFPPOLICY

    Access SFPPOLICY,
       Set SFPPOLICY:USER_ID = L_USERNAME, Approximate

    Next SFPPOLICY
    End

l_app     = "sfs"
L_DEC     = L_APP+Str(I_CSEXEC_NUMBER)+".cq"
L_DEC_PDF = L_APP+Str(I_CSEXEC_NUMBER)+".pdf"

Include "virtualdecs2.pro"
End

