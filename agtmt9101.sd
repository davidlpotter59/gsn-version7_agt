procedure get_sfszipcode
begin
l_val_zipcode = agqpersonal:zipcode
l_str_zipcode = agqpersonal:str_zipcode[1,5] + "0000"
access sfszipcode, set sfszipcode:company_id       = agqpersonal:company_id,
