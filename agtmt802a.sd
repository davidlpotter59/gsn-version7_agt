Screen
        Name = "agtmt802a";
        Type = "Detail" ;
        Highlight=Underscore ;
        Padcharacter=Space ;
        Screensize="dynamic";
--   Windowtitle="%a     %s Policy Information - Ver. 7.22   %m";
        Domain="agqmaster" ;
        iconline = 0 ;


        Global Unsigned Ascii Number G_app_NO[11] ,
                                     G_LINE_OF_BUSINESS[4],
                                     g_prem_no[4],
                                     g_build_no[4],
                                     g_sub_code[4],
                                     G_STATE[2],
                                     g_enhancement[1] ;


        Global String G_COMPANY_ID[10] ,
                      g_literal[100] ,
                      g_inquiry[1] ,
                      g_code[7] ;

        local unsigned ascii number l_sub_code[4],
                                    l_form[2],
                                    l_remove_form[1],
                                    L_STATE[2],
                                    l_agent_no[4],
                                    l_liability_code[2] ;

        local string l_code[7],
                     l_code_1[7],
                     l_code_2[7],
                     l_system_id[15],
                     l_st_type[6],
                     l_st_direction[2],
                     l_username[15],
                     l_add_endorsement[1],
                     l_Form_edition[18],
                     L_COMPANY_ID[10] ,
                     l_description[55] ;

        Access agqmaster,
           Set agqmaster:app_NO   = G_app_NO , generic ;

        Include "stdkeys7.inc"

Style Definition

        Include "styles.inc"
        Displayonly Field Style {
                                  backgroundcolor="#CCCCCC"
                                }


Functions

        add/autoupdate tagged add_fn ;
        change/autoupdate tagged change_fn ;
        Include "stdfunc7.inc"

Menu Definition

Toolbar Definition

screen entry
access agqcontractor, set agqcontractor:app_no = g_app_no, generic ;

access sfsemail, set sfsemail:company_id = agqcontractor:company_id,
                     sfsemail:user_id    = username, generic

l_username  = username
l_system_id = "bop"
access sfssecurity_rating,
   set sfssecurity_rating:company_id = agqcontractor:company_id,
       sfssecurity_rating:user_id    = l_username,
       sfssecurity_rating:system_id  = l_system_id, generic

include "t_line_of_business.scr"

access sfsdefault, set sfsdefault:sfs_code = "SFS", generic

if sfsdefault:company_id one of "FLEMINGTON" then
    hide(farmers_fn)
else
if sfsdefault:company_id one of "FARMERS" then
    hide(ficof_fn)

Access Validation
If agqmaster:app_NO <> G_app_NO Then
    Error 1000 ;

