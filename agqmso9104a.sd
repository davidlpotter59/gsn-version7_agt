access agqsupp_alias, set agqsupp_alias:app_no = l_app_no, generic

while agqsupp_alias:app_no = l_app_no
    begin
    l_no_mtg = l_no_mtg + 1

    next agqsupp_alias
    end

if l_no_mtg >= 3 then
    begin
    Writefile(L_FILE_NAME,"%s",l_arial_bold_9)
    Writefile(L_FILE_NAME,"%s","<033>&a200h+0V")
    writefile(l_file_name,"%s%s","3 or more Mortgagees","<NL>")
    l_line_counter = l_line_counter + 1
    do set_position
    end

access agqwatercraft, set agqwatercraft:app_no = l_app_no, generic

while agqwatercraft:app_no = l_app_no
    begin
    if agqwatercraft:horse_power[1] > 100 then
        begin
        Writefile(L_FILE_NAME,"%s",l_arial_bold_9)
        Writefile(L_FILE_NAME,"%s","<033>&a200h+0V")
        writefile(l_file_name,"%s%s","MPL20 - Outboard Motor horsepower greater than 100 h.p. is NOT Bound","<NL>")
        l_line_counter = l_line_counter + 1
        do set_position
        end

    if agqwatercraft:horse_power[2] > 150 then
        begin
        Writefile(L_FILE_NAME,"%s",l_arial_bold_9)
        Writefile(L_FILE_NAME,"%s","<033>&a200h+0V")
        writefile(l_file_name,"%s%s","MPL20 - Inboard/Inboard-Outdrive horsepower greater than 150 h.p. is NOT Bound","<NL>")
        l_line_counter = l_line_counter + 1
        do set_position
        end

    if agqwatercraft:sailboat_feet > 26 then
        begin
        Writefile(L_FILE_NAME,"%s",l_arial_bold_9)
        Writefile(L_FILE_NAME,"%s","<033>&a200h+0V")
        writefile(l_file_name,"%s%s","MPL20 - Sailboat over 26' is NOT Bound.","<NL>")
        l_line_counter = l_line_counter + 1
        do set_position
        end

    next agqwatercraft
    end

access agqdesc1, set agqdesc1:app_no = l_app_no, generic

while agqdesc1:app_no = l_app_no
    begin
    if agqdesc1:code = "MPL15" then
        begin
        if agqdesc1:article = "JEWELRY" and
           agqdesc1:limit > 25000 and
           l_jewelry_first = "Y" then
            begin
            l_jewelry_first = "N"
            Writefile(L_FILE_NAME,"%s",l_arial_bold_9)
            Writefile(L_FILE_NAME,"%s","<033>&a200h+0V")
            writefile(l_file_name,"%s%s","MPL15 - Jewelry Limit exceeds 25,000 is NOT Bound.","<NL>")
            l_line_counter = l_line_counter + 1
            do set_position
            end

        if agqdesc1:article = "FINE ARTS" and
           agqdesc1:limit > 50000 and
           l_fine_arts_First = "Y" then
            begin
            l_fine_arts_first = "N"
            Writefile(L_FILE_NAME,"%s",l_arial_bold_9)
            Writefile(L_FILE_NAME,"%s","<033>&a200h+0V")
            writefile(l_file_name,"%s%s","MPL15 - Fine Arts Limit Exceeds 50,000 is NOT Bound.","<NL>")
            l_line_counter = l_line_counter + 1
            do set_position
            end

        end

    next agqdesc1
    end

do minimum_maximum_limits
l_limit = agqpersonal:dwelling_limit * 0.70
if agqpersonal:contents_limit > l_limit then
    begin
    Writefile(L_FILE_NAME,"%s",l_arial_bold_9)
    Writefile(L_FILE_NAME,"%s","<033>&a200h+0V")
    writefile(l_file_name,"%s%s","Personal Property is More Than 70% of Coverage A","<NL>")
    l_line_counter = l_line_counter + 1
    do set_position
    end

