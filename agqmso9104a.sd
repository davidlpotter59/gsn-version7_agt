Procedure set_position

begin
If l_literal_pos > l_remarks_pos then
  {
    l_next_pos = l_literal_pos
  }
Else
  If l_literal_pos <= l_remarks_pos then
    {
      l_next_pos = l_remarks_pos
    }

If (l_remarks_pos - l_literal_pos) = 120 then
  {
    l_line_counter = l_line_counter - 1
    l_next_pos = l_next_pos - 120
  }
end

procedure print_single_remarks(string p_current_app_remark)

BEGIN
  If p_current_app_remark <> "" then
    {
      if len(trun(p_current_app_remark)) > 40 then
        {
          Writefile(L_FILE_NAME,"%s",l_arial_8)
        }
      else
        {
          Writefile(L_FILE_NAME,"%s",l_arial_bold_9)
        }
      Writefile(L_FILE_NAME,"%s%n%s","<033>&a3600h",l_next_pos,"V")
      Writefile(L_FILE_NAME,"%s",p_current_app_remark)
      Writefile(L_FILE_NAME,"%s",l_arial_9)
      do set_position
    }
  else
    do set_position
END


