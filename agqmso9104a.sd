    l_line_counter = l_line_counter + 1
    do set_position
    end

if agqpersonal:year_construction < 1920 then
    begin
    Writefile(L_FILE_NAME,"%s",l_arial_bold_9)
    Writefile(L_FILE_NAME,"%s","<033>&a200h+0V")
    writefile(l_file_name,"%s%s","Home built prior to 1920","<NL>")
    l_line_counter = l_line_counter + 1
    do set_position
    end

access agqapplication, set agqapplication:app_no = l_app_no, generic

while agqapplication:app_no = l_app_no
    begin
    if agqapplication:sub_code one of 2, 16, 22, 23, 24, 28, 29, 30 and
       agqapplication:answer = "Y" then
        begin
        Writefile(L_FILE_NAME,"%s",l_arial_bold_9)
        Writefile(L_FILE_NAME,"%s","<033>&a200h+0V")
        writefile(l_file_name,"%s%s",trun(agqapplication:literal),"<NL>")
        l_line_counter = l_line_counter + 1
        do set_position
        end

    if agqapplication:sub_code = 31 and
       agqapplication:answer = "Y" and
       agqapplication:breed_for_profit = "Y" then
        begin
        Writefile(L_FILE_NAME,"%s",l_arial_bold_9)
        Writefile(L_FILE_NAME,"%s","<033>&a200h+0V")
        writefile(l_file_name,"%s%s","Breed dogs for profit","<NL>")
        l_line_counter = l_line_counter + 1
        do set_position
        end

    next agqapplication
    end

