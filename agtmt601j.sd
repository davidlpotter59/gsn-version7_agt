update
if updateok = "NO" then
    begin
    disable(accept_fn)
    enable(close_fn)
    end

if abort = "YES" then
    begin
    disable(accept_fn)
    enable(close_fn)
    end

If UPDATEOK = "YES" And
   Abort    = "NO" Then
    Begin
    disable(accept_fn)
    enable(close_fn)
    function = "redisplay"
    end

procedure definition

Procedure SUBCODE
Begin
Access agqdriver_ALIAS, Set agqdriver_ALIAS:app_no = g_app_no, generic

L_SUB_CODE = 0
while agqdriver_ALIAS:app_no = g_app_no
    begin
    L_SUB_CODE = agqdriver_ALIAS:SUB_CODE

    Next agqdriver_ALIAS
    End

L_SUB_CODE = L_SUB_CODE + 10
End

End
