procedure minimum_premium
begin
Access SFSCONTROL, Set SFSCONTROL:COMPANY_ID       = agqgeneral1:company_id,
                       SFSCONTROL:STATE            = agqgeneral1:STATE,
                       SFSCONTROL:LINE_OF_BUSINESS = agqgeneral1:line_of_business, Approximate

while SFSCONTROL:COMPANY_ID       = agqgeneral1:company_id and
      SFSCONTROL:STATE            = agqgeneral1:STATE and
      SFSCONTROL:LINE_OF_BUSINESS = agqgeneral1:line_of_business
    begin
    If (agqname:eff_date >= SFSCONTROL:EFF_DATE and
       (agqname:eff_date < sfscontrol:exp_date or
       sfscontrol:exp_date = 0)) Then
        begin
        L_MINIMUM               = SFSCONTROL:MINIMUM_PREMIUM
        end

    Next SFSCONTROL
    End

l_minimum_applies = "N"
if l_total < l_minimum then
    begin
    l_total = l_minimum
    l_minimum_applies = "Y"
    access agqgeneral1_alias, set agqgeneral1_alias:app_no  = agqgeneral1:app_no, generic

    l_mp_total = 0
    while agqgeneral1_alias:app_no = agqgeneral1:app_no
            begin
            l_mp_total = agqgeneral1_alias:total_premium[1] +
                         l_mp_total

            next agqgeneral1_alias
            end

    access agqgeneral1_alias, set agqgeneral1_alias:app_no  = agqgeneral1:app_no, generic

    change agqgeneral1_alias
        begin
        agqgeneral1_alias:total_premium[1] = (agqgeneral1_alias:total_premium[1] +
                                            (l_minimum -
                                            l_mp_total))
        agqgeneral1_alias:premium[12]      = l_minimum -
                                            l_mp_total
        end
    end
else
    begin
    access agqgeneral1_alias, set agqgeneral1_alias:app_no = agqgeneral1:app_no, generic

    while agqgeneral1_alias:app_no = agqgeneral1:app_no
        begin
        change agqgeneral1_alias
            begin
            agqgeneral1_alias:premium[12]      = 0
            end

        next agqgeneral1_alias
        end
    end
end

Procedure FINAL_TOTAL
Begin
L_EFF_DATE_1 = Dateadd(agqname:eff_date,0,1)
If agqname:exp_date < L_EFF_DATE_1 Then
    begin
    i_eff_date = dateadd(agqname:exp_date,0,-1)
    i_total_days = i_eff_date - agqname:exp_date
    i_cx_days = i_eff_date - agqname:eff_date

    include "prorata.pro"

    access agqgeneral1_alias, set agqgeneral1_alias:app_no = g_app_no, generic

    l_total = 0
    while agqgeneral1_alias:app_no = g_app_no
        begin
        change agqgeneral1_alias
            begin
            if agqgeneral1_alias:premium[1] <> 0 then
                agqgeneral1_alias:premium[5] = agqgeneral1_alias:premium[1] * i_pro_rata
            if agqgeneral1_alias:premium[2] <> 0 then
                agqgeneral1_alias:premium[6] = agqgeneral1_alias:premium[2] * i_pro_rata
            if agqgeneral1_alias:premium[3] <> 0 then
                agqgeneral1_alias:premium[7] = agqgeneral1_alias:premium[3] * i_pro_rata
            if agqgeneral1_alias:premium[4] <> 0 then
                agqgeneral1_alias:premium[8] = agqgeneral1_alias:premium[4] * i_pro_rata
            agqgeneral1_alias:total_premium[2] = agqgeneral1_alias:premium[5] +
                                                 agqgeneral1_alias:premium[6] +
                                                 agqgeneral1_alias:premium[7] +
                                                 agqgeneral1_alias:premium[8]
            end

        l_total = l_total + agqgeneral1_alias:total_premium[2]
        l_total_1 = l_total_1 + agqgeneral1_alias:total_premium[1]

        next agqgeneral1_alias
        end
    end

L_FINAL_TOTAL = L_TOTAL + L_BOILER_PREM_TOTAL

If L_IRPM = 0 Then
    L_IRPM = 1

access sfssurcharge, set sfssurcharge:company_id       = agqgeneral1:company_id,
                         sfssurcharge:state            = agqgeneral1:state,
                         sfssurcharge:line_of_business = agqgeneral1:line_of_business, generic

l_surcharge_date = 0
while sfssurcharge:company_id       = agqgeneral1:company_id and
      sfssurcharge:state            = agqgeneral1:state and
      sfssurcharge:line_of_business = agqgeneral1:line_of_business
        begin
        if (agqname:eff_date >= sfssurcharge:eff_date and
           (l_surcharge_date < sfssurcharge:eff_date or
           l_surcharge_date = 0)) then
            begin
            l_surcharge_date    = sfssurcharge:eff_date
            l_surcharge_factor  = sfssurcharge:surcharge_factor
            l_surcharge_minimum = sfssurcharge:minimum_premium
            end

        next sfssurcharge
        end

l_minimum_applies = "N"
If L_final_TOTAL <= L_MINIMUM Then
    Begin
    L_final_total = L_MINIMUM
    L_MINIMUM_APPLIES = "Y"
    end

L_SURCHARGE = L_FINAL_TOTAL * L_SURCHARGE_FACTOR
If L_SURCHARGE < L_SURCHARGE_MINIMUM Then
    L_SURCHARGE = L_SURCHARGE_MINIMUM

L_COMPANY_ID = agqgeneral1:company_id
access sfsline, set sfsline:company_id       = agqname:company_id,
                    sfsline:line_of_business = agqname:line_of_business,
                    sfsline:lob_subline      = "00", generic

if sfsline:lob_code one of "CPORTFOLIO" then
    begin
    access agqmastsupp, set agqmastsupp:app_no = g_app_no,
                            agqmastsupp:line_of_business = agqgeneral1:line_of_business, generic

    If agqmastsupp:app_no = G_app_no and
       agqmastsupp:line_of_business = agqgeneral1:line_of_business Then
        begin
        Change agqmastsupp
            Begin
            agqmastsupp:TOTAL[17] = L_TOTAL
            agqmastsupp:TOTAL[18] = L_FINAL_TOTAL
            agqmastsupp:TOTAL[20] = L_FINAL_TOTAL + L_SURCHARGE
            agqmastsupp:AMT_DUE_TOTAL[20] = L_FINAL_TOTAL + L_SURCHARGE
            agqmastsupp:TOTAL[1] = L_FINAL_TOTAL - L_BOILER_PREM_TOTAL
            agqmastsupp:other_totals[4] = L_BOILER_PREM_TOTAL
            If L_MINIMUM_APPLIES = "Y" Then
                agqmastsupp:MINIMUM_PREM_APPLIES = "Y"
            else
                agqmastsupp:minimum_prem_applies = "N"
            agqmastsupp:SURCHARGE = L_SURCHARGE
            agqmastsupp:RAW_PREMIUM_AMT_DUE = l_total
            End
        end
    end
else
    begin
    If agqmaster:app_no = agqgeneral1:app_no Then
        Change agqmaster
            Begin
            agqmaster:TOTAL[17] = L_TOTAL
            agqmaster:TOTAL[18] = L_FINAL_TOTAL
            agqmaster:TOTAL[20] = L_FINAL_TOTAL + L_SURCHARGE
            agqmaster:AMT_DUE_TOTAL[20] = L_FINAL_TOTAL + L_SURCHARGE
            agqmaster:TOTAL[1] = L_FINAL_TOTAL - L_BOILER_PREM_TOTAL
            agqmaster:OTHER_TOTALS[4] = L_BOILER_PREM_TOTAL
            agqmaster:INSTALLMENT_CHARGE = L_INSTALLMENT
            If L_MINIMUM_APPLIES = "Y" Then
                agqmaster:MINIMUM_PREM_APPLIES = "Y"
            else
                agqmaster:minimum_prem_applies = "N"
            agqmaster:SURCHARGE = L_SURCHARGE
            agqmaster:RAW_PREMIUM_AMT_DUE = l_total
            End
    end

End

Procedure RATE_ENDORSEMENTS
Begin
Access SFSOPTEND, Set SFSOPTEND:COMPANY_ID       = agqgeneral1:company_id,
                      SFSOPTEND:STATE            = agqgeneral1:STATE,
                      SFSOPTEND:LINE_OF_BUSINESS = agqgeneral1:rating_line_of_business,
                      SFSOPTEND:CODE             = L_CODE, Generic

while SFSOPTEND:COMPANY_ID       = agqgeneral1:company_id and
      SFSOPTEND:STATE            = agqgeneral1:STATE and
      SFSOPTEND:LINE_OF_BUSINESS = agqgeneral1:rating_line_of_business and
      SFSOPTEND:CODE             =  L_CODE
    begin
    If (agqname:eff_date >= SFSOPTEND:EFF_DATE and
       (agqname:eff_date <= sfsoptend:exp_date or
       sfsoptend:exp_date = 0)) Then
        Begin
        L_END_FACTOR   = SFSOPTEND:PREMIUM_1[1]
        L_END_LIMIT    = SFSOPTEND:LIMIT[1]
        L_END_PREMIUM1 = SFSOPTEND:PREMIUM_1[1]

        if l_code one of "CP0418" then
            begin
            access agqendorse5, set agqendorse5:app_no = agqend:app_no,
                                   agqendorse5:prem_no  = agqend:prem_no,
                                   agqendorse5:build_no = agqend:build_no,
                                   agqendorse5:code     = l_code, generic

            if agqendorse5:app_no = agqend:app_no and
               agqendorse5:prem_no  = agqend:prem_no and
               agqendorse5:build_no = agqend:build_no and
               agqendorse5:code     = l_code then
                begin
                l_end_premium1 = 0
                if agqendorse5:limit[1] <> 0 then
                    begin
                    if agqendorse5:limit[1] = sfsoptend:limit[1] then
                        begin
                        switch(agqgeneral1:form)
                          case 1  : l_end_premium1 = sfsoptend:premium_1[1] * l_loss_cost
                          default : l_end_premium1 = sfsoptend:premium_1[2] * l_loss_cost
                          end
                        end
                    else
                    if agqendorse5:limit[1] = sfsoptend:limit[3] then
                        begin
                        switch(agqgeneral1:form)
                          case 1  : l_end_premium1 = sfsoptend:premium_1[3] * l_loss_cost
                          default : l_end_premium1 = sfsoptend:premium_1[4] * l_loss_cost
                          end
                        end
                    else
                    if agqendorse5:limit[1] = sfsoptend:limit[5] then
                        begin
                        switch(agqgeneral1:form)
                          case 1  : l_end_premium1 = sfsoptend:premium_1[5] * l_loss_cost
                          default : l_end_premium1 = sfsoptend:premium_1[6] * l_loss_cost
                          end
                        end
                    else
                    if agqendorse5:limit[1] > sfsoptend:limit[5] then
                        begin
                        l_limit_1 = ((agqendorse5:limit[1] - sfsoptend:limit[5])
                                    divide sfsoptend:limit[7])
                        switch(agqgeneral1:form)
                          case 1  : begin
                                    l_end_premium1 = sfsoptend:premium_1[5] * l_loss_cost
                                    l_end_premium1 = (l_end_premium1 +
                                                     ((sfsoptend:premium_1[7] *
                                                     l_loss_cost) *
                                                     l_limit_1))
                                    end
                          default : begin
                                    l_end_premium1 = sfsoptend:premium_1[6] * l_loss_cost
                                    l_end_premium1 = (l_end_premium1 +
                                                     ((sfsoptend:premium_1[8] *
                                                     l_loss_cost) *
                                                     l_limit_1))
                                    end
                          end
                        end
                    end

                if agqendorse5:limit[2] <> 0 then
                    begin
                    l_end_premium1 = (l_end_premium1 +
                                     ((agqendorse5:limit[2] divide
                                     sfsoptend:limit[9]) *
                                     (sfsoptend:premium_1[9] *
                                     l_loss_cost)))
                    end
                end
            end

        if l_code one of "CP0415", "CP1430", "CP1450" then
            begin
            access agqendorse5, set agqendorse5:app_no = agqend:app_no,
                                   agqendorse5:prem_no  = agqend:prem_no,
                                   agqendorse5:build_no = agqend:build_no,
                                   agqendorse5:code     = l_code, generic

            if agqendorse5:app_no = agqend:app_no and
               agqendorse5:prem_no  = agqend:prem_no and
               agqendorse5:build_no = agqend:build_no and
               agqendorse5:code     = l_code then
                begin
                if l_code not one of "CP1450" then
                    l_end_premium1 = agqendorse5:other_premium
                else
                    l_end_premium1 = agqendorse5:premium[1]
                end
            end
        end

    Next SFSOPTEND
    End

if l_code one of "CP0418", "CP0415", "CP1430", "CP1450" then
    begin
    l_update_sfpend_premium = l_end_premium1
    end

if l_code one of "CP0403" then
    begin
    l_update_sfpend_premium = l_end_premium1
    end

if l_code one of "CP0440" then
    begin
    access agqendorse5, set agqendorse5:app_no = agqend:app_no,
                           agqendorse5:prem_no  = agqend:prem_no,
                           agqendorse5:build_no = agqend:build_no,
                           agqendorse5:code     = l_code, generic

    if agqendorse5:app_no = agqend:app_no and
       agqendorse5:prem_no  = agqend:prem_no and
       agqendorse5:build_no = agqend:build_no and
       agqendorse5:code     = l_code then
        begin
        l_state            = agqgeneral1:state
        l_line_of_business = agqgeneral1:rating_line_of_business
        l_class            = agqendorse5:option[1]
        access cpsspoilagerates, set cpsspoilagerates:company_id       = agqgeneral1:company_id,
                                     cpsspoilagerates:state            = l_state,
                                     cpsspoilagerates:line_of_business = l_line_of_business,
                                     cpsspoilagerates:class            = l_class, generic

        while cpsspoilagerates:company_id       = agqgeneral1:company_id and
              cpsspoilagerates:state            = l_state and
              cpsspoilagerates:line_of_business = l_line_of_business and
              cpsspoilagerates:class            = l_class
                begin
                if (agqname:eff_date >= cpsspoilagerates:eff_date and
                   (agqname:eff_date <= cpsspoilagerates:exp_date or
                   cpsspoilagerates:exp_date = 0)) then
                    begin
                    switch(agqendorse5:coverages[1])
                      case 1  : begin
                                if agqendorse5:coverages[2] = 1 and
                                   agqendorse5:coverages[3] = 1 then
                                    l_rate = cpsspoilagerates:rate[4]
                                else
                                if agqendorse5:coverages[2] = 1 and
                                   agqendorse5:coverages[3] = 0 then
                                    begin
                                    l_rate = cpsspoilagerates:rate[1]
                                    end
                                else
                                if agqendorse5:coverages[2] = 0 and
                                   agqendorse5:coverages[3] = 1 then
                                    l_rate = cpsspoilagerates:rate[3]
                                end
                      default : begin
                                if agqendorse5:coverages[2] = 1 and
                                   agqendorse5:coverages[3] = 1 then
                                    l_rate = cpsspoilagerates:rate[5]
                                else
                                if agqendorse5:coverages[2] = 1 and
                                   agqendorse5:coverages[3] = 0 then
                                    l_rate = cpsspoilagerates:rate[2]
                                else
                                if agqendorse5:coverages[2] = 0 and
                                   agqendorse5:coverages[3] = 1 then
                                    l_rate = cpsspoilagerates:rate[3]
                                end
                      end

                    l_selling_price = cpsspoilagerates:selling_price
                    end

                next cpsspoilagerates
                end

        if agqendorse5:coverages[4] = 1 then
            l_update_sfpend_premium = ((agqendorse5:limit[1] divide 100) *
                                      (l_rate * l_selling_price))
        else
            l_update_sfpend_premium = ((agqendorse5:limit[1] divide 100) *
                                      l_rate)
        end

    end

