    l_adj_rate_3 = agqgeneral1_alias:adj_rate[3]
    end
else
if l_coverage = "C" and
   l_code not one of "CP0405", "CP1440" and
   agqgeneral1:blanket = 0 then
    begin
    access agqgeneral1_alias, set agqgeneral1_alias:app_no  = agqgeneral1:app_no,
                                 agqgeneral1_alias:prem_no   = agqgeneral1:prem_no,
                                 agqgeneral1_alias:build_no  = agqgeneral1:build_no,
                                 agqgeneral1_alias:rate_type = agqgeneral1:rate_type, generic

    change agqgeneral1_alias
        begin
        agqgeneral1_alias:base_rate[3] = l_special
        if agqgeneral1_alias:new_rate[3] = 0 then
            begin
            agqgeneral1_alias:adj_rate[3]  = l_special *
                                            l_coinsurance_factor *
                                            l_deductible_factor_3 *
                                            l_agreed_factor *
                                            l_loss_cost *
                                            agqgeneral1_alias:company_deviation[1] *
                                            agqgeneral1_alias:package_mod[1]
            end
        else
            begin
            agqgeneral1_alias:adj_rate[3]  = agqgeneral1_alias:new_rate[3] *
                                            l_coinsurance_factor *
                                            l_deductible_factor_3 *
                                            l_agreed_factor *
                                            l_loss_cost *
                                            agqgeneral1_alias:company_deviation[1] *
                                            agqgeneral1_alias:package_mod[1]
            end

        if agqgeneral1:theft = 0 then
            begin
            if agqgeneral1:type not one of "A", "O" then
                agqgeneral1_alias:base_rate[4] = l_special_theft *
                                                l_special_relativities
            else
                begin
                l_special_relativities        = 1.00
                agqgeneral1_alias:base_rate[4] = l_special_theft
                end
            end

        if l_code not one of "CP1440", "CP0405" then
            l_limit_1 = agqgeneral1:limit[1] divide 100
        if agqgeneral1:theft = 0 then
            begin
            if agqgeneral1_alias:new_rate[4] = 0 then
                begin
                if agqgeneral1:type one of "A" then
                    begin
                    agqgeneral1_alias:adj_rate[4]  = l_special_theft *
                                                    l_deductible_factor_3 *
                                                    l_loss_cost *
                                                    agqgeneral1_alias:company_deviation[1] *
                                                    agqgeneral1_alias:package_mod[1]
                    end
                else
                    begin
                    agqgeneral1_alias:adj_rate[4]  = ((l_special_theft *
                                                    l_special_relativities *
                                                    l_deductible_factor_3 *
                                                    l_agreed_factor) divide l_limit_1)
                    agqgeneral1_alias:adj_rate[4]  = agqgeneral1_alias:adj_rate[4] *
                                                    l_loss_cost *
                                                    agqgeneral1_alias:company_deviation[1] *
                                                    agqgeneral1_alias:package_mod[1]
                    end
                end
            else
                begin
                agqgeneral1_alias:adj_rate[4]  = agqgeneral1_alias:new_rate[4] *
                                                l_deductible_factor_3 *
                                                l_agreed_factor *
                                                l_loss_cost *
                                                agqgeneral1_alias:company_deviation[1] *
                                                agqgeneral1_alias:package_mod[1]
                end
            end
        else
            begin
            agqgeneral1_alias:adj_rate[4]  = 0
            agqgeneral1_alias:base_rate[4] = 0
            agqgeneral1_alias:new_rate[4]  = 0
            end
        end

    l_adj_rate_3 = agqgeneral1_alias:adj_rate[3]
    l_theft_rate = agqgeneral1_alias:adj_rate[4]
    end
else
if l_coverage = "C" and
   l_code not one of "CP0405", "CP1440" and
   agqgeneral1:blanket = 1 then
    begin
    access agqgeneral1_alias, set agqgeneral1_alias:app_no  = agqgeneral1:app_no,
                                  agqgeneral1_alias:prem_no   = agqgeneral1:prem_no,
                                  agqgeneral1_alias:build_no  = agqgeneral1:build_no,
                                  agqgeneral1_alias:rate_type = agqgeneral1:rate_type, generic

    l_adj_rate_3 = agqgeneral1_alias:adj_rate[3]
    l_theft_rate = agqgeneral1_alias:adj_rate[4]
    end

if l_code = "CP1440" then
    begin
    l_theft_rate  = ((l_special_theft *
                    l_special_relativities *
                    l_deductible_factor_3 *
                    l_agreed_factor) divide l_limit_1)
    l_theft_rate  = l_theft_rate *
                    l_loss_cost *
                    agqgeneral1_alias:company_deviation[1] *
                    agqgeneral1_alias:package_mod[1]
    end

end

