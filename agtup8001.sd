Procedure ACCESS_SFSMSOTERROR
begin
Access SFSMSOTERROR, Set SFSMSOTERROR:COMPANY_ID       = agqgeneral1:company_id,
                         SFSMSOTERROR:STATE            = agqgeneral1:STATE,
                         SFSMSOTERROR:LINE_OF_BUSINESS = l_terror_line_of_business, generic

l_terror_percentage = 0
l_terror_minimum = 0
While SFSMSOTERROR:COMPANY_ID       = agqgeneral1:company_id And
      SFSMSOTERROR:STATE            = agqgeneral1:STATE And
      SFSMSOTERROR:LINE_OF_BUSINESS = l_terror_line_of_business
    begin
    if agqname:eff_date >= sfsmsoterror:eff_date then
        begin
        L_TERROR_EFF_DATE   = SFSMSOTERROR:EFF_DATE
        L_TERROR_PERCENTAGE = SFSMSOTERROR:PERCENTAGE
        L_TERROR_MINIMUM    = SFSMSOTERROR:MINIMUM_PREMIUM
        end

    Next SFSMSOTERROR
    end

Access SFSMSOTERROR, Set SFSMSOTERROR:COMPANY_ID       = agqgeneral1:company_id,
                         SFSMSOTERROR:STATE            = agqgeneral1:STATE,
                         SFSMSOTERROR:LINE_OF_BUSINESS = l_terror_line_of_business,
                         SFSMSOTERROR:EFF_DATE         = L_TERROR_EFF_DATE, Exact

end

Procedure ACCESS_KEY
Begin
Access SFSOPTEND, Set SFSOPTEND:COMPANY_ID       = agqgeneral1:company_id ,
                      SFSOPTEND:STATE            = agqgeneral1:STATE,
                      SFSOPTEND:LINE_OF_BUSINESS = agqgeneral1:rating_line_of_business,
                      SFSOPTEND:CODE             = L_CODE, Generic


While SFSOPTEND:COMPANY_ID       = agqgeneral1:company_id And
      SFSOPTEND:STATE            = agqgeneral1:STATE And
      SFSOPTEND:LINE_OF_BUSINESS = agqgeneral1:rating_line_of_business And
      SFSOPTEND:CODE             = L_CODE
        Begin
        If (agqname:eff_date >= SFSOPTEND:EFF_DATE and
           (agqname:eff_date <= sfsoptend:exp_date or
           sfsoptend:exp_date = 0)) Then
            Begin
            L_DESCRIPTION  = SFSOPTEND:DESCRIPTION
            L_FORM_EDITION = SFSOPTEND:FORM_EDITION
            End

        Next SFSOPTEND
        End

End

Procedure SUBCODE
Begin
Access agqend_alias, Set agqend_alias:app_no = G_app_no,
                         agqend_alias:prem_no  = 0,
                         agqend_alias:build_no = 0, generic

L_SUB_CODE = 0
while agqend_alias:app_no = G_app_no and
      agqend_alias:prem_no  = 0 and
      agqend_alias:build_no = 0
        begin
        L_SUB_CODE = agqend_alias:SUB_CODE

        Next agqend_alias
        End

End

procedure loc_totals
begin
if agqgeneral1:blanket = 0 then
    begin
    l_premium_1 = 0
    l_premium_2 = 0
    l_premium_3 = 0
    l_premium_4 = 0
    l_limit_1 = agqgeneral1:limit[1] divide 100
    if sfsline:lob_code one of "CPORTFOLIO" then
        begin
        access agqmastsupp, set agqmastsupp:app_no = g_app_no,
                                agqmastsupp:line_of_business = agqgeneral1:line_of_business, generic

        l_location_credit = agqmastsupp:irpm
        if l_location_credit = 0 then
            begin
            l_location_credit = 1.00
            if agqmastsupp:app_no = g_app_no and
               agqmastsupp:line_of_business = agqgeneral1:line_of_business then
                begin
                change agqmastsupp
                    begin
                    agqmastsupp:irpm = l_location_credit
                    end
                end
            end
        end
    else
        begin
        l_location_credit = agqmaster:irpm
        if l_location_credit = 0 then
            begin
            l_location_credit = 1.00
            access agqmaster, set agqmaster:app_no = g_app_no, generic

            change agqmaster
                begin
                agqmaster:irpm = l_location_credit
                end
            end
        end
    switch(agqgeneral1:rate_type)
      case 1, 8, 9, 10  : l_coverage = "B"
      default           : l_coverage = "C"
      end
    do base_rate_1
    l_premium_1 = l_adj_rate_1 * l_limit_1
    do base_rate_2
    l_premium_2 = l_adj_rate_2 * l_limit_1
    if l_premium_2 = 0 then
        l_premium_2 = 1.00
    switch(agqgeneral1:form)
      case 2  : do broad_rating
      case 3  : do special_rating
      end
    l_limit_1 = agqgeneral1:limit[1] divide 100
    if agqgeneral1:form = 1 then
        begin
        l_premium_3 = 0
        l_premium_4 = 0
