    l_limit_1 = agqgeneral1:limit[1]
access cpsoption5, set cpsoption5:company_id       = agqgeneral1:company_id,
                       cpsoption5:state            = agqgeneral1:state,
                       cpsoption5:line_of_business = agqgeneral1:rating_line_of_business, generic

while cpsoption5:company_id       = agqgeneral1:company_id and
      cpsoption5:state            = agqgeneral1:state and
      cpsoption5:line_of_business = agqgeneral1:rating_line_of_business
        begin
        if agqname:eff_date >= cpsoption5:eff_date then
            begin
            if l_code not one of "CP1440", "CP0405" then
                l_limit_1 = agqgeneral1:limit[1]
            switch(l_coverage)
              case "B" : switch(agqgeneral1:theft)
                           case 1  : switch(agqgeneral1:construction)
                                       case 5, 6 : l_special = cpsoption5:coverage_a_rates[4]
                                       default   : l_special = cpsoption5:coverage_a_rates[2]
                                       end
                           default : switch(agqgeneral1:construction)
                                       case 5, 6 : l_special = cpsoption5:coverage_a_rates[3]
                                       default   : l_special = cpsoption5:coverage_a_rates[1]
                                       end
                           end
              default  : begin
                         l_special = cpsoption5:coverage_b_no_theft
                         if agqgeneral1:theft = 0 then
                             begin
                             if agqgeneral1:type = "A" then
                                 l_special_theft = cpsoption5:coverage_b_residential
                             else
                             if agqgeneral1:type = "O" then
                                 begin
                                 l_special_theft = cpsoption5:coverage_b_charge
                                 end
                             else
                                 do special_other
                             end
                         end
              end

            end

        next cpsoption5
        end

if l_coverage = "B" and
   l_code not one of "CP0405", "CP1440" and
   agqgeneral1:blanket = 0 then
    begin
    access agqgeneral1_alias, set agqgeneral1_alias:app_no    = agqgeneral1:app_no,
                                  agqgeneral1_alias:prem_no   = agqgeneral1:prem_no,
                                  agqgeneral1_alias:build_no  = agqgeneral1:build_no,
                                  agqgeneral1_alias:rate_type = agqgeneral1:rate_type, generic

    change agqgeneral1_alias
        begin
        agqgeneral1_alias:base_rate[3] = l_special
        if agqgeneral1_alias:new_rate[3] = 0 then
            begin
            agqgeneral1_alias:adj_rate[3]  = l_special *
                                            l_coinsurance_factor *
                                            l_deductible_factor_3 *
                                            l_inflation_factor *
                                            l_agreed_factor *
                                            l_loss_cost *
                                            agqgeneral1_alias:company_deviation[1] *
                                            agqgeneral1_alias:package_mod[1] *
                                            l_location_credit
            end
        else
            begin
            agqgeneral1_alias:adj_rate[3]  = agqgeneral1_alias:new_rate[3] *
                                            l_coinsurance_factor *
                                            l_deductible_factor_3 *
                                            l_inflation_factor *
                                            l_agreed_factor *
                                            l_loss_cost *
                                            agqgeneral1_alias:company_deviation[1] *
                                            agqgeneral1_alias:package_mod[1] *
                                            l_location_credit
            end
        end

    l_adj_rate_3 = agqgeneral1_alias:adj_rate[3]
    end
else
if l_coverage = "B" and
   l_code not one of "CP0405", "CP1440" and
   agqgeneral1:blanket = 1 then
    begin
    access agqgeneral1_alias, set agqgeneral1_alias:app_no  = agqgeneral1:app_no,
