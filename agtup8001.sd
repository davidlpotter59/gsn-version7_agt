Procedure BOILER_MINIMUM
Begin
Access CPSPROPRATE, Set CPSPROPRATE:COMPANY_ID       = agqgeneral1:COMPANY_ID,
                        CPSPROPRATE:STATE            = agqgeneral1:STATE,
                        CPSPROPRATE:LINE_OF_BUSINESS = agqgeneral1:RATING_LINE_OF_BUSINESS, Generic

while CPSPROPRATE:COMPANY_ID       = agqgeneral1:COMPANY_ID and
      CPSPROPRATE:STATE            = agqgeneral1:STATE and
      CPSPROPRATE:LINE_OF_BUSINESS = agqgeneral1:RATING_LINE_OF_BUSINESS
    begin
    If agqname:eff_date >= CPSPROPRATE:EFF_DATE Then
        Begin
        L_MINIMUM_boiler = cpsproprate:BOILER_MINIMUM
        End

    Next cpsproprate
    End

Access agqgeneral1_ALIAS, Set agqgeneral1_ALIAS:app_no = agqgeneral1:app_no, Generic

L_BOILER_PREM_TOTAL = 0
while agqgeneral1_ALIAS:app_no = agqgeneral1:app_no
    begin
    L_BOILER_PREM_TOTAL = agqgeneral1_ALIAS:BOILERS_PREMIUM +
                          L_BOILER_PREM_TOTAL

    Next agqgeneral1_ALIAS
    End

If L_BOILER_PREM_TOTAL < L_MINIMUM_boiler And
   L_BOILER_PREM_TOTAL <> 0 Then
    Begin
    L_BOILER_PREM_TOTAL = L_MINIMUM_boiler
    Access agqgeneral1_ALIAS, Set agqgeneral1_ALIAS:app_no = agqgeneral1:app_no,
                                 agqgeneral1_ALIAS:PREM_NO  = agqgeneral1:PREM_NO,
                                 agqgeneral1_ALIAS:BUILD_NO = agqgeneral1:BUILD_NO, Generic

    Change agqgeneral1_ALIAS
        Begin
        agqgeneral1_ALIAS:BOILERS_PREMIUM = l_boiler_prem_total
        End
    End

End

