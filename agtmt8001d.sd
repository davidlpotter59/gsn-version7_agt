Panel at 3.45,75 to 4.45, 100
Properties {
           LayoutType = screen
           }
{
%%
App No: ___________

%%

fields
101     begin
        g_add_prem_no  = 0
        g_add_build_no = 0
        if function one of "ADD", "NEW" then
            begin
            access agqbusiness_alias, set agqbusiness_alias:app_no = g_app_no, generic

            if agqbusiness_alias:app_no = g_app_no then
                begin
                l_screen_1 = "agtmt8001a"
                display modal screen l_screen_1
                with initial function "CHANGE|BACK"

                if g_add_prem_no  = 0 and
                   g_add_build_no = 0 then
                    begin
                    function = "redisplay"
                    abort()
                    end

                if g_add_prem_no = 1 then
                    begin
                    l_prem_no = 0
                    access agqbusiness_alias, set agqbusiness_alias:app_no = g_app_no, generic

                    while agqbusiness_alias:app_no = g_app_no
                        begin
                        l_prem_no  = agqbusiness_alias:prem_no

                        next agqbusiness_alias
                        end

                    access agqmaster, set agqmaster:app_no = g_app_no, generic

                    access agqbusiness_alias, set agqbusiness_alias:app_no = g_app_no, generic

                    agqbusiness:app_no            = g_app_no
                    agqbusiness:prem_no           = l_prem_no + 1
                    agqbusiness:build_no          = 1
                    end
                else
                if g_add_build_no = 1 then
                    begin
                    access agqbusiness_alias, set agqbusiness_alias:app_no = g_app_no, generic

                    l_build_no = 0
                    while agqbusiness_alias:app_no = g_app_no
                        begin
                        if agqbusiness_alias:prem_no = g_prem_no then
                            l_build_no  = agqbusiness_alias:build_no

                        next agqbusiness_alias
                        end

                    access agqmaster, set agqmaster:app_no = g_app_no, generic

                    access agqbusiness_alias, set agqbusiness_alias:app_no = g_app_no, generic

                    agqbusiness:app_no           = g_app_no
                    agqbusiness:prem_no          = g_prem_no
                    agqbusiness:build_no         = l_build_no + 1
                    end
                end
            else
                begin
                agqbusiness:app_no           = g_app_no
                agqbusiness:prem_no          = 1
                agqbusiness:build_no         = 1
                end

            g_prem_no                   = agqbusiness:prem_no
            g_build_no                  = agqbusiness:build_no
            end
        end
        agqbusiness:APP_NO/displayonly ;

}

