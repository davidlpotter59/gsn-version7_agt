procedure set_display
begin
l_loop = 1
while l_loop <= 17
    begin
    l_literal[1]  = "Monthly Limit of Indemnity:"
    l_literal[2]  = "Extended Period of Indemnity:"
    l_literal[3]  = "Dependent Properties:"
    l_literal[4]  = "Agreed Value:"
    l_literal[5]  = "Premium Adjustment:"
    l_literal[6]  = "Electronic Media & Records:"
    l_literal[7]  = "Ordinary Payroll Exclusion & Limitation:"
    l_literal[8]  = "Maximum Period of Indemnity:"
    l_literal[9]  = "Mining Properties:"
    l_literal[10] = "Power, Heat & Refrigeration Deduction:"
    l_literal[11] = "Seasonal Leases:"
    l_literal[12] = "Loss Tuition:"
    l_literal[13] = "Ordinance or Law - Increased Period of Restoration:"
    l_literal[14] = "Utility Services:"
    l_literal[15] = "Loss Adjustment:"
    l_literal[16] = "Time Period:"
    l_literal[17] = "Civil Authority Increased Coverage Period:"
    l_tag_position = l_loop
    l_the_tag[l_loop] = tag("tview" + trun(str(l_loop)))
    do printguidetext(l_the_tag[l_tag_position],trun(l_literal[l_tag_position]))

    l_loop = l_loop + 1
    end

end

