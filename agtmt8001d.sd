Update
if UPDATEOK = "NO" then
    begin
    access agqbusiness_alias, set agqbusiness_alias:app_no = g_app_no, generic

    if agqbusiness_alias:app_no <> g_app_no then
        g_app_no = 0

    disable(accept_fn)
    enable(back_fn)
    end

If UPDATEOK = "YES" And
   Abort = "NO" Then
    Begin
    disable(accept_fn)
    enable(back_fn)
    if function = "DELETE" then
        begin
        access agqbusiness_alias, set agqbusiness_alias:app_no = g_app_no, generic

        access agqend, set agqend:app_no = g_app_no, generic

        while agqend:app_no = g_app_no
            begin
            if agqend:lob_end_code = "B" then
                begin
                if agqbusiness_alias:app_no <> g_app_no then
                    delete agqend
                else
                if agqend:prem_no = agqbusiness:prem_no and
                   agqend:build_no = agqbusiness:build_no then
                    delete agqend
                end

            next agqend
            end
        end


    if function one of "ADD", "NEW", "CHANGE", "CHG" then
        begin
        Access cpsbuscovtypes, Set cpsbuscovtypes:COMPANY_ID       = agqbusiness:company_id,
                                   cpsbuscovtypes:STATE            = agqbusiness:state,
                                   cpsbuscovtypes:LINE_OF_BUSINESS = agqbusiness:rating_line_of_business, generic

        l_code = ""
        While cpsbuscovtypes:COMPANY_ID       = agqbusiness:company_id And
              cpsbuscovtypes:STATE            = agqbusiness:state And
              cpsbuscovtypes:LINE_OF_BUSINESS = agqbusiness:rating_line_of_business
                Begin
                l_code = cpsbuscovtypes:code
                Do ACCESS_AGQEND
                If L_ADD_ENDORSEMENT = "Y" and
                   cpsbuscovtypes:coverage = agqbusiness:coverage then
                    Do ADD_AGQEND
                else
                if l_add_endorsement = "N" and
                   cpsbuscovtypes:coverage <> agqbusiness:coverage then
                    do delete_agqend

                Next cpsbuscovtypes
                End

        l_code_1 = cpsbusdefault:civil_code
        l_code   = ""
        if l_code_1 <> "" then
            begin
            l_code = l_code_1
            do access_agqend
            if l_add_endorsement = "Y" and
               agqbusiness:civil_authority = 1 then
                do add_agqend
            if l_add_endorsement = "N" and
               agqbusiness:civil_authority = 0 then
                do delete_agqend
            end

        Access CPSBUSMISCRATES, Set CPSBUSMISCRATES:COMPANY_ID = agqbusiness:company_id,
                                    CPSBUSMISCRATES:STATE = agqbusiness:state,
                                    CPSBUSMISCRATES:line_of_business = agqbusiness:rating_line_of_business, Generic

        l_code_1 = ""
        l_code_2 = ""
        l_code_3 = ""
        l_code_4 = ""
        l_code_5 = ""
        l_code   = ""
        While CPSBUSMISCRATES:COMPANY_ID = agqbusiness:company_id And
              CPSBUSMISCRATES:STATE = agqbusiness:state And
              CPSBUSMISCRATES:line_of_business = agqbusiness:rating_line_of_business
            Begin
            If agqNAME:EFF_DATE >= CPSBUSMISCRATES:EFF_DATE Then
                Begin
                L_CODE_1 = CPSBUSMISCRATES:PAYROLL_LIMITATION_CODE[1]
                L_CODE_2 = CPSBUSMISCRATES:time_period_code[1]
                L_CODE_3 = CPSBUSMISCRATES:time_period_code[2]
                L_CODE_4 = CPSBUSMISCRATES:dependant_prop_code[1]
                l_code_5 = cpsbusmiscrates:dependant_prop_code[2]
                End

            Next CPSBUSMISCRATES
            End


        If L_CODE_1 <> "" Then
            Begin
            L_CODE = L_CODE_1
            Do ACCESS_agqEND
            If L_ADD_ENDORSEMENT = "N" and
               agqbusiness:payroll_limitation = 0 Then
                Do delete_agqEND
            end

        If L_CODE_2 <> "" Then
            Begin
            L_CODE = L_CODE_2
            Do ACCESS_agqEND
            If L_ADD_ENDORSEMENT = "N" And
               agqbusiness:time_period = 0 Then
                Do DELETE_agqEND
            End

        If L_CODE_3 <> "" Then
            Begin
            L_CODE = L_CODE_3
            Do ACCESS_agqEND
            If L_ADD_ENDORSEMENT = "N" And
               agqbusiness:time_period = 0 Then
                Do DELETE_agqEND
            End

        If L_CODE_4 <> "" Then
            Begin
            L_CODE = L_CODE_4
            Do ACCESS_agqEND
            if l_add_endorsement = "N" and
               agqbusiness:dependent_property = 0 then
                do delete_agqend
            End

        If L_CODE_5 <> "" Then
            Begin
            L_CODE = L_CODE_5
            Do ACCESS_agqEND
            if l_add_endorsement = "N" and
               agqbusiness:dependent_property = 0 then
                do delete_agqend
            End


        Access cpsutility, Set cpsutility:COMPANY_ID       = agqbusiness:company_id,
                               cpsutility:STATE            = agqbusiness:state,
                               cpsutility:LINE_OF_BUSINESS = agqbusiness:rating_line_of_business, Generic

        l_code   = ""
        l_code_1 = ""
        While cpsutility:COMPANY_ID       = agqbusiness:company_id And
              cpsutility:STATE            = agqbusiness:state And
              cpsutility:LINE_OF_BUSINESS = agqbusiness:rating_line_of_business
            Begin
            If agqNAME:EFF_DATE >= cpsutility:EFF_DATE Then
                Begin
                L_CODE_1 = cpsutility:code
                End
            Next cpsutility
            End

        If L_CODE_1 <> "" Then
            Begin
            L_CODE = L_CODE_1
            Do ACCESS_agqEND
            If L_ADD_ENDORSEMENT = "N" and
               agqbusiness:utility = 0 Then
                Do delete_agqEND
            End


        End

    function = "redisplay"
    End

screen exit
g_coverage = agqbusiness:coverage

