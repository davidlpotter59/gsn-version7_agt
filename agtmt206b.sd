    end

Access Validation
If agqapplication_cook:app_NO <> G_app_NO then
    Error 1000

access agqapplication_cook_alias, set agqapplication_cook_alias:app_no   = agqapplication_cook:app_no, generic

l_no_locations   = 0
l_first_location = "Y"
l_first_prem_no  = 0
l_first_build_no = 0
l_last_prem_no   = 0
l_last_build_no  = 0
while agqapplication_cook_alias:app_no = agqapplication_cook:app_no
    begin
    if l_first_location = "Y" then
        begin
        l_first_str_location  = str(agqapplication_cook_alias:prem_no) +
                               str(agqapplication_cook_alias:build_no)
        l_first_location = "N"
        end
    l_last_str_location  = str(agqapplication_cook_alias:prem_no) +
                           str(agqapplication_cook_alias:build_no)

    if agqapplication_cook_alias:sub_code = 53 then
        l_no_locations  = l_no_locations + 1

    next agqapplication_cook_alias
    end

l_str_location = str(agqapplication_cook:prem_no) +
                 str(agqapplication_cook:build_no)

if l_no_locations one of 0, 1 then
    begin
    disable(next_fn)
    disable(previous_fn)
    end
else
if l_str_location = l_first_str_location and
   l_no_locations > 1 then
    begin
    disable(previous_fn)
    enable(next_fn)
    end
else
if l_str_location <> l_first_str_location and
   l_str_location <> l_last_str_location and
   l_no_locations > 1 then
    begin
    enable(next_fn)
    enable(previous_fn)
    end
else
if l_str_location = l_last_str_location and
   l_no_locations > 1 then
    begin
    disable(next_fn)
    enable(previous_fn)
    end

