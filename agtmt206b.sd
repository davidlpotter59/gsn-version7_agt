procedure definition

procedure check_agqapplication
begin
access agqapplication_hab_alias, set agqapplication_hab_alias:app_no = g_app_no, generic

if agqapplication_hab_alias:app_no = g_app_no then
    l_habitational = "Y"
else
    l_habitational = "N"

end

procedure change_current_browser (string p_server_name, string p_current_web_page)

BEGIN
  l_web_site_address = trun(p_server_name) + trun(p_current_web_page)
  setproperty(helper_tg,url,trun(l_web_site_address))
END

procedure check_popup(number l_sub_code_1)
begin
l_popup_complete = 0
access agqapplication_cook_alias, set agqapplication_cook_alias:app_no   = g_app_no,
                                      agqapplication_cook_alias:sub_code = l_sub_code_1,
                                      agqapplication_cook_alias:prem_no  = agqapplication_cook:prem_no,
                                      agqapplication_cook_alias:build_no = agqapplication_cook:build_no, generic

if agqapplication_cook_alias:screen_name one of "agtmt59d", "agtmt578", "agtmt579", "agtmt581", "agtmt582" and
   agqapplication_cook_alias:remarks <> "" then
    begin
    l_popup_complete = 1
    end

if agqapplication_cook_alias:screen_name one of "agtmt580" and
   agqapplication_cook_alias:remarks <> "" and
   agqapplication_cook_alias:liquor <> "" then
    begin
    l_popup_complete = 1
    end

end

procedure display_blue(tag l_tag, string l_color, string l_position, string l_display_screen)
begin
if l_position = "Y" and
   l_display_screen <> "" then
    begin
    setproperty(l_tag,foregroundcolor,l_color)
    end
end

procedure display_red(tag l_tag, string l_color, string l_position, string l_display_screen)
begin
if l_position = "Y" and
   l_display_screen <> "" then
    begin
    setproperty(l_tag,foregroundcolor,l_color)
    end
end

procedure display_background_red(tag l_tag, string l_color, string l_position, string l_display_screen)
begin
if l_position = "Y" then
    begin
    setproperty(l_tag,backgroundcolor,l_color)
    end
end

procedure remove_blue(tag l_tag, string l_color, string l_position, string l_display_screen)
begin
if l_position = "N" or
   l_display_screen = "" then
    begin
    setproperty(l_tag,foregroundcolor,l_color)
    end
end

procedure remove_red(tag l_tag, string l_color, string l_position, string l_display_screen)
begin
if l_position = "N" or
   l_display_screen = "" then
    begin
    setproperty(l_tag,foregroundcolor,l_color)
    end
end

procedure clear_popup_information
begin
access agqapplication_cook_alias, set agqapplication_cook_alias:app_no = g_app_no, generic

while agqapplication_cook_alias:app_no = g_app_no
    begin
    change agqapplication_cook_alias
        begin
        if agqapplication_cook_alias:screen_name = "agtmt595" and
           agqapplication_cook_alias:answer <> "Y" then
            begin
            agqapplication_cook_alias:hours = ""
            end

        if agqapplication_cook_alias:screen_name one of "agtmt594" and
           agqapplication_cook_alias:answer <> "Y" then
            begin
            agqapplication_cook_alias:remarks = ""
            end

        if agqapplication_cook_alias:screen_name one of "agtmt596" and
           agqapplication_cook_alias:answer <> "Y" then
            begin
            agqapplication_cook_alias:fuel_tank[1] = 0
            agqapplication_cook_alias:fuel_tank[2] = 0
            agqapplication_cook_alias:fuel_tank[3] = 0
            end

        if agqapplication_cook_alias:screen_name one of "agtmt597", "agtmt598", "agtmt599", "agtmt650" and
           agqapplication_cook_alias:answer <> "Y" then
            begin
            agqapplication_cook_alias:year_replaced_updated = 0
            end

        if agqapplication_cook_alias:screen_name one of "agtmt651" and
           agqapplication_cook_alias:answer <> "Y" then
            begin
            agqapplication_cook_alias:completion_date  = 0
            agqapplication_cook_alias:renovation_value = 0
            end

        end

    next agqapplication_cook_alias
    end

end

procedure access_agsapplication
begin
access agsapplication, set agsapplication:company_id         = l_company_id,
                           agsapplication:line_of_business   = l_line_of_business, generic

while agsapplication:company_id         = l_company_id and
      agsapplication:line_of_business   = l_line_of_business
        begin
        if (l_eff_date >= agsapplication:reff_date and
           (l_eff_date <= agsapplication:rexp_date or
           agsapplication:rexp_date = 0)) then
            begin
            if agsapplication:sub_code >= 53 and
               agsapplication:sub_code <= 65 then
                begin
                l_sub_code_1                  = agsapplication:sub_code
                l_screen_name[l_sub_code_1]   = agsapplication:screen_name[1]
                l_screen_name_2[l_sub_code_1] = agsapplication:screen_name[2]
                l_literal[l_sub_code_1]       = agsapplication:literal
                l_the_tag[l_sub_code_1]       = tag(trun(agsapplication:tag_name))
                end
            end

        next agsapplication
        end

end

procedure access_agqapplication_cook
begin
access agqapplication_cook_alias, set agqapplication_cook_alias:app_no = g_app_no, generic

while agqapplication_cook_alias:app_no = g_app_no
        begin
        if agqapplication_cook_alias:prem_no = agqapplication_cook:prem_no and
           agqapplication_cook_alias:build_no = agqapplication_cook:build_no then
            begin
            if agqapplication_cook_alias:answer = "Y" then
                l_answer[agqapplication_cook_alias:sub_code] = "Y"
            else
            if agqapplication_cook_alias:answer = "N" then
                l_answer[agqapplication_cook_alias:sub_code] = "N"
            else
                l_answer[agqapplication_cook_alias:sub_code] = ""
            l_highlight[agqapplication_cook_alias:sub_code] = agqapplication_cook_alias:highlight_if_yes

            if agqapplication_cook_alias:sub_code one of 53 and
               agqapplication_cook_alias:remarks <> "" then
                l_answer[agqapplication_cook_alias:sub_code] = "Y"
            end

        next agqapplication_cook_alias
        end

end

procedure update_answer
begin
access agqapplication_cook_alias, set agqapplication_cook_alias:app_no   = g_app_no,
                                     agqapplication_cook_alias:sub_code = l_sub_code_1,
                                     agqapplication_cook_alias:prem_no  = agqapplication_cook:prem_no,
                                     agqapplication_cook_alias:build_no = agqapplication_cook:build_no, generic

if agqapplication_cook_alias:app_no   = g_app_no and
   agqapplication_cook_alias:sub_code = l_sub_code_1 and
   agqapplication_cook_alias:prem_no  = agqapplication_cook:prem_no and
   agqapplication_cook_alias:build_no = agqapplication_cook:build_no then
    begin
    change agqapplication_cook_alias
        begin
        agqapplication_cook_alias:literal = l_literal[l_sub_code_1]
        if l_answer[l_sub_code_1] = "Y" then
            agqapplication_cook_alias:answer  = "Y"
        else
        if l_answer[l_sub_code_1] = "N" then
            agqapplication_cook_alias:answer  = "N"

        if l_sub_code_1 one of 53 then
            begin
            agqapplication_cook_alias:answer = "Y"
            end
        end

    if l_answer[l_sub_code_1] = "Y" or
       l_sub_code_1 one of 53 then
        begin
        if l_screen_name[l_sub_code_1] <> "" then
            begin
            g_sub_code = l_sub_code_1
            g_prem_no  = agqapplication_cook:prem_no
            g_build_no = agqapplication_cook:build_no
            l_screen = trun(l_screen_name[l_sub_code_1])
            g_literal = trun(l_literal[l_sub_code_1])
            display modal screen l_screen
            end
        end
    end

if l_answer[l_sub_code_1] = "N" and
   l_sub_code_1 not one of 53 then
    begin
    do remove_blue(l_the_tag[l_sub_code_1],"black",l_answer[l_sub_code_1],l_screen_name[l_sub_code_1])
    do remove_red(l_the_tag[l_sub_code_1],"black",l_answer[l_sub_code_1],l_screen_name[l_sub_code_1])
    do clear_popup_information
    end

end

procedure display_popup
begin
do update_answer
if l_old_answer[l_sub_code_1] one of "N", "" and
   l_answer[l_sub_code_1] one of "Y" and
   function = "CHANGE" then
    begin
    g_sub_code = l_sub_code_1
    g_prem_no  = agqapplication_cook:prem_no
    g_build_no = agqapplication_cook:build_no
    if l_screen_name[l_sub_code_1] <> "" then
        begin
        g_literal = l_literal[l_sub_code_1]
        display modal screen l_screen_name[l_sub_code_1]
        with initial function "CHANGE|BACK"
        end
    end

end

procedure display_view (number l_sub)
begin
if ((l_screen_name[l_sub] <> "" and
   l_answer[l_sub] = "Y") or
   (l_screen_name[l_sub] <> "" and
   l_sub one of 53)) then
    begin
    l_screen = l_screen_name[l_sub]
    g_sub_code = l_sub
    g_prem_no  = agqapplication_cook:prem_no
    g_build_no = agqapplication_cook:build_no
    g_literal  = trun(l_literal[l_sub])
    display modal screen l_screen
--    function = "func1"
--    abort()
    end

end

End
