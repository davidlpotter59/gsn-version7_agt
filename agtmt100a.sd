%%                 
Screen
        Name = "agtmt100a";
        Type = "Detail" ;
        Highlight=Underscore ;
        Padcharacter=Space ;
        Screensize="dynamic";
--   Windowtitle="%a     %s Policy Information - Ver. 7.22   %m";
        Domain="agqMASTER" ;
        iconline = 0 ;


        Global Unsigned Ascii Number G_app_NO[11] ,
                                     G_LINE_OF_BUSINESS[4],
                                     g_prem_no[4],
                                     g_build_no[4],
                                     G_STATE[2],
                                     g_enhancement[1] ;


        Global String G_COMPANY_ID[10] ,
                      g_inquiry[1] ;

        local wdate l_eff_date ;

        local unsigned ascii number l_sub_code[4],
                                    l_form[2],
                                    l_remove_form[1],
                                    L_STATE[2],
                                    l_agent_no[4],
                                    l_liability_code[2] ;

        local string l_code[7],
                     l_code_1[7],
                     l_delete_nonowned_hired[1],
                     l_code_2[7],
                     l_system_id[15],
                     l_username[15],
                     l_server_name[100],
                     l_current_web_page[50],
                     l_web_site_address[150],
                     l_company_id[10],
                     l_add_endorsement[1],
                     l_Form_edition[18],
                     l_description[55] ;

        Access agqMASTER,
           Set agQMASTER:app_NO = G_app_NO ;

        Include "stdkeys7.inc"

Style Definition

        Include "styles.inc"
        Displayonly Field Style {
                                  backgroundcolor="#CCCCCC"
                                }


Functions
        "change" sequence "chg"   tagged change_fn ;
        "chg" change/hidden ;
        "calc" display "agtmt200" tagged calc_fn ;
        "redisplay" display/noreturn "agtmt100a" ;
        "update_totals" display/noreturn "boqmt010" ;
--        forwardtab ;

        Include "stdfunc7.inc"

Menu Definition

Toolbar Definition

screen entry
disable(accept_fn)
access agqname, set agqname:app_no = g_app_no, generic

if agqname:quote_no <> 0 then
    begin
    disable(change_fn)
    disable(accept_fn)
    end

access sfsemail, set sfsemail:company_id = agqgeneral:company_id,
                     sfsemail:user_id    = username, generic

l_username  = username
l_username = username
l_company_id = sfsdefault:company_id

access sfsprofile,
   set sfsprofile:user_id  = l_username, exact

If SFSPROFILE:USER_ID <> L_USERNAME Then
  Begin
    L_USERNAME = "default"
    Access SFSPROFILE, Set SFSPROFILE:USER_ID = L_USERNAME, Exact
  End

if agqgeneral:company_id = "FLEMINGTON" then
    begin
    l_server_name = sfsprofile:UNIX_SERVER
    l_current_web_page = "webhelp_scr_pol_liab_limits.htm"
    do change_current_browser(l_server_name,l_current_web_page)
    end

l_system_id = "bop"
access sfssecurity_rating,
   set sfssecurity_rating:company_id = agqgeneral:company_id,
       sfssecurity_rating:user_id    = l_username,
       sfssecurity_rating:system_id  = l_system_id, generic

switch(agqgeneral:line_of_business)
  case 5  : switch(g_enhancement)
              case 1  : setproperty(t_line_of_business_1,iconname,"title_bop_enhance.gif")
              default : setproperty(t_line_of_business,iconname,"title_bop.gif")
              end
  case 50 : switch(g_enhancement)
              case 1  : setproperty(t_line_of_business_1,iconname,"title_bop_cs_enhance.gif")
              default : setproperty(t_line_of_business,iconname,"title_bop_cs.gif")
              end
  default : switch(g_enhancement)
              case 1  : setproperty(t_line_of_business_1,iconname,"")
              default : setproperty(t_line_of_business,iconname,"")
              end
  end

if g_enhancement = 1 then
    hide(t_line_of_business)
else
    hide(t_line_of_business_1)


access sfsdefault, set sfsdefault:sfs_code = "SFS", generic

if sfsdefault:company_id one of "FLEMINGTON" then
    begin
    hide(lebins_fn)
    hide(farmers_fn)
    hide(ggund_fn)
    if agqgeneral:eff_date >= 05.01.2013 then
        hide(all_others_fn)
    else
        hide(ficof_screen_fn)
    end
else
if sfsdefault:company_id one of "FARMERS" then
    begin
    hide(lebins_fn)
    hide(ficof_fn)
    hide(ficof_screen_fn)
    hide(ggund_fn)
    end
else
if sfsdefault:company_id = "LEBINS" then
    begin
    hide(other_fn)
    hide(ficof_fn)
    hide(ficof_screen_fn)
    hide(ggund_fn)
    hide(farmers_fn)
    end
else
if sfsdefault:company_id = "GGUND" then
    begin
    hide(ficof_fn)
    hide(ficof_screen_fn)
    hide(lebins_fn)
    hide(all_others_fn)
    hide(farmers_fn)
    hide(t_line_of_business_1)
    hide(t_line_of_business)
    end

