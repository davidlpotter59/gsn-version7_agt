components

Fields
101     Begin
        if function one of "ADD", "NEW", "CHG", "CHANGE" then
            begin
            enable(accept_fn)
            disable(calc_fn)
            end
        else
            begin
            disable(accept_fn)
            enable(calc_fn)
            end

        access sfscompany,
           set sfscompany:company_id = agqgeneral:company_id, generic

        do access_bpsclass_type
        l_agent_no   = agqgeneral:agent_no
        Access agsdefault_bop,
           Set agsdefault_bop:COMPANY_ID       = agqgeneral:COMPANY_ID,
               agsdefault_bop:state            = agqgeneral:state,
               agsdefault_bop:agent_no         = agqgeneral:agent_no,
               agsdefault_bop:LINE_OF_BUSINESS = agqgeneral:line_of_business, generic

        access sfsform_alias,
           set sfsform_alias:company_id       = agqgeneral:company_id,
               sfsform_alias:state            = agqgeneral:state,
               sfsform_alias:line_of_business = agqgeneral:line_of_business, generic

        End
        agqmaster:FORM/displayonly ;
101     agqmaster:form/default/tabstop
        hide if agqgeneral:company_id = "FLEMINGTON"
        l_form  = agqmaster:form
        access sfsform, set sfsform:company_id       = agqgeneral:company_id,
                            sfsform:state            = agqgeneral:state,
                            sfsform:line_of_business = agqgeneral:line_of_business,
                            sfsform:policy_form      = l_form, generic

        if agqmaster:form <> sfsform:policy_form or
           agqmaster:form = 0 then
            error 1205

        activehelp sfsform_alias:policy_form,
                   sfsform_alias:description ;
102     begin
        l_form  = agqmaster:form
        access sfsform, set sfsform:company_id       = agqgeneral:company_id,
                            sfsform:state            = agqgeneral:state,
                            sfsform:line_of_business = agqgeneral:line_of_business,
                            sfsform:policy_form      = l_form, generic
        end
        sfsform:description ;
201     Begin
        access sfsdeductible_alias, set sfsdeductible_alias:company_id = agqgeneral:company_id,
                                        sfsdeductible_alias:state      = agqgeneral:state,
                                        sfsdeductible_alias:line_of_business = agqgeneral:line_of_business, generic

        End
        agqmaster:DEDUCTIBLE/default/tabstop
        access sfsdeductible, set sfsdeductible:company_id       = agqgeneral:company_id,
                                  sfsdeductible:state            = agqgeneral:state,
                                  sfsdeductible:line_of_business = agqgeneral:line_of_business,
                                  sfsdeductible:deductible       = agqmaster:deductible, generic

        if agqmaster:deductible <> sfsdeductible:deductible then
            error 1207

        activehelp if sfsdeductible_alias:show_in_activehelp = 1 then
                       sfsdeductible_alias:deductible
                   else
                       null ;

401     Begin
        access sfsliability_alias, set sfsliability_alias:company_id       = agqgeneral:company_id,
                                       sfsliability_alias:state            = agqgeneral:state,
                                       sfsliability_alias:line_of_business = agqgeneral:line_of_business, generic

        End
        agqmaster:LIABILITY_code/default/tabstop
        l_liability_code = agqmaster:liability_code
        access sfsliability, set sfsliability:company_id       = agqgeneral:company_id,
                                 sfsliability:state            = agqgeneral:state,
                                 sfsliability:line_of_business = agqgeneral:line_of_business,
                                 sfsliability:liability_code   = l_liability_code, generic

        if agqmaster:liability_code <> sfsliability:liability_code then
            error 1238

        agqmaster:liability = sfsliability:limit

        activehelp sfsliability_alias:liability_code,
                   sfsliability_alias:limit ;
402     begin
        l_liability_code = agqmaster:liability_code
        access sfsliability, set sfsliability:company_id       = agqgeneral:company_id,
                                 sfsliability:state            = agqgeneral:state,
                                 sfsliability:line_of_business = agqgeneral:line_of_business,
                                 sfsliability:liability_code   = l_liability_code, generic
        end
        sfsliability:limit/displayonly ;
501     agqmaster:MEDICAL_LIMIT/displayonly/mask="ZZ,ZZZ,ZZ9";
601     begin
        if function one of "CHANGE", "CHG" then
            agqmaster:personal_add_injury = sfsliability:limit
        end
        agqmaster:PERSONAL_ADD_INJURY/displayonly/mask="ZZ,ZZZ,ZZ9" ;
701     Begin
        If FUNCTION one of "CHANGE", "CHG" Then
            Begin
            Access SFSLIABILITY, Set SFSLIABILITY:COMPANY_ID = agqgeneral:COMPANY_ID,
                                     SFSLIABILITY:STATE = agqgeneral:STATE,
                                     SFSLIABILITY:LINE_OF_BUSINESS = agqgeneral:LINE_OF_BUSINESS,
                                     SFSLIABILITY:LIABILITY_CODE = agqmaster:LIABILITY_CODE, Generic

            agqmaster:GENERAL_AGGREGATE = SFSLIABILITY:LIMIT * agsdefault_bop:AGGREGATE_PERCENT
            End
        End
        agqmaster:GENERAL_AGGREGATE/displayonly ;

901     Begin
        l_eff_date = agqgeneral:eff_date
        access bpsoffprem_alias, set bpsoffprem_alias:company_id       = agqgeneral:company_id,
                                     bpsoffprem_alias:state            = agqgeneral:state,
                                     bpsoffprem_alias:line_of_business = agqgeneral:line_of_business, generic

        End
        agqmaster:OFF_PREMISES_LIMIT/default/tabstop/mask="ZZ,ZZZ,ZZ9"
        access bpsoffprem, set bpsoffprem:company_id       = agqgeneral:company_id,
                               bpsoffprem:state            = agqgeneral:state,
                               bpsoffprem:line_of_business = agqgeneral:line_of_business,
                               bpsoffprem:limit            = agqmaster:off_premises_limit, generic
        if agqmaster:off_premises_limit <> bpsoffprem:limit then
            error 1238

        Access SFSPACKAGE, Set SFSPACKAGE:COMPANY_ID       = G_COMPANY_ID,
                               SFSPACKAGE:STATE            = agqgeneral:state,
                               SFSPACKAGE:LINE_OF_BUSINESS = agqgeneral:line_of_business, Generic

        while SFSPACKAGE:COMPANY_ID       = G_COMPANY_ID and
              SFSPACKAGE:STATE            = agqgeneral:state and
              SFSPACKAGE:LINE_OF_BUSINESS = agqgeneral:line_of_business
            begin
            If agqgeneral:EFF_DATE >= SFSPACKAGE:EFF_DATE Then
                agqmaster:PACKAGE_DEVIATION = SFSPACKAGE:PACKAGE_MOD

            Next SFSPACKAGE
            End

        activehelp if bpsoffprem_alias:hide_from_activehelp = 0 and
                      bpsoffprem_alias:show_in_activehelp = 0 then
                       bpsoffprem_alias:limit
                   else
                   if bpsoffprem_alias:hide_from_activehelp = 1 and
                      l_eff_date < bpsoffprem_alias:hide_from_activehelp_date then
                       bpsoffprem_alias:limit
                   else
                   if bpsoffprem_alias:show_in_activehelp = 1 and
                      l_eff_date >= bpsoffprem_alias:show_in_activehelp_date then
                       bpsoffprem_alias:limit
                   else
                       null/width=12;
1001    agqmaster:DELETE_HIRED_NONOWNED/checkbox/values="N,Y"/tabstop
        hide if agqgeneral:class_code one of 51 and
                agqgeneral:sub_code = 30 and
                agqgeneral:company_id <> "FARMERS"

        if l_delete_nonowned_hired = "Y" and
           agqmaster:delete_hired_nonowned = "N" then
            begin
            agqmaster:delete_hired_nonowned = "Y"
            error "Nonowned must be deleted on this class"
            end

        If agqmaster:DELETE_HIRED_NONOWNED = "Y" Then
            agqmaster:EXTEND_HIRED_NONOWNED = "N" ;
1101    agqmaster:EXTEND_HIRED_NONOWNED/checkbox/values="N,Y"/tabstop
        hide if agqgeneral:class_code one of 51 and
                agqgeneral:sub_code = 30
        hide if l_delete_nonowned_hired = "Y"
        Hide If agqmaster:DELETE_HIRED_NONOWNED = "Y" ;
1001    agqmaster:DELETE_HIRED_NONOWNED/checkbox/values="N,Y"/displayonly ;
1101    agqmaster:EXTEND_HIRED_NONOWNED/checkbox/values="N,Y"/displayonly ;

1201     begin
         if function one of "CHANGE", "CHG" then
            begin
            if agqmaster:extend_hired_nonowned = "" then
                agqmaster:extend_hired_nonowned = "N"
            if agqmaster:delete_hired_nonowned = "" then
                agqmaster:delete_hired_nonowned = "N"
            do update_exclusion
            if l_remove_form = 1 then
                agqmaster:exclude_terrorism = 1
            end
        end
        agqmaster:exclude_terrorism/checkbox/values="0,1"/default/tabstop ;
}

