    disable(accept_fn)
    end

if abort = "YES" then
    begin
    disable(accept_fn)
    enable(calc_fn)
    end

If UPDATEOK = "YES" And
   Abort    = "NO" Then
    Begin
    disable(accept_fn)
    enable(calc_fn)
    If FUNCTION one of "CHANGE", "CHG€" Then
        Begin
        Access agQgeneral, Set agqgeneral:app_no = agqmaster:APP_NO, Generic

        Access agqgeneral, Set agqgeneral:app_no = agqmaster:APP_NO, Generic

        while agqgeneral:app_no = agqmaster:APP_NO
            begin
            If agqgeneral:PROPERTY_LIMIT <> 0 And
               agqmaster:FORM = 3 And
               agqgeneral:MONEY_LIMIT_ON = 0 Then
                Change agqgeneral
                    Begin
                    agqgeneral:MONEY_LIMIT_ON  = agsdefault_bop:MONEY_LIMIT_ON
                    agqgeneral:MONEY_LIMIT_OFF = agsdefault_bop:MONEY_LIMIT_OFF
                    End

            If agqmaster:FORM = 1 And
               agqgeneral:ATTACH_BU5001 = "N" Then
                Change agqgeneral
                    Begin
                    agqgeneral:MONEY_LIMIT_ON = 0
                    agqgeneral:MONEY_LIMIT_OFF = 0
                    End

            If agqgeneral:DIFFERENT_OFF_PREMISES = 0 Then
                Change agqgeneral
                    Begin
                    agqgeneral:OFF_PREMISES_LIMIT = agqmaster:OFF_PREMISES_LIMIT
                    End

            Next agqgeneral
            End

        End

    if function one of "ADD", "CHANGE", "CHG" then
        begin
        Access agqgeneral_ALIAS, Set agqgeneral_ALIAS:app_NO = G_app_NO, generic

        while agqgeneral_alias:app_no = g_app_no
            begin
            change agqgeneral_alias
                begin
                agqgeneral_alias:remove_terrorism_form = agqmaster:exclude_terrorism
                end

            next agqgeneral_alias
            end

        Access agqgeneral, Set agqgeneral:app_no = g_APP_NO, generic

        Access BPSRATE, Set BPSRATE:COMPANY_ID       = agqgeneral:COMPANY_ID,
                            BPSRATE:STATE            = agqgeneral:state,
                            BPSRATE:LINE_OF_BUSINESS = agqgeneral:line_of_business, Generic

        l_code_1 = ""
        while bpsrate:company_id = agqgeneral:company_id and
              bpsrate:state = agqgeneral:state and
              bpsrate:line_of_business = agqgeneral:line_of_business
            begin
            If agqgeneral:EFF_DATE >= BPSRATE:EFF_DATE Then
                Begin
                L_CODE_1 = BPSRATE:HIRED_NONOWNED_CODE[1] -- BU5004
                L_CODE_2 = BPSRATE:HIRED_NONOWNED_CODE[2] -- BU0140
                End

            Next BPSRATE
            End

        If L_CODE_1 <> "" Then
            Begin
            -- delete Hired/Nonown
            L_CODE = L_CODE_1
            Do ACCESS_agqend
            If L_ADD_ENDORSEMENT = "Y" And
               agqmaster:DELETE_HIRED_NONOWNED = "Y" Then
                Do ADD_agqend
            If L_ADD_ENDORSEMENT = "N" And
               agqmaster:DELETE_HIRED_NONOWNED = "N" Then
                Do DELETE_agqend
            End

        If L_CODE_2 <> "" Then
            Begin
            -- extend hired/nonowned
            L_CODE = L_CODE_2
            Do ACCESS_agqend
            If L_ADD_ENDORSEMENT = "Y" And
               agqmaster:EXTEND_HIRED_NONOWNED = "Y" Then
                Do ADD_agqend
            If L_ADD_ENDORSEMENT = "N" And
               agqmaster:EXTEND_HIRED_NONOWNED = "N" Then
                Do DELETE_agqend
            End
        end

    end

Procedure Definition

procedure access_bpsclass_type
begin
access agqgeneral_alias, set agqgeneral_alias:app_no = agqgeneral:app_no, generic

l_delete_nonowned_hired = ""
while agqgeneral_alias:app_no = agqgeneral:app_no
    begin
    Access BPSCLASS_type, Set BPSCLASS_type:COMPANY_ID       = agqGENERAL_alias:COMPANY_ID ,
                              BPSCLASS_type:STATE            = agqGENERAL_alias:STATE,
                              BPSCLASS_type:LINE_OF_BUSINESS = agqGENERAL_alias:LINE_OF_BUSINESS,
                              bpsclass_type:bop_type         = agqgeneral_alias:bop_type,
                              BPSCLASS_type:CLASS_CODE       = agqGENERAL_alias:CLASS_CODE,
                              BPSCLASS_type:SUB_CODE         = agqGENERAL_alias:SUB_CODE, Generic

    while bpsclass_type:COMPANY_ID       = agqGENERAL_alias:COMPANY_ID and
          bpsclass_type:STATE            = agqGENERAL_alias:STATE and
          bpsclass_type:LINE_OF_BUSINESS = agqGENERAL_alias:LINE_OF_BUSINESS and
          bpsclass_type:bop_type         = agqgeneral_alias:bop_type and
          bpsclass_type:CLASS_CODE       = agqGENERAL_alias:CLASS_CODE and
          bpsclass_type:SUB_CODE         = agqGENERAL_alias:SUB_CODE
        begin
        If (AGQGENERAL_alias:EFF_DATE >= bpsclass_type:EFF_DATE and
           (agqgeneral_alias:eff_date <= bpsclass_type:exp_date or
           bpsclass_type:exp_date = 0)) Then
            begin
            if l_delete_nonowned_hired <> "Y" then
                l_delete_nonowned_hired = bpsclass_type:delete_nonowned_hired
            end

        Next bpsclass_type
        End

    next agqgeneral_alias
    end

end

procedure change_current_browser (string p_server_name, string p_current_web_page)

BEGIN
  l_web_site_address = trun(p_server_name) + trun(p_current_web_page)
  setproperty(helper_tg,url,trun(l_web_site_address))
END

procedure update_exclusion
begin
access agqgeneral, set agqgeneral:app_no = g_APP_NO, generic

l_remove_form = 0
while agqgeneral:app_no = g_APP_NO
        begin
        if agqgeneral:remove_terrorism_Form = 1 then
            l_remove_form = 1

        next agqgeneral
        end

end

Procedure ACCESS_agqend
Begin
Access agqend, Set agqend:app_no = g_APP_NO ,
                   agqend:PREM_NO  = 0,
                   agqend:BUILD_NO = 0, Generic

L_ADD_ENDORSEMENT = "Y"
while agqend:app_no = g_APP_NO and
      agqend:PREM_NO  = 0 and
      agqend:BUILD_NO = 0
    begin
    If agqend:CODE = L_CODE Then
        L_ADD_ENDORSEMENT = "N"

    Next agqend
    End

End

Procedure ADD_agqend
Begin
Access agqend, Set agqend:app_no = g_APP_NO ,
                   agqend:PREM_NO  = 0,
                   agqend:BUILD_NO = 0, Generic

L_SUB_CODE = 0
while agqend:app_no = g_APP_NO and
      agqend:PREM_NO  = 0 and
      agqend:BUILD_NO = 0
        begin
        L_SUB_CODE = agqend:SUB_CODE

        Next agqend
        End

L_SUB_CODE = L_SUB_CODE + 10

Access agqgeneral_ALIAS, Set agqgeneral_ALIAS:app_no = g_APP_NO,
                             agqgeneral_ALIAS:PREM_NO  = G_PREM_NO,
                             agqgeneral_ALIAS:BUILD_NO = G_BUILD_NO, Generic

Access SFSOPTEND, Set SFSOPTEND:COMPANY_ID       = agqgeneral:COMPANY_ID,
                      SFSOPTEND:STATE            = agqgeneral:state,
                      SFSOPTEND:LINE_OF_BUSINESS = agqgeneral:line_of_business,
                      SFSOPTEND:CODE             = L_CODE, Generic

while SFSOPTEND:COMPANY_ID       = agqgeneral:COMPANY_ID and
      SFSOPTEND:STATE            = agqgeneral:state and
      SFSOPTEND:LINE_OF_BUSINESS = agqgeneral:line_of_business and
      SFSOPTEND:CODE             = L_CODE
        begin
        If (agqgeneral:EFF_DATE >= SFSOPTEND:EFF_DATE and
           (agqgeneral:eff_date <= sfsoptend:exp_date or
           sfsoptend:exp_date = 0)) Then
            Begin
            L_FORM_EDITION = SFSOPTEND:FORM_EDITION
            L_DESCRIPTION  = SFSOPTEND:DESCRIPTION
            End

        Next SFSOPTEND
        End

if l_form_edition <> "" then
    begin
    Add agqend
        Begin
        agqend:app_no             =       g_APP_NO
        agqend:PREM_NO              =       0
        agqend:BUILD_NO             =       0
        agqend:SUB_CODE             =       L_SUB_CODE
        agqend:CODE                 =       L_CODE
        agqend:FORM_EDITION         =       L_FORM_EDITION
        agqend:DESCRIPTION          =       L_DESCRIPTION
        agqend:PREMIUM              =       0
        agqend:lob_end_code         =       "X"
        end
    End

End

Procedure DELETE_agqend
Begin
Access agqend, Set agqend:app_no = g_APP_NO ,
                   agqend:PREM_NO  = 0,
                   agqend:BUILD_NO = 0, Generic

while agqend:app_no = g_APP_NO and
      agqend:PREM_NO  = 0 and
      agqend:BUILD_NO = 0
        begin
        If agqend:CODE = L_CODE Then
            Delete agqend

        Next agqend
        End

End

End
