                       else
                       if l_special_class_code = "Y" then
                           l_screen_1 = "agtmt751f"
                       else
                       if l_painting = "Y" then
                           l_screen_1 = "agtmt751h"
                       else
                       if l_janitor = "Y" then
                           l_screen_1 = "agtmt751g"
                       else
                       if l_lawn_care = "Y" then
                           l_screen_1 = "agtmt751i"
                       else
                           l_screen_1 = "agtmt752"
                       function = "popup"
                       end ;
        "popup" display/noreturn l_screen_1 ;

