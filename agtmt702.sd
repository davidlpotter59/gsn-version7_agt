procedure definition

procedure change_form
begin
access agqmaster, set agqmaster:app_no = g_app_no, generic

if agqmaster:app_no = g_app_no then
    begin
    change agqmaster
        begin
        if agqmaster:form = 2 then
            begin
            agqmaster:form = 3
            end
        else
        if agqmaster:form = 3 then
            begin
            agqmaster:form = 2
            end
        end
    end

access agqcontractor_alias, set agqcontractor_alias:app_no = g_app_no, generic

while agqcontractor_alias:app_no = g_app_no
    begin
    change agqcontractor_alias
        begin
        if agqcontractor_alias:form = 2 then
            begin
            agqcontractor_alias:form = 3
            end
        else
        if agqcontractor_alias:form = 3 then
            begin
            agqcontractor_alias:form = 2
            end
        end

    next agqcontractor_alias
    end

if sfscompany:debug_mode = 0 then
    proc("cli csbatch agtup702 %s",str(g_app_no,11))/noconsole
else
    proc("cli csbatch agtup702 %s",str(g_app_no,11))
end


procedure update_exclusion
begin
access agqcontractor_alias, set agqcontractor_alias:app_no = g_APP_NO, generic

l_remove_form = 0
while agqcontractor_alias:app_no = g_APP_NO
        begin
        if agqcontractor_alias:remove_terror_Form = 1 then
            l_remove_form = 1

        next agqcontractor_alias
        end

end

procedure change_current_browser (string p_server_name, string p_current_web_page)

BEGIN
  l_web_site_address = trun(p_server_name) + trun(p_current_web_page)
  setproperty(helper_tg,url,trun(l_web_site_address))
END


procedure update_agqmaster
begin
access agqcontractor_alias, set agqcontractor_alias:app_no = g_app_no, generic

while agqcontractor_alias:app_no = g_app_no
    begin
    change agqcontractor_alias
        begin
        if l_deductible_checkbox = 1 and
           agqcontractor_alias:deductible <> l_deductible_1[1] then
            begin
            agqcontractor_alias:deductible = l_deductible_1[1]
            end
        else
        if l_deductible_checkbox = 2 and
           agqcontractor_alias:deductible <> l_deductible_1[2] then
            begin
            agqcontractor_alias:deductible = l_deductible_1[2]
            end
        else
        if l_deductible_checkbox = 3 and
           agqcontractor_alias:deductible <> l_deductible_1[3] then
            begin
            agqcontractor_alias:deductible = l_deductible_1[3]
            end
        else
        if l_deductible_checkbox = 4 and
           agqcontractor_alias:deductible <> l_deductible_1[4] then
            begin
            agqcontractor_alias:deductible = l_deductible_1[4]
            end
        else
        if l_deductible_checkbox = 5 and
           agqcontractor_alias:deductible <> l_deductible_1[5] then
            begin
            agqcontractor_alias:deductible = l_deductible_1[5]
            end
        else
        if l_deductible_checkbox = 6 and
           agqcontractor_alias:deductible <> l_deductible_1[6] then
            begin
            agqcontractor_alias:deductible = l_deductible_1[6]
            end
        end

    next agqcontractor_alias
    end

function = "redisplay"
end

procedure display_blue(tag l_tag, string l_color)
begin
    setproperty(l_tag,foregroundcolor,l_color)
end

procedure check_agqtotals
begin
access agqtotals, set agqtotals:app_no = g_app_no, generic

while agqtotals:app_no = g_app_no
    begin
    access agqcontractor_alias, set agqcontractor_alias:app_no   = agqtotals:app_no,
                                 agqcontractor_alias:prem_no  = agqtotals:prem_no,
                                 agqcontractor_alias:build_no = agqtotals:build_no, generic

    if agqtotals:prem_no <> agqcontractor_alias:prem_no or
       agqtotals:build_no <> agqcontractor_alias:build_no then
        delete agqtotals

    next agqtotals
    end

end

procedure get_heading(string l_display_heading)

begin
  postevent("agtmt010b","set_info_box",l_display_heading)
end


procedure back_a_screen
begin
  postevent("agtmt010","REMOVE_SCREEN")
  function = "func25"
end

procedure read_sfsdeductible
begin
access sfscompany, set sfscompany:company_id = agqcontractor:company_id, generic

access sfsdeductible, set sfsdeductible:company_id       = agqcontractor:company_id,
                          sfsdeductible:state            = agqcontractor:state,
                          sfsdeductible:line_of_business = agqcontractor:line_of_business, generic

l_sub = 0
while sfsdeductible:company_id       = agqcontractor:company_id and
      sfsdeductible:state            = agqcontractor:state and
      sfsdeductible:line_of_business = agqcontractor:line_of_business
        begin
        if agqcontractor:eff_date >= sfsdeductible:eff_date then
            begin
            if sfsdeductible:show_in_activehelp = 1 and
               sfsdeductible:liability = 0 then
                begin
                l_sub = l_sub + 1
                l_deductible_1[l_sub] = sfsdeductible:deductible
                if agqcontractor:deductible = l_deductible_1[l_sub] then
                    l_deductible_checkbox = l_sub
                if function <> "CHANGE" then
                    begin
                    if sfscompany:debug_mode = 0 then
                        proc("cli csbatch agtup702 %s %s",str(g_app_no,11),str(l_deductible_1[l_sub]))/noconsole
                    else
                        proc("cli csbatch agtup702 %s %s",str(g_app_no,11),str(l_deductible_1[l_sub]))
                    end
                end
            end

        next sfsdeductible
        end

access agqmaster, set agqmaster:app_no = g_app_no, generic

if function <> "CHANGE" then
    begin
    l_deductible = agqcontractor:deductible
    if sfscompany:debug_mode = 0 then
        proc("cli csbatch agtup702 %s %s",str(g_app_no,11),str(l_deductible))/noconsole
    else
