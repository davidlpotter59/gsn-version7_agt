procedure location_credit
begin
access agqcontractor_alias, set agqcontractor_alias:app_no = agqcontractor:app_no, generic

l_location_count = 0
while agqcontractor_alias:app_no = agqcontractor:app_no
    begin
    if l_prem_no <> agqcontractor_alias:prem_no then
        begin
        l_prem_no = agqcontractor_alias:prem_no
        l_location_count = l_location_count + 1
        end

    next agqcontractor_alias
    end

if l_location_count > 10 then
    l_location_count = 10
access bpslocation, set bpslocation:company_id       = agqcontractor:company_id,
                        bpslocation:state            = agqcontractor:state,
                        bpslocation:line_of_business = agqcontractor:line_of_business,
                        bpslocation:no_locations     = l_location_count, generic

while bpslocation:company_id       = agqcontractor:company_id and
      bpslocation:state            = agqcontractor:state and
      bpslocation:line_of_business = agqcontractor:line_of_business and
      bpslocation:no_locations     = l_location_count
        begin
        if (agqcontractor:eff_date >= bpslocation:eff_date and
           (agqcontractor:eff_date <= bpslocation:exp_date or
           bpslocation:exp_date = 0)) then
            begin
            if sfsline:lob_code one of "CPORTFOLIO" then
                begin
                access agqmastsupp, set agqmastsupp:app_no = agqcontractor:app_no,
                                        agqmastsupp:line_of_business = agqcontractor:line_of_business, generic

                if agqmastsupp:override_location_credit = 0 then
                    begin
                    change agqmastsupp
                        begin
                        agqmastsupp:location_credit = bpslocation:credit
                        end
                    end
                end
            else
                begin
                access agqmaster, set agqmaster:app_no = agqcontractor:app_no, exact

                if agqmaster:override_location_credit = 0 then
                    begin
                    change agqmaster
                        begin
                        agqmaster:location_credit = bpslocation:credit
                        end
                    end
                end
            end

        next bpslocation
        end

end

procedure print_quote_letter
begin
L_CSEXEC_APPLICATION = "PRINT"/raw

Access SFSCSEXEC, Set SFSCSEXEC:APPLICATION = L_CSEXEC_APPLICATION , Exact

If SFSCSEXEC:APPLICATION_NUMBER = 0 Then
    Begin
    L_CSEXEC_NUMBER = 99999
    End
Else
    Begin
    L_CSEXEC_NUMBER = SFSCSEXEC:APPLICATION_NUMBER + 1

    Change SFSCSEXEC
        Begin
        SFSCSEXEC:APPLICATION_NUMBER = L_CSEXEC_NUMBER
        End
    End

l_app           = "agq"
I_CSEXEC_NUMBER = L_CSEXEC_NUMBER
L_DEC           = L_APP+Str(I_CSEXEC_NUMBER)+".cq"
L_DEC_PDF       = L_APP+Str(I_CSEXEC_NUMBER)+".pdf"

proc("cli csbatch agqmso004 %s",str(g_app_no,11))/noconsole
Proc("cli joinit agqmso004.cq %s<nl>",l_dec)/noconsole

access sfscompany, set sfscompany:company_id = agqcontractor:company_id, generic

access agqend, set agqend:app_no = g_app_no, generic

while agqend:app_no = g_app_no
    begin
    if agqend:code = "MSC711" then
        begin
        l_form_file_name = "agqsiscds01"
        l_commandline = str(g_app_no,11) + str(agqend:prem_no,4) + str(agqend:build_no,4)
        if l_form_file_name <> "" then
            begin
            proc("cli csbatch %s %s",trun(l_form_file_name),l_commandline)/noconsole
            Proc("cli joinit %s.cq %s<nl>",trun(l_form_file_name),l_dec)/noconsole
            end
        end
    else
    if agqend:code = "MSC712" then
        begin
        l_form_file_name = "agqsiscds02"
        l_commandline = str(g_app_no,11) + str(agqend:prem_no,4) + str(agqend:build_no,4)
        if l_form_file_name <> "" then
            begin
            proc("cli csbatch %s %s",trun(l_form_file_name),l_commandline)/noconsole
            Proc("cli joinit %s.cq %s<nl>",trun(l_form_file_name),l_dec)/noconsole
            end
        end

    next agqend
    end

access agqend, set agqend:app_no = g_app_no, generic

while agqend:app_no = g_app_no
    begin
    if agqend:prem_no = 0 then
        access agqcontractor_alias, set agqcontractor_alias:app_no   = g_app_no, generic
    else
        access agqcontractor_alias, set agqcontractor_alias:app_no   = g_app_no,
                                        agqcontractor_alias:prem_no  = agqend:prem_no,
                                        agqcontractor_alias:build_no = agqend:build_no, generic

    l_code = agqend:code
    l_class_code = 99998
    access sfspolicywideoptend, set sfspolicywideoptend:company_id       = agqcontractor_alias:company_id,
                                    sfspolicywideoptend:state            = agqcontractor_alias:state,
                                    sfspolicywideoptend:line_of_business = agqcontractor_alias:line_of_business,
                                    sfspolicywideoptend:class_code       = l_class_code,
                                    sfspolicywideoptend:code             = l_code, generic

    l_form_file_name = ""
    while sfspolicywideoptend:company_id       = agqcontractor_alias:company_id and
          sfspolicywideoptend:state            = agqcontractor_alias:state and
          sfspolicywideoptend:line_of_business = agqcontractor_alias:line_of_business and
          sfspolicywideoptend:class_code       = l_class_code and
          sfspolicywideoptend:code             = l_code
        begin
        if (agqcontractor:eff_date >= sfspolicywideoptend:eff_date and
           (agqcontractor:eff_date <= sfspolicywideoptend:exp_date or
           sfspolicywideoptend:exp_date = 0)) then
            begin
            l_form_file_name = sfspolicywideoptend:form_file_name
            end

        next sfspolicywideoptend
        end

    l_commandline = str(g_app_no,11) + str(agqend:prem_no,4) + str(agqend:build_no,4)
    if l_form_file_name <> "" then
        begin
        proc("cli csbatch %s %s",trun(l_form_file_name),l_commandline)/noconsole
        Proc("cli joinit %s.cq %s<nl>",trun(l_form_file_name),l_dec)/noconsole
        end

    next agqend
    end

access agqapplication, set agqapplication:app_no = g_app_no, generic

if agqapplication:app_no = g_app_no then
    begin
    proc("cli csbatch agqmso002 %s",str(g_app_no,11))/noconsole
    Proc("cli joinit agqmso002.cq %s<nl>",l_dec)/noconsole
    end

L_LOB_CODE = SFSLINE:LOB_CODE
Access SCIPSCONTROL, Set SCIPSCONTROL:COMPANY_ID = agqcontractor:COMPANY_ID,
                         SCIPSCONTROL:STATE      = agqcontractor:STATE,
                         SCIPSCONTROL:LOB_CODE   = L_LOB_CODE,Exact

if agqcontractor:company_id = "FARMERS" then
    begin
    proc(":cli csbatch tpd4q %s",str(g_app_no,11))/noconsole
    Proc(":cli joinit tpd4q.cq %s<nl>",l_dec)/noconsole
    end

L_USERNAME = Trun(USERNAME)

Access SFSPROFILE, Set SFSPROFILE:USER_ID = L_USERNAME, Exact

If SFSPROFILE:USER_ID <> L_USERNAME Then
    Begin
    L_USERNAME = "default"
    Access SFSPROFILE, Set SFSPROFILE:USER_ID = L_USERNAME, Exact
    End

L_UNIX_PATH   = SFSPROFILE:UNIX_PATH/raw
L_UNIX_SERVER = SFSPROFILE:UNIX_SERVER/raw
L_CLIENT_PATH = SFSPROFILE:CLIENT_PATH/raw

If SFSCOMPANY:DEBUG_MODE = 0 Then
    Begin
    Proc("pcl2pdf -letter -csize -ra %s %s ",L_DEC, L_DEC_PDF)/noconsole
    Proc("chmod 777 %s ",L_DEC_PDF)/noconsole
    Proc("mv %s %s %s", L_DEC_PDF," ",Trun(L_UNIX_PATH))/noconsole
    End
Else
    Begin
    Proc("pcl2pdf -letter -csize -ra %s %s ",L_DEC, L_DEC_PDF)
    Proc("chmod 777 %s ",L_DEC_PDF)
    Proc("mv %s %s %s", L_DEC_PDF," ",Trun(L_UNIX_PATH))
    End

Proc("%s %s%s",L_CLIENT_PATH,Trun(L_UNIX_SERVER),Trun(L_DEC_PDF))/client
end

