Toolbar Definition
-- needs to be blank

Screen Entry
access agqname, set agqname:app_no = g_app_no, generic

access agqmaster, set agqmaster:app_no = g_app_no, generic

if agqname:quote_no <> 0 and
   agqcontractor:company_id = "FLEMINGTON" then
    disable(submit_fn)

if agqname:quote_no <> 0 and
   sfsemail:agent_no <> 0 and
   agqcontractor:company_id = "FARMERS" then
    disable(submit_fn)

l_user_id = username
access sfsemail, set sfsemail:company_id = agqcontractor:company_id,
                     sfsemail:user_id    = l_user_id, generic

if agqname:quote_no <> 0 and
   sfsemail:agent_no <> 0 then
    disable(submit_fn)

if sfsemail:agent_no <> 0 and
   agqname:quote_no = 0 then
    hide(quote_letter_fn)

--if sfsemail:agent_no <> 0 then
--    hide(irpm_fn)

access agqcontractor_alias, set agqcontractor_alias:app_no = g_app_no, generic

l_property_exists = "N"
while agqcontractor_alias:app_no = g_app_no
    begin
    if agqcontractor_alias:form one of 1, 2, 3 then
        l_property_exists = "Y"

    next agqcontractor_alias
    end

if l_property_exists = "N" then
    begin
    hide(property_fn)
    disable(supplemental_info_fn)
    end
else
    begin
    hide(liability_fn)
    enable(supplemental_info_fn)
    end

do check_agqtotals
Access SFSLINE, Set SFSLINE:COMPANY_ID = agqcontractor:company_id,
                    SFSLINE:LINE_OF_BUSINESS = agqcontractor:line_of_business,
                    SFSLINE:LOB_SUBLINE = "00",Generic

l_total       = 0
l_total_3     = 0
l_sub         = 0
l_sub_1       = 0
l_loc_total_1 = 0
l_loc_total_2 = 0
l_loc_total_3 = 0
l_count_locations = 0
l_territory_Rating = 0

switch(g_line_of_business)
  case 5  : switch(g_enhancement)
              case 1  : setproperty(t_line_of_business_1,iconname,"title_bop_enhance.gif")
              default : setproperty(t_line_of_business,iconname,"title_bop.gif")
              end
  case 50 : switch(g_enhancement)
              case 1  : setproperty(t_line_of_business_1,iconname,"title_bop_cs_enhance.gif")
              default : setproperty(t_line_of_business,iconname,"title_bop_cs.gif")
              end
  default : switch(g_enhancement)
              case 1  : setproperty(t_line_of_business_1,iconname,"")
              default : setproperty(t_line_of_business,iconname,"")
              end
  end

if g_enhancement = 1 then
    hide(t_line_of_business)
else
    hide(t_line_of_business_1)

access sfsdefault, set sfsdefault:sfs_code = "SFS", generic

if sfsdefault:company_id one of "FLEMINGTON" then
    hide(farmers_fn)
else
if sfsdefault:company_id one of "FARMERS" then
    hide(ficof_fn)
else
    begin
    hide(ficof_fn)
    hide(farmers_fn)
    end

l_username = username
l_company_id = sfsdefault:company_id

access sfsprofile,
   set sfsprofile:user_id  = l_username, exact

If SFSPROFILE:USER_ID <> L_USERNAME Then
  Begin
    L_USERNAME = "default"
    Access SFSPROFILE, Set SFSPROFILE:USER_ID = L_USERNAME, Exact
  End

if l_company_id = "FLEMINGTON" then
    begin
    l_server_name = sfsprofile:UNIX_SERVER
    l_current_web_page = "webhelp_scr_prop_summary.htm"
    do change_current_browser(l_server_name,l_current_web_page)
    end

access agqmaster, set agqmaster:app_no = g_app_no, generic

if agqcontractor:form = 2 then
    begin
    hide(btn_basic)
    end
else
if agqcontractor:form = 3 then
    begin
    hide(btn_expanded)
    end
else
    begin
    hide(btn_basic)
    hide(btn_expanded)
    end

if agqname:quote_no <> 0 and
   sfsdefault:company_id = "FARMERS" then
    begin
    hide(btn_basic)
    hide(btn_expanded)
    end


