        Include "stdfunc7.inc"

Menu Definition

Toolbar Definition

screen entry
access agqname, set agqname:app_no = g_app_no, generic

if agqname:quote_no <> 0 then
    begin
    disable(change_fn)
    disable(accept_fn)
    disable(add_fn)
    end

access agqvehicle, set agqvehicle:app_no = g_app_no, generic

access agqvehicle_alias, set agqvehicle_alias:app_no = g_app_no, generic

l_hide_pip = "Y"
while agqvehicle_alias:app_no = g_app_no
     begin
     if agqvehicle_alias:type = 2 then
         l_hide_pip = "N"

     next agqvehicle_alias
     end

if agqvehicle:company_id <> "FARMERS" then
    hide(additional_pip_md)

if l_hide_pip = "Y" and
   agqvehicle:company_id = "FLEMINGTON" then
    begin
    hide(additional_pip_nj)
    end

access agqmaster, set agqmaster:app_no = g_app_no, generic

access sfsemail, set sfsemail:company_id = agqvehicle:company_id,
                     sfsemail:user_id    = username, generic

l_username  = username
l_system_id = "cap"
access sfssecurity_rating,
   set sfssecurity_rating:company_id = agqvehicle:company_id,
       sfssecurity_rating:user_id    = l_username,
       sfssecurity_rating:system_id  = l_system_id, generic

switch(g_line_of_business)
  case 15 : setproperty(t_line_of_business,iconname,"title_com_auto_std.bmp")
  case 16 : setproperty(t_line_of_business,iconname,"title_com_auto_pref.bmp")
  default : setproperty(t_line_of_business,iconname,"")
  end

access sfsdefault, set sfsdefault:sfs_code = "SFS", generic

if sfsdefault:company_id one of "FLEMINGTON" then
    hide(farmers_fn)
else
if sfsdefault:company_id one of "FARMERS" then
    hide(ficof_fn)

access sfsprofile,
   set sfsprofile:user_id  = l_username, exact

If SFSPROFILE:USER_ID <> L_USERNAME Then
  Begin
    L_USERNAME = "default"
    Access SFSPROFILE, Set SFSPROFILE:USER_ID = L_USERNAME, Exact
  End

l_server_name = sfsprofile:UNIX_SERVER                            --"http://192.168.1.197/"

if l_company_id = "FLEMINGTON" then
    begin
    l_current_web_page = "034webhelp_agtmt601i_scr_broad_pip_doc.htm"
    do change_current_browser(l_server_name,l_current_web_page)
    end

access agqvehicle_alt, set agqvehicle_alt:app_no = g_app_no, generic

while agqvehicle_alt:app_no = g_app_no
    begin
    if agqvehicle_alt:type one of 2 then
        l_private_passenger = "Y"

    next agqvehicle_alt
    end

if agqvehicle:state = 29 then
    begin
    hide(additional_pip_md_pp)
    hide(additional_pip_md)
    end
else
    begin
    hide(doc)
    hide(additional_pip_nj)
    if l_private_passenger <> "Y" then
        hide(additional_pip_md_pp)
    end

access agqmaster, set agqmaster:app_no = g_app_no, generic

if agqvehicle:state = 19 then
    begin
    if agqmaster:form_of_business one of "I" then
        hide(mca842_fn)
    else
        hide(mca841_fn)
    end
else
    begin
    hide(mca841_fn)
    hide(mca842_fn)
    end

Access Validation
If agqdoc:app_NO <> G_app_NO Then
    Error 1000 ;

access agqdoc_alias, set agqdoc_alias:app_no = agqdoc:app_no, generic

l_first_str_name = ""
l_first_name = "Y"
l_last_str_name = ""
l_no_names = 0
while agqdoc_alias:app_no = agqdoc:app_no
    begin
    if l_first_name = "Y" then
        begin
        l_first_str_name  = str(agqdoc_alias:name)
        l_first_name = "N"
        end
    l_last_str_name  = str(agqdoc_alias:name)
    l_no_names  = l_no_names + 1

    next agqdoc_alias
    end

l_str_name = agqdoc:name

if l_no_names one of 0, 1 then
    begin
    disable(next_fn)
    disable(previous_fn)
    end
else
if l_str_name = l_first_str_name and
   l_no_names > 1 then
    begin
    disable(previous_fn)
    enable(next_fn)
    end
else
if l_str_name <> l_first_str_name and
   l_str_name <> l_last_str_name and
   l_no_names > 1 then
    begin
    enable(next_fn)
    enable(previous_fn)
    end
else
if l_str_name = l_last_str_name and
   l_no_names > 1 then
    begin
    disable(next_fn)
    enable(previous_fn)
    end

