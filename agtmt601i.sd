        hide if agqdoc:broadened_addl_pip = 0
        if l_prior_resident_children <> l_resident_children and
           l_resident_children <> "" then
            do update_agqpip

        if l_resident_children = "Y" and
           l_prior_resident_children = "N" then
            begin
            g_code = "MCA822"
            display modal screen "agtmt601g"
            end

        if l_resident_children = "N" and
           l_prior_resident_children = "Y" then
            begin
            access agqaddlpip, set agqaddlpip:app_no     = g_app_no,
                                   agqaddlpip:vehicle_no = g_vehicle_no, generic

            while agqaddlpip:app_no     = g_app_no and
                  agqaddlpip:vehicle_no = g_vehicle_no
                begin
                delete agqaddlpip

                next agqaddlpip
                end
            end ;

