                                    l_mca841_prior[1],
                                    l_mca842_prior[1],
                                    l_mca841[1],
                                    l_mca842[2],
                                    l_agent_no[4],
                                    l_additional_pip_option[2],
                                    l_prior_additional_pip_option[2],
                                    l_liability_code[2] ;

        local string l_code[7],
                     l_code_1[7],
                     l_name[50],
                     l_waiver_form[7],
                     l_non_employee[1],
                     l_code_2[7],
                     l_code_3[7],
                     l_named_family_members[1],
