                l_current_web_page = "062webhelp_agtmt601e_scr_no_fault_pip_cbx_children.htm"
                do change_current_browser(l_server_name,l_current_web_page)
                end
            }
        g_code = "MCA822"
        l_prior_resident_children = l_resident_children
