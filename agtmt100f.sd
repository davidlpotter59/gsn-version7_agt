procedure check_for_Red
begin
l_sub = 1
while l_sub < 30
    begin
    if l_value[l_sub] = 1 then
        begin
        l_code = l_policywide_code[l_sub]
        if l_location_specific[l_sub] = 0 then
            begin
            l_prem_no  = 0
            l_build_no = 0
            g_prem_no  = 0
            g_build_no = 0
            end
        else
            begin
            l_prem_no  = agqgeneral:prem_no
            l_build_no = agqgeneral:build_no
            g_prem_no  = agqgeneral:prem_no
            g_build_no = agqgeneral:build_no
            end
        do check_popup
        if l_popup_complete = 1 then
            do remove_red(l_the_tag[l_sub],"black",l_value[l_sub],l_popup_screen[l_sub])
        end

    l_sub = l_sub + 1
    end

end


