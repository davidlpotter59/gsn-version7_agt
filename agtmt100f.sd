procedure read_agqend
begin
access agqend, set agqend:app_no   = g_app_no,
                   agqend:prem_no  = l_prem_no,
                   agqend:build_no = l_build_no, generic

while agqend:app_no   = g_app_no and
      agqend:prem_no  = l_prem_no and
      agqend:build_no = l_build_no
    begin
    if l_policywide_code[l_tag_position] one of "BU0601A" then
        begin
        if agqend:code one of "BU0601", "BU0602", "BU0603", "BU0604", "BU0605",
                              "BU0606", "BU0607", "BU0608", "BU0609", "BU0610",
                              "BU0616", "BU0300", "BU0301", "BU0304", "BU0321",
                              "BU0327", "BU0315", "BU0309" then
            l_value[l_tag_position] = 1
        end
    else
    if l_policywide_code[l_tag_position] one of "MIM010A" then
        begin
        if agqend:code one of "MIM010", "MIM011", "MIM013", "MIM101", "MIM102",
                              "MIM103", "MIM111", "MIM112", "MIM113", "MIM114",
                              "MIM115" then
            l_value[l_tag_position] = 1
        end
    else
    if l_policywide_code[l_tag_position] one of "FOS174" then
        l_value[l_tag_position] = 1
    else
    if agqend:code = l_policywide_code[l_tag_position] then
        begin
        l_value[l_tag_position] = 1
        end

    next agqend
    end

end

