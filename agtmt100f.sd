                       agqgeneral:prem_no  = l_prem_no,
                       agqgeneral:build_no = l_build_no, generic

access sfspolicywideoptend, set sfspolicywideoptend:company_id       = agqgeneral:company_id,
                                sfspolicywideoptend:state            = agqgeneral:state,
                                sfspolicywideoptend:line_of_business = agqgeneral:line_of_business,
                                sfspolicywideoptend:class_code       = agqgeneral:class_code, generic

l_tag_position = 0
while sfspolicywideoptend:company_id       = agqgeneral:company_id and
      sfspolicywideoptend:state            = agqgeneral:state and
      sfspolicywideoptend:line_of_business = agqgeneral:line_of_business and
      sfspolicywideoptend:class_code       = agqgeneral:class_code
    {
    if (agqgeneral:eff_date >= sfspolicywideoptend:eff_date and
       (agqgeneral:eff_date <= sfspolicywideoptend:exp_date or
       sfspolicywideoptend:exp_date = 0)) then
        {
        if agqgeneral:bop_type one of 4, 5 then
            begin
            if sfspolicywideoptend:code not one of "BU0284", "BU0283", "BU0285", "BU0290" then
                begin
                l_tag_position = l_tag_position + 1 --val(sfspolicywideoptend:tag_name[7,8])
                l_policywide_code[l_tag_position] = sfspolicywideoptend:code
                l_value[l_tag_position] = 0
                if sfspolicywideoptend:code one of "BU0140" then
                    begin
                    l_prem_no  = 0
                    l_build_no = 0
                    end
                else
                if sfspolicywideoptend:code one of "SIR007", "BU0504", "BU0291" , "BU0282" and
                   agqgeneral:company_id = "GGUND" then
                    begin
                    l_prem_no = 0
                    l_build_no = 0
                    end
                else
                    begin
                    l_prem_no  = agqgeneral:prem_no
                    l_build_no = agqgeneral:build_no
