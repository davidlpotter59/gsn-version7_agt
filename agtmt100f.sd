                l_popup_screen[l_tag_position] = sfspolicywideoptend:popup_screen
                l_the_tag[l_tag_position] = tag("t_view" + trun(str(l_tag_position)))
                l_location_specific[l_tag_position] = sfspolicywideoptend:location_specific
                l_refer_to_co[l_tag_position] = sfspolicywideoptend:refer_to_company
                l_hide_from_agent[l_tag_position] = sfspolicywideoptend:hide_from_agent
                do printguidetext(l_the_tag[l_tag_position],trun(l_view_desc[l_tag_position]))
                end
            end
        else
        if agqgeneral:bop_type one of 6, 8 then
            begin
            if sfspolicywideoptend:code not one of "BU0286", "BU0287", "BU0288" then
                begin
                l_tag_position = l_tag_position + 1 --val(sfspolicywideoptend:tag_name[7,8])
