procedure update_endorsements(number l_sub)
begin
if l_value[l_sub] = 1 then
    begin
    if l_location_specific[l_sub] = 0 then
        begin
        l_prem_no  = 0
        l_build_no = 0
        g_prem_no  = 0
        g_build_no = 0
        end
    else
        begin
        l_prem_no  = agqgeneral:prem_no
        l_build_no = agqgeneral:build_no
        g_prem_no  = agqgeneral:prem_no
        g_build_no = agqgeneral:build_no
        end
    l_code = l_policywide_code[l_sub]
    if l_code = "BU0140" then
        begin
        l_prem_no = 0
        l_build_no = 0
        end

        if l_code one of "BU0142" and
           agqgeneral:building_limit = 0 then
            begin
            error "Can not add this form without building coverage"
            l_value[l_sub] = 0
            abort()
            end
    if l_code one of "SIR007", "BU0504", "BU0291", "BU0282" and
       agqgeneral:company_id = "GGUND" then
        begin
        l_prem_no = 0
        l_build_no = 0
        end
    g_code = l_policywide_code[l_sub]
    do add_endorsements
    if l_popup_screen[l_sub] <> "" then
        begin
        if l_location_specific[l_sub] = 0 then
            begin
            l_prem_no  = 0
            l_build_no = 0
            g_prem_no  = 0
            g_build_no = 0
            end
        else
            begin
            l_prem_no  = agqgeneral:prem_no
            l_build_no = agqgeneral:build_no
            g_prem_no  = agqgeneral:prem_no
            g_build_no = agqgeneral:build_no
            end
        if l_code = "BU0140" then
            begin
            l_prem_no = 0
            l_build_no = 0
            end
        if l_code one of "SIR007", "BU0504", "BU0291", "BU0282" and
           agqgeneral:company_id = "GGUND" then
            begin
            l_prem_no = 0
            l_build_no = 0
            end

        l_screen = trun(l_popup_screen[l_sub])
        display modal screen l_screen
--        function = "func1"
--        abort()
        end
--    else
--        begin
--        abort()
--        end
    end

if l_value[l_sub] = 0 then
    begin
    do remove_blue(l_the_tag[l_sub],"black",l_value[l_sub],l_popup_screen[l_sub])
    do remove_red(l_the_tag[l_sub],"black",l_value[l_sub],l_popup_screen[l_sub])
    if l_location_specific[l_sub] = 0 then
        begin
        l_prem_no  = 0
        l_build_no = 0
        end
    else
        begin
        l_prem_no  = agqgeneral:prem_no
        l_build_no = agqgeneral:build_no
        end
    l_code = l_policywide_code[l_sub]
    if l_code = "BU0140" then
        begin
        l_prem_no = 0
        l_build_no = 0
        end
    if l_code one of "SIR007", "BU0504", "BU0291", "BU0282" and
       agqgeneral:company_id = "GGUND" then
        begin
        l_prem_no = 0
        l_build_no = 0
        end
    do delete_endorsements
--    abort()
    end

