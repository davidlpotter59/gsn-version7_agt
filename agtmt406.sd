                                    l_checkbox_2[1],
                                    l_checkbox_3[1],
                                    l_checkbox_4[1],
                                    l_checkbox_5[1],
                                    l_checkbox_6[1],
                                    l_checkbox_7[1],
                                    l_csexec_number[12],
                                    i_csexec_number[12],
                                    l_restricted[1]=0,
