procedure read_agqcontractor
begin
access agqcontractor, set agqcontractor:app_no = g_app_no, generic

l_property = "N"
while agqcontractor:app_no = g_app_no
    begin
    if agqcontractor:form one of 1, 2, 3 then
        l_property = "Y"

    next agqcontractor
    end

end

procedure change_current_browser (string p_server_name, string p_current_web_page)

BEGIN
  l_web_site_address = trun(p_server_name) + trun(p_current_web_page)
  setproperty(helper_tg,url,trun(l_web_site_address))
END

procedure check_popup(number l_sub_code_1)
begin
access agqapplication, set agqapplication:app_no   = g_app_no,
                           agqapplication:sub_code = l_sub_code_1, generic

l_popup_complete = 0

if agqapplication:screen_name one of "agtmt563" and
   agqapplication:final_hook_up one of "N", "Y" and
   agqapplication:remarks <> "" then
    begin
    l_popup_complete = 1
    end

if agqapplication:screen_name = "agtmt560" and
   agqapplication:transfer_flammable one of "N", "Y" and
   agqapplication:transfer_chemicals one of "N", "Y" and
   agqapplication:transfer_gases one of "N", "Y" and
   agqapplication:transfer_caustics one of "N", "Y" then
    begin
    l_popup_complete = 1
    end

if agqapplication:screen_name one of "agtmt562" and
   agqapplication:certified one of "N", "Y" then
    l_popup_complete = 1

if agqapplication:screen_name one of "agtmt561" and
   agqapplication:roof_installations not one of 0 and
   agqapplication:cranes_used one of "N", "Y" and
   agqapplication:cranes_leased one of "N", "Y" then
    begin
    l_popup_complete = 1
    end

if agqapplication:screen_name one of "agtmt566" and
   agqapplication:percentage_spray_painting <> 0 then
    l_popup_complete = 1

if agqapplication:screen_name one of "agtmt570" and
   agqapplication:no_units[1] <> 0 then
    l_popup_complete = 1

if agqapplication:screen_name one of "agtmt503", "agtmt565", "agtmt568", "agtmt569", "agtmt559" and
   agqapplication:remarks <> "" then
    begin
    l_popup_complete = 1
    end

end

procedure display_blue(tag l_tag, string l_color, string l_position, string l_display_screen)
begin
if l_position = "Y" and
   l_display_screen <> "" then
    begin
    setproperty(l_tag,foregroundcolor,l_color)
    end
end

procedure display_red(tag l_tag, string l_color, string l_position, string l_display_screen)
begin
if l_position = "Y" and
   l_display_screen <> "" then
    begin
    setproperty(l_tag,foregroundcolor,l_color)
    end
end

procedure display_background_red(tag l_tag, string l_color, string l_position, string l_display_screen)
begin
if l_position = "Y" then
    begin
    setproperty(l_tag,backgroundcolor,l_color)
    end
end

procedure remove_blue(tag l_tag, string l_color, string l_position, string l_display_screen)
begin
if l_position = "N" or
   l_display_screen = "" then
    begin
    setproperty(l_tag,foregroundcolor,l_color)
    end
end

procedure remove_red(tag l_tag, string l_color, string l_position, string l_display_screen)
begin
if l_position = "N" or
   l_display_screen = "" then
    begin
    setproperty(l_tag,foregroundcolor,l_color)
    end
end

procedure clear_popup_information
begin
access agqapplication_alias, set agqapplication_alias:app_no = g_app_no, generic

while agqapplication_alias:app_no = g_app_no
    begin
    change agqapplication_alias
        begin
        if agqapplication_alias:screen_name = "agtmt554" and
           agqapplication_alias:answer = "Y" then
            begin
            agqapplication_alias:hold_harmless = ""
            agqapplication_alias:written_contracts = ""
            agqapplication_alias:additional_insured_sub = ""
            agqapplication_alias:carry_liability_limits = ""
            agqapplication_alias:certificates_obtained = ""
            end

        if agqapplication_alias:screen_name = "agtmt510" and
           agqapplication_alias:answer <> "Y" then
            begin
            agqapplication_alias:hours = ""
            end

        if agqapplication_alias:screen_name = "agtmt511" and
           agqapplication_alias:answer <> "Y" then
            begin
            agqapplication_alias:total_number[1] = 0
            end

        if agqapplication_alias:screen_name one of "agtmt503" and
           agqapplication_alias:answer <> "Y" then
            begin
            agqapplication_alias:remarks = ""
            end

        if agqapplication_alias:screen_name one of "agtmt556" and
           agqapplication_alias:answer <> "Y" then
            begin
            agqapplication_alias:percent_new_york = 0
            agqapplication_alias:remarks = ""
            end

        if agqapplication_alias:screen_name one of "agtmt557" and
           agqapplication_alias:answer <> "Y" then
            agqapplication_alias:website = ""

        if agqapplication_alias:screen_name one of "agtmt503" and
           agqapplication_alias:answer <> "Y" then
            begin
            agqapplication_alias:remarks = ""
            end

        if agqapplication_alias:screen_name one of "agtmt512" and
           agqapplication_alias:answer <> "Y" then
            begin
            agqapplication_alias:fuel_tank[1] = 0
            agqapplication_alias:fuel_tank[2] = 0
            agqapplication_alias:fuel_tank[3] = 0
            end

        if agqapplication_alias:screen_name one of "agtmt513", "agtmt514", "agtmt515", "agtmt516" and
           agqapplication_alias:answer <> "Y" then
            begin
            agqapplication_alias:year_replaced_updated = 0
            end

        if agqapplication_alias:screen_name one of "agtmt517" and
           agqapplication_alias:answer <> "Y" then
            begin
            agqapplication_alias:completion_date  = 0
            agqapplication_alias:renovation_value = 0
            end

        end

    next agqapplication_alias
    end

end

