Procedure Definition

Procedure fraud_warning
begin
l_line_counter = 1
Access SCIPSFRAUD, Set SCIPSFRAUD:COMPANY_ID = L_COMPANY_ID, approximate

Writefile(L_FILE_NAME,"%s",l_arial_bold_9)
Writefile(L_FILE_NAME,"%s","<033>&a200h6700V")
Writefile(L_FILE_NAME,"%s","FRAUD WARNING")
Writefile(L_FILE_NAME,"%s",l_arial_bold_7)
Writefile(L_FILE_NAME,"%s","<033>&a3200h+0V")
Writefile(L_FILE_NAME,"%s","APPLICANT'S SIGNATURE")
if agqname:company_id one of "FLEMINGTON" then
  {
    Writefile(L_FILE_NAME,"%s","<033>&a4900h+0V")
    Writefile(L_FILE_NAME,"%s","DATE")
  }
Writefile(L_FILE_NAME,"%s",l_arial_8)
Writefile(L_FILE_NAME,"%s","<033>&a200h+120V")

l_max_lines        = 60
L_FONT             = l_arial_8
L_V_POSITION       = "+0"
L_H_POSITION       = "200"
L_CHARACTER_STRING = trun(SCIPSFRAUD:FRAUD_WARNING)
L_MAX_CHAR         = 75
include "line_spacing.inc"

if agqname:company_id one of "FLEMINGTON" then
  {
    Writefile(L_FILE_NAME,"%s","<033>&a3200h6950V<033>*c2402h5v0P")
  }
Else
  {
    Writefile(L_FILE_NAME,"%s","<033>&a3200h6950V<033>*c2037h5v0P")
  }
end

Procedure questions
begin
l_line_counter = 9

Access agqAPPLICATION, Set agqAPPLICATION:app_no = l_app_no, approximate

While agqAPPLICATION:app_no = l_app_no
    begin
    If l_max_lines - l_line_counter <= 3 then
        begin
        if agqpersonal:company_id <> "FARMERS" then
            do fraud_warning
        else
            Writefile(L_FILE_NAME,"%s%s%s%s%n",
                      L_ARIAL_BOLD_9,
                      "<033>&a5000h7250V",
                      l_arial,
                      "Page ",
                      L_PAGE_NUMBER)
        l_page_number = l_page_number + 1
        Writefile(L_FILE_NAME,"%s","<FF>")
        l_line_counter = 1
        l_next_pos = l_line_counter * 120
        l_remarks_pos = l_next_pos
        do draw_columns
        end

    If SFSAPPCONTROL:hide_more_applicant_info = 0 then
        {
        Writefile(L_FILE_NAME,"%s",l_arial_bold_9)
        Writefile(L_FILE_NAME,"%s%n%s","<033>&a3285h",l_next_pos,"V")
        }

    If agqAPPLICATION:ANSWER = "N" and
       agqapplication:dont_print_with_app_questions = 0 then
        {
        Writefile(L_FILE_NAME,"%s",l_arial_bold_9)
        Writefile(L_FILE_NAME,"%s%n%s","<033>&a3310h",l_next_pos,"V")
        Writefile(L_FILE_NAME,"%s","NO")
        }

    If agqAPPLICATION:ANSWER = "Y" and
