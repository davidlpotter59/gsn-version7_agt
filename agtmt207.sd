procedure definition

procedure change_current_browser (string p_server_name, string p_current_web_page)

BEGIN
  l_web_site_address = trun(p_server_name) + trun(p_current_web_page)
  setproperty(helper_tg,url,trun(l_web_site_address))
END

procedure check_files
begin
l_checkbox_1 = 1
l_checkbox_2 = 1
l_checkbox_4 = 1

access agqend, set agqend:app_no = g_app_no, generic

if agqend:app_no = g_app_no then
    begin
    l_checkbox_3 = 1
    do check_endorsement_edition_dates
    end

access agqname, set agqname:app_no = g_app_no, generic

access agqmaster, set agqmaster:app_no = g_app_no, generic

if agqname:name[1] = "" or
   agqname:address[1] = "" or
   agqname:city = "" or
   agqname:str_state = "" or
   agqname:str_zipcode = "" then
    l_checkbox_5 = 0
else
    l_checkbox_5 = 1

access agqgeneral_alias, set agqgeneral_alias:app_no = g_app_no, generic

l_checkbox_6 = 1
while agqgeneral_alias:app_no = g_app_no
    begin
    access agqlocation, set agqlocation:app_no   = agqgeneral_alias:app_no,
                            agqlocation:prem_no  = agqgeneral_alias:prem_no,
                            agqlocation:build_no = agqgeneral_alias:build_no, generic

    if agqlocation:address = "" or
       agqlocation:city = "" or
       agqlocation:str_state = "" or
       agqlocation:str_zipcode = "" or
       agqgeneral_alias:business_desc[1] = "" then
        l_checkbox_6 = 0

    next agqgeneral_alias
    end

if sfsdefault:company_id <> "GGUND" then
    begin
    access agqapplication, set agqapplication:app_no = g_app_no, generic

    l_checkbox_7 = 1
    while agqapplication:app_no = g_app_no
        begin
        if agqapplication:literal <> "" and
           agqapplication:answer not one of "N", "Y" and
           agqapplication:sub_code < 60 then
            begin
            l_checkbox_7 = 0
            end

        next agqapplication
        end
    end
else
    begin
    l_checkbox_7 = 1
    access agqapplication, set agqapplication:app_no = g_app_no, generic

    while agqapplication:app_no = g_app_no
        begin
        if agqapplication:literal <> "" and
           agqapplication:answer not one of "N", "Y" and
           agqapplication:sub_code <= 15 then
            l_checkbox_7 = 0

        next agqapplication
        end

    access agqapplication_loc, set agqapplication_loc:app_no = g_app_no, generic

    while agqapplication_loc:app_no = g_app_no
        begin
        if agqapplication_loc:literal <> "" and
           agqapplication_loc:answer not one of "N", "Y" and
           agqapplication_loc:sub_code >= 16 and
           agqapplication_loc:sub_code <= 36 then
            l_checkbox_7 = 0

        next agqapplication_loc
        end

    access agqapplication_cook, set agqapplication_cook:app_no = g_app_no, generic

    while agqapplication_cook:app_no = g_app_no
        begin
        if agqapplication_cook:literal <> "" and
           agqapplication_cook:answer not one of "N", "Y" and
           agqapplication_cook:sub_code >= 53 and
           agqapplication_cook:sub_code <= 65 then
            l_checkbox_7 = 0

        next agqapplication_cook
        end

    access agqapplication_hab, set agqapplication_hab:app_no = g_app_no, generic

    while agqapplication_hab:app_no = g_app_no
        begin
        if agqapplication_hab:literal <> "" and
           agqapplication_hab:answer not one of "N", "Y" and
           agqapplication_hab:sub_code >= 53 and
           agqapplication_hab:sub_code <= 65 then
            l_checkbox_7 = 0

        next agqapplication_hab
        end

    end

access agqapplication, set agqapplication:app_no = g_app_no, generic

