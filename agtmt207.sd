%%
Screen
        Name = "agtmt207" ;
        Type = "Detail" ;
        Padcharacter = Space ;
        Highlight = Underscore ;
        Screensize = "dynamic" ;
        Domain = "agqgeneral" ;
        resizeable = yes ;
        iconline = 0 ;

        Global Unsigned Ascii Number G_app_NO[11] ;
        Global Unsigned Ascii Number G_PREM_NO[4] ;
        Global Unsigned Ascii Number G_BUILD_NO[4] ;
        Global Unsigned Ascii Number g_line_of_business[4],
                                     g_state[2],
                                     g_enhancement[1] ;

        global string g_zipcode_change[1] ,
                      g_initial_screen[50] ;


        Local Unsigned Ascii Number L_LINE_OF_BUSINESS[4]=0,
                                    l_val_zipcode[5],
                                    l_zipcode_5[5],
                                    l_checkbox_1[1],
                                    l_checkbox_2[1],
                                    l_checkbox_3[1],
                                    l_checkbox_4[1],
                                    l_checkbox_5[1],
                                    l_checkbox_6[1],
                                    l_checkbox_7[1],
                                    l_csexec_number[12],
                                    i_csexec_number[12],
                                    l_restricted[1]=0,
                                    l_class_code[5],
                                    l_restricted_distance[5]=0/dec=2,
                                    l_tier[2]=0 ;

        local string l_zipcode[9],
                     l_csexec_application[25],
                     l_cooking_equipment[1],
                     l_habitational[1],
                     l_form_file_name[20],
                     l_app[3]="agq",
                     l_dec[15],
                     l_screen[9],
                     l_business_desc_1[30],
                     l_business_desc_2[30],
                     l_continue_1[3],
                     l_str_zipcode[9],
                     l_str_state[2],
                     l_code[7],
                     l_form_edition[25],
                     l_description[75],
                     l_commandline[19],
                     l_sub_line[2],
                     l_server_name[100],
                     l_current_web_page[50],
                     l_web_site_address[150],
                     l_user_id[15]="",
                     l_lob_code[10],
                     L_COMPANY_ID[10] ;

        include "swsut100.var"

        include "virtualdecs.var"

        include "print.var"

        Access agqgeneral,
           Set agqgeneral:app_NO = G_app_NO, generic ;

        Include "stdkeys7.inc"

Style definition
                include "styles.inc"
                Displayonly Field Style {
                                  backgroundcolor="#CCCCCC"
                                }

Functions

        "func1" sequence "final" tagged submit_fn ;
        "final" continue begin
                         if l_checkbox_1 = 1 and
                            l_checkbox_2 = 1 and
                            l_checkbox_3 = 1 and
                            l_checkbox_4 = 1 and
                            l_checkbox_5 = 1 and
                            l_checkbox_6 = 1 and
                            l_checkbox_7 = 1 then
                             begin
                             l_continue_1 = dialog("Warning","question.bmp","You are about to submit your application to underwriting, do you wish to continue?","Yes","No")
                             if l_continue_1 = "Yes" then
                                 begin
                                 if agqgeneral:company_id = "FARMERS" then
                                     l_screen = "agtmt208b"
                                 else
                                     l_screen = "agtmt208"
                                 function = "popup"
                                 end
                             end
                         else
                             error "You need to complete all the information before you can submit to underwriting"
                         end ;
        "popup" display/noreturn l_screen ;

        "func2" display "agtmt200" tagged back_fn ;

        "func3" sequence "worksheet"  tagged worksheet_fn ;
        "worksheet" continue begin
                             do create_macro
                             end  ;

        "func4" sequence "quoteletter"  tagged quote_letter_fn ;
        "quoteletter" continue begin
                               do print_quote_letter
                               end ;
        "func5"  sequence "general" tagged rating_info_fn ;
        "general" continue begin
                           if sfsdefault:company_id = "GGUND" then
                               l_screen = "agtmt100l"
                           else
                               l_screen = "agtmt100"
                           function = "disscreen"
                           end ;
        "func6" display/noreturn "agtmt100a" tagged policy_info_fn ;
        "func7" sequence "optend" tagged optional_info_fn ;
        "optend" continue begin
                          if agqgeneral:company_id = "FLEMINGTON" then
                              l_screen = "agtmt100c"
                          else
                              l_screen = "agtmt100f"
                          function = "disscreen"
                          end ;
        "disscreen" display/noreturn l_screen ;
        "func8" sequence "supp" tagged supplemental_info_fn ;
        "supp" continue begin
                        if agqgeneral:company_id one of "LEBINS", "GGUND" then
                            l_screen = "agtmt100h"
                        else
                            l_screen =  "agtmt100b"
                        function = "disscreen"
                        end ;
        "func9" display/noreturn "agtmt201" tagged mailing_address_fn ;
        "func10" display/noreturn "agtmt202m" tagged location_address_fn ;
        "func11" sequence "application" tagged application_fn ;
        "application" continue begin
                               if agqgeneral:company_id = "GGUND" then
                                   l_screen = "agtmt204a"
                               else
                                   l_screen = "agtmt204"
                               function = "disscreen"
                               end ;

        Include "stdfunc7.inc"

        reaccess ;

EVENT DEFINITION
        default eventhandler {
                                "REMOVE_SCREEN"  { function = "exit" abort () }
                             }

Toolbar Definition
-- needs to be blank

Screen Entry
l_user_id = username
l_username = username
l_company_id = sfsdefault:company_id

access sfsprofile,
   set sfsprofile:user_id  = l_username, exact

If SFSPROFILE:USER_ID <> L_USERNAME Then
  Begin
    L_USERNAME = "default"
    Access SFSPROFILE, Set SFSPROFILE:USER_ID = L_USERNAME, Exact
  End

if l_company_id = "FLEMINGTON" then
    begin
    l_server_name = sfsprofile:UNIX_SERVER
    l_current_web_page = "webhelp_scr_final_check.htm"
    do change_current_browser(l_server_name,l_current_web_page)
    end

g_initial_screen = ""
switch(g_line_of_business)
  case 5  : switch(g_enhancement)
              case 1  : setproperty(t_line_of_business_1,iconname,"title_bop_enhance.gif")
              default : setproperty(t_line_of_business,iconname,"title_bop.gif")
              end
  case 50 : switch(g_enhancement)
              case 1  : setproperty(t_line_of_business_1,iconname,"title_bop_cs_enhance.gif")
              default : setproperty(t_line_of_business,iconname,"title_bop_cs.gif")
              end
  default : switch(g_enhancement)
              case 1  : setproperty(t_line_of_business_1,iconname,"")
              default : setproperty(t_line_of_business,iconname,"")
              end
  end

if g_enhancement = 1 then
    hide(t_line_of_business)
else
    hide(t_line_of_business_1)

access sfsdefault, set sfsdefault:sfs_code = "SFS", generic

access sfscompany, set sfscompany:company_id = sfsdefault:company_id, generic

if sfsdefault:company_id one of "FLEMINGTON" then
    begin
    hide(lebins_fn)
    hide(farmers_fn)
    hide(ggund_fn)
    end
else
if sfsdefault:company_id one of "FARMERS" then
    begin
    hide(lebins_fn)
    hide(ficof_fn)
    hide(ggund_fn)
    end
else
if sfsdefault:company_id one of "LEBINS" then
    begin
    hide(ficof_fn)
    hide(farmers_fn)
    hide(ggund_fn)
    hide(application_fn)
    end
else
if sfsdefault:company_id one of "GGUND" then
    begin
    hide(ficof_fn)
    hide(farmers_fn)
    hide(lebins_fn)
    hide(t_line_of_business)
    hide(t_line_of_business_1)
    end

l_user_id = username
access sfsemail, set sfsemail:company_id = agqgeneral:company_id,
                     sfsemail:user_id    = l_user_id, generic

if agqgeneral:company_id = "GGUND" and
   sfsemail:agent_no <> 0 then
    hide(quote_letter_fn)

do check_files
if l_checkbox_1 = 0 or
   l_checkbox_2 = 0 or
   l_checkbox_3 = 0 or
   l_checkbox_4 = 0 or
   l_checkbox_5 = 0 or
   l_checkbox_6 = 0 or
   l_checkbox_7 = 0 then
    disable(submit_fn)

Access Validation
If agqgeneral:app_NO <> G_app_NO Then
    Error 1000 ;

