Screen
        Name = "agtmt601g";
        Type = "multiline" ;
        repeat LINE_ITEMS, 10 times down ;
        Highlight=Underscore ;
        Padcharacter=Space ;
        Screensize="dynamic";
--   Windowtitle="%a     %s Policy Information - Ver. 7.22   %m";
        Domain="agqaddlpip" ;
        iconline = 0 ;


        Global Unsigned Ascii Number G_app_NO[11] ,
                                     G_LINE_OF_BUSINESS[4],
                                     g_vehicle_no[4],
                                     g_build_no[4],
                                     G_STATE[2],
                                     g_enhancement[1] ;


        Global String G_COMPANY_ID[10] ,
                      g_inquiry[1] ,
                      g_code[7] ;

        local tag l_tag[10] ;

        local unsigned ascii number l_sub_code[4],
                                    l_vehicle_no[4],
                                    l_form[2],
                                    l_limit[8],
                                    l_mca104[1],
                                    l_mca201[1],
                                    l_mca104_prior[1],
                                    l_mca201_prior[1],
                                    l_remove_form[1],
                                    L_STATE[2],
                                    l_agent_no[4],
                                    l_liability_code[2] ;

        local string l_code[7],
                     l_code_1[7],
                     l_code_2[7],
                     l_system_id[15],
                     l_username[15],
                     l_server_name[100],
                     l_current_web_page[100],
                     l_web_site_address[150],
                     l_add_endorsement[1],
                     l_Form_edition[25],
                     l_hide_pip[1],
                     l_filler[25],
                     l_um_uim[3],
                     l_record_key[2],
                     L_COMPANY_ID[10] ,
                     l_screen[9],
                     l_description[75] ;

        Access agqaddlpip,
           Set agqaddlpip:app_NO     = G_app_NO ,
               agqaddlpip:vehicle_no = g_vehicle_no ,
               agqaddlpip:code       = g_code ;

        Include "stdkeys7.inc"

Style Definition

        Include "styles.inc"
        Displayonly Field Style {
                                  backgroundcolor="#CCCCCC"
                                }


Functions
        "change" sequence "chg"   tagged change_fn ;
        "chg" change/hidden ;
        "add" sequence "new"  tagged add_fn ;
         "new" continue begin
                        if agqaddlpip:app_no <> g_app_no then
                            function = "addnew|append"
                        else
                            function = "append"
                        end;
        "addnew" add/autoupdate ;
        append ;

        "redisplay" display/noreturn "agtmt601g" ;
        "popup" display l_screen ;
--        forwardtab ;

        Include "stdfunc7m.inc"

Menu Definition

Toolbar Definition

screen entry
disable(accept_fn)
access agqname, set agqname:app_no = g_app_no, generic

if agqname:quote_no <> 0 then
    begin
    disable(change_fn)
    disable(accept_fn)
    end

access sfsemail, set sfsemail:company_id = agqvehicle:company_id,
                     sfsemail:user_id    = username, generic

l_username  = username
l_system_id = "cap"
access sfssecurity_rating,
   set sfssecurity_rating:company_id = agqvehicle:company_id,
       sfssecurity_rating:user_id    = l_username,
       sfssecurity_rating:system_id  = l_system_id, generic

switch(g_line_of_business)
  case 15 : setproperty(t_line_of_business,iconname,"title_com_auto_std.bmp")
  case 16 : setproperty(t_line_of_business,iconname,"title_com_auto_pref.bmp")
  default : setproperty(t_line_of_business,iconname,"")
  end

access sfsdefault, set sfsdefault:sfs_code = "SFS", generic

access sfsprofile,
   set sfsprofile:user_id  = l_username, exact

If SFSPROFILE:USER_ID <> L_USERNAME Then
  Begin
    L_USERNAME = "default"
    Access SFSPROFILE, Set SFSPROFILE:USER_ID = L_USERNAME, Exact
  End

l_server_name = sfsprofile:UNIX_SERVER                            --"http://192.168.1.197/"

