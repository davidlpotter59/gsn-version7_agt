601    agqLOCATION:ADDRESS1[1]/default/tabstop;
701    agqLOCATION:ADDRESS1[2]/default/tabstop
       Hide If agqLOCATION:ADDRESS1[1] = "" ;
801     agqLOCATION:ADDRESS1[3]/default/tabstop
