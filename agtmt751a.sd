procedure definition

procedure change_current_browser (string p_server_name, string p_current_web_page)

BEGIN
  l_web_site_address = trun(p_server_name) + trun(p_current_web_page)
  setproperty(helper_tg,url,trun(l_web_site_address))
END

procedure read_business_desc
begin
access agqcontractor, set agqcontractor:app_no   = g_app_no,
                       agqcontractor:prem_no  = agqlocation:prem_no,
                       agqcontractor:build_no = agqlocation:build_no, generic

l_business_desc_1 = agqcontractor:business_desc[1]
l_business_desc_2 = agqcontractor:business_desc[2]

end

procedure update_business_desc
begin
access agqcontractor, set agqcontractor:app_no   = g_app_no,
                       agqcontractor:prem_no  = agqlocation:prem_no,
                       agqcontractor:build_no = agqlocation:build_no, generic

if agqcontractor:app_no   = g_app_no and
   agqcontractor:prem_no  = agqlocation:prem_no and
   agqcontractor:build_no = agqlocation:build_no then
    begin
    change agqcontractor
        begin
        agqcontractor:business_desc[1] = l_business_desc_1
        agqcontractor:business_desc[2] = l_business_desc_2
        end
    end

end

procedure get_heading(string l_display_heading)

begin
  postevent("agtmt010b","set_info_box",l_display_heading)
end


procedure back_a_screen

begin
  postevent("menutree","REMOVE_SCREEN","Display_Main")
end

procedure get_sfszipcode
begin
l_val_zipcode = agqlocation:zipcode
l_str_zipcode = agqlocation:str_zipcode[1,5] + "0000"
access sfszipcode, set sfszipcode:company_id       = agqcontractor:company_id,
                       sfszipcode:state            = g_state,
                       sfszipcode:line_of_business = agqcontractor:line_of_business,
                       sfszipcode:str_zipcode      = l_str_zipcode, generic

l_tier = 0
while sfszipcode:company_Id       = agqcontractor:company_id and
      sfszipcode:state            = g_state and
      sfszipcode:line_of_business = agqcontractor:line_of_business and
      sfszipcode:str_zipcode      = l_str_zipcode
        begin
        if (agqname:eff_date >= sfszipcode:eff_date and
           (agqname:eff_date <= sfszipcode:exp_date or
           sfszipcode:exp_date = 0)) then
            begin
            l_restricted = sfszipcode:restricted
            l_restricted_distance = sfszipcode:restricted_distance
            end

        next sfszipcode
        end

end

procedure add_blank_agqlocation
BEGIN
access agqcontractor, set agqcontractor:app_no = g_app_no, generic

while agqcontractor:app_no = g_app_no
    {
    access agqlocation_alias, set agqlocation_alias:app_no   = agqcontractor:app_no,
                                  agqlocation_alias:prem_no  = agqcontractor:prem_no,
                                  agqlocation_alias:build_no = agqcontractor:build_no, generic

    if agqlocation_alias:app_no   <> agqcontractor:app_no or
       agqlocation_alias:prem_no  <> agqcontractor:prem_no or
       agqlocation_alias:build_no <> agqcontractor:build_no then
        {
        access agqname, set agqname:app_no = g_app_no, generic

        add agqlocation_alias
            {
            agqlocation_alias:app_no           = agqcontractor:app_no
            agqlocation_alias:prem_no          = agqcontractor:prem_no
            agqlocation_alias:build_no         = agqcontractor:build_no
            agqlocation_alias:LINE_OF_BUSINESS = agqcontractor:LINE_OF_BUSINESS
            agqlocation_alias:STATE            = agqcontractor:STATE
            agqlocation_alias:COUNTY           = agqcontractor:COUNTY
            agqlocation_alias:TRANS_DATE       = agqNAME:TRANS_DATE
            agqlocation_alias:TRANS_CODE       = agqname:trans_code   --changed it from 10
            agqlocation_alias:EFF_DATE         = agqNAME:EFF_DATE
            agqlocation_alias:EXP_DATE         = agqNAME:EXP_DATE
            }
        }

    next agqcontractor
    }

END

procedure update_name
begin
access agqname, set agqname:app_no = g_app_no, generic

if agqname:app_no = g_app_no then
    begin
    change agqname
        begin
        agqname:name[1]     = agqcontractor:business_name[1]
        agqname:address[1]  = agqlocation:address
        agqname:address[2]  = agqlocation:address1[1]
        agqname:address[3]  = agqlocation:address1[2]
        agqname:city        = agqlocation:city
        agqname:str_state   = agqlocation:str_state
        agqname:str_zipcode = agqlocation:str_zipcode
        end
    end
else
    begin
    add agqname
        begin
        agqname:app_no      = agqlocation:app_no
        agqname:name[1]     = agqcontractor:business_name[1]
        agqname:address[1]  = agqlocation:address
        agqname:address[2]  = agqlocation:address1[1]
        agqname:address[3]  = agqlocation:address1[2]
        agqname:city        = agqlocation:city
        agqname:str_state   = agqlocation:str_state
        agqname:str_zipcode = agqlocation:str_zipcode
        end
    end

end


