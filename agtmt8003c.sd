components

Fields
201     agqisocrime:LIMIT[1]
        if agqisocrime:limit[1] > 0 then
            do GET_PER_AMOUNT ;

202     begin
        if function = "ADD" then
            agqisocrime:plan = 1
        end
        agqisocrime:plan/default
        If agqisocrime:plan <> 1 Then
            Error 1262

        Activehelp CPSisoCRplan_alias:plan,
                   CPSisocrplan_alias:DESC ;
203     Begin
        If FUNCTION one of  "ADD", "CHANGE" Then
            agqisocrime:SAFE_CLASS = CPSCRIMEDEFAULTS:SAFE_CLASS
        End
        agqisocrime:SAFE_CLASS/default/uppercase
--        If agqisocrime:SAFE_CLASS <> CPSISOCRSAFES_alias:SAFE_CLASS Then
--            Error 1262

        agqisocrime:SAFE_CLASS_SUB_CODE = CPSISOCRSAFES:SUB_CODE

        Activehelp CPSISOCRSAFES_ALIAS:SAFE_CLASS/width=2/heading="CLASS",
                   cpsisocrsafes_alias:safe_vault/width=2/heading="TYPE",
                   cpsisocrsafes_alias:doors/width=40,
                   CPSISOCRSAFES_ALIAS:walls/width=75,
                   cpsisocrsafes_alias:labels[1]/width=100,
                   if cpsisocrsafes_alias:labels[2] <> "" then
                      cpsisocrsafes_alias:labels[2]
                   else
                      null,
                   if cpsisocrsafes_alias:labels[3] <> "" then
                      cpsisocrsafes_alias:labels[3] ;

501     agqisocrime:money_exposure[1] ;
502     begin
        if function one of "ADD", "CHANGE" then
            begin
            If agqisocrime:SAFE_CLASS <> " " Then
                Do GET_SAFE_CLASS
            Else
                L_FACTOR1 = 1

            agqisocrime:base_factor_inside[1] = l_factor1
            end
        end
        agqisocrime:base_factor_inside[1]/displayonly ;
503     begin
        if function one of "ADD", "CHANGE" then
            agqisocrime:crime_total[1] = (agqisocrime:base_factor_inside[1] *
                                         (agqisocrime:money_exposure[1] divide
                                         l_inside_per))
        end
        agqisocrime:crime_total[1]/displayonly ;
601     agqisocrime:securities_exposure[1] ;
602     begin
        if function one of "ADD", "CHANGE" then
            begin
            If agqisocrime:securities_exposure[1] > 0 Then
                begin
                l_misc_code = "B"
                Do GET_BASE_FACTOR
                end
            Else
                L_FACTOR1 = 1.00
            agqisocrime:base_factor_inside[2] = l_factor1
            end
        end
        agqisocrime:base_factor_inside[2]/displayonly ;
603     begin
        if function one of "ADD", "CHANGE" then
            agqisocrime:crime_total[2] = ((agqisocrime:base_factor_inside[2] * agqisocrime:base_factor_inside[1]) *
                                         (agqisocrime:securities_exposure[1] divide
                                         l_inside_per))
        end
        agqisocrime:crime_total[2]/displayonly ;
701     agqisocrime:checks_exposure[1] ;
702     begin
        if function one of "ADD", "CHANGE" then
            begin
            If agqisocrime:checks_exposure[1] > 0 Then
                begin
                l_misc_code = "C"
                Do GET_BASE_FACTOR
                end
            Else
                L_FACTOR1 = 1.00
            agqisocrime:base_factor_inside[3] = l_factor1
            end
        end
        agqisocrime:base_factor_inside[3]/displayonly ;
703     begin
        if function one of "ADD", "CHANGE" then
            agqisocrime:crime_total[3] = (agqisocrime:base_factor_inside[3] *
                                         (agqisocrime:checks_exposure[1] divide
                                         l_inside_per))
        end
        agqisocrime:crime_total[3]/displayonly ;
801     agqisocrime:payroll_exposure[1] ;
802     begin
        if function one of "ADD", "CHANGE" then
            begin
            If agqisocrime:payroll_exposure[1] > 0 Then
                begin
                l_misc_code = "D"
                Do GET_BASE_FACTOR
                end
            Else
                L_FACTOR1 = 1.00
            agqisocrime:base_factor_inside[4] = l_factor1
            end
        end
        agqisocrime:base_factor_inside[4]/displayonly ;
803     begin
        if function one of "ADD", "CHANGE" then
            agqisocrime:crime_total[4] = (agqisocrime:base_factor_inside[4] *
                                         (agqisocrime:payroll_exposure[1] divide
                                         l_inside_per))
        end
        agqisocrime:crime_total[4]/displayonly ;
901     agqisocrime:bank_exposure[1] ;
902     begin
        if function one of "ADD", "CHANGE" then
            begin
            If agqisocrime:bank_exposure[1] > 0 Then
                begin
                l_misc_code = "E"
                Do GET_BASE_FACTOR
                end
            Else
                L_FACTOR1 = 1.00
            agqisocrime:base_factor_inside[5] = l_factor1
            end
        end
        agqisocrime:base_factor_inside[5]/displayonly ;
903     begin
        if function one of "ADD", "CHANGE" then
            agqisocrime:crime_total[5] = (agqisocrime:base_factor_inside[5] *
                                         (agqisocrime:bank_exposure[1] divide
                                         l_inside_per))
        end
        agqisocrime:crime_total[5]/displayonly ;
1001    begin
        if function one of "ADD", "CHANGE" then
            agqisocrime:excess_exposure[1] = agqisocrime:limit[1] - agqisocrime:money_exposure[1] - agqisocrime:securities_exposure[1] -
                                             agqisocrime:checks_exposure[1] - agqisocrime:payroll_exposure[1] - agqisocrime:bank_exposure[1]
        end
        agqisocrime:excess_exposure[1]/default ;
1002    begin
        if function one of "ADD", "CHANGE" then
            begin
            if agqisocrime:excess_exposure[1] > 0 then
                agqisocrime:base_factor_inside[6] = agqisocrime:base_factor_inside[1]
            else
                agqisocrime:base_factor_inside[6] = 1.00
            end
        end
        agqisocrime:base_factor_inside[6]/displayonly ;
1003    begin
        if function one of "ADD", "CHANGE" then
            agqisocrime:excess_multiplier[1] = (agqisocrime:base_factor_inside[6] * (agqisocrime:excess_exposure[1] divide l_inside_per))
        end
        agqisocrime:excess_multiplier[1]/displayonly/mask="ZZZZ.99" ;
1101    begin
        if function one of "ADD", "CHANGE" then
            agqisocrime:gross_premium[1] = (agqisocrime:crime_total[1] + agqisocrime:crime_total[2] +
                                           agqisocrime:crime_total[3] + agqisocrime:crime_total[4] +
                                           agqisocrime:crime_total[5] + agqisocrime:excess_multiplier[1])
        end
        agqisocrime:gross_premium[1]/displayonly ;

1501    begin
        if function one of "ADD", "CHANGE" then
            do get_totals
        end
        agqisocrime:gross_premium[1]/displayonly ;
1502    begin
            l_deviation = 1.00
        end
        l_deviation/displayonly ;
1503    agqisocrime:territory_factor/displayonly ;
1504    agqisocrime:net_premium[1]/displayonly ;
1505    agqisocrime:deductible_premium[1]/displayonly ;
1506    agqisocrime:adjusted_rate[1]/displayonly ;
1507    agqisocrime:rmf_total[1]/displayonly ;
1508    agqisocrime:total_premium[1]/displayonly ;
1601    agqisocrime:minimum_premium[1]/displayonly ;
1701    agqisocrime:final_premium[1]/displayonly ;

}

