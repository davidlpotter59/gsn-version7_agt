               agqisocrime:crime_form = g_crime_form, generic

        Include "stdkeys7.inc"

Style Definition
        Include "styles.inc"
        Displayonly Field Style {
                                  backgroundcolor="#CCCCCC"
                                }


Functions

        "redisplay" display/noreturn "agtmt8003c" ;

        reaccess ;

        include "stdfunc7.inc"

