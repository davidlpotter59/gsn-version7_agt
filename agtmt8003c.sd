Procedure Definition

procedure get_totals
begin
Do GET_DEDUCTIBLE_FACTOR
Do GET_MINIMUM_PREMIUM

If agqisocrime:code <> " " Then
    Do GET_WATCHPERSON_RATES
Else
    L_WATCHPERSON_FACTOR = 1.00
If agqisocrime:NO_GUARDS_SERVICE <> 0 Then
    Do GET_GUARDS_SERVICE
Else
    L_GUARDSERVICE_FACTOR = 1.00
if agqisocrime:NO_MESSENGERS > 1 then
    DO GET_MESSENGERS
else
    l_multi_messenger_factor = 1.00
If agqisocrime:ALARM_SERVICE_NO <> 0 Then
    Do GET_ALARM_SERVICE
Else
    L_ALARM_FACTOR = 1.00
If agqisocrime:SAFE_VAULT <> 0 Then
    Do GET_SAFE_SERVICE
Else
    L_SAFE_FACTOR = 1.00
If agqisocrime:HOLDUP <> " " Then
    Do GET_HOLDUP_ALARMS
Else
    L_HOLDUP_FACTOR = 1.00
If agqisocrime:BULLET_Code <> " " Then
    Do GET_BULLET_RATES
Else
    L_BULLET_FACTOR = 1.00

If agqisocrime:CONVEYANCE_code <> " " Then
    Do GET_CONVEYANCE_RATES
Else
    L_CONVEYANCE_FACTOR = 1.00

l_limit = agqisocrime:limit[3]
DO LIMIT_EXCESS
IF l_LIMIT > 0 then
    begin
    l_section = "M"
    Do GET_BOND_EXCESS_RATES
    end
Else
    L_Bond_Excess_factor2 = 1.00

-- Decrease limit of business while not open for business
If agqisocrime:business_code = "D" Then
    begin
    l_business_closed_premium = (agqisocrime:base_factor_inside[1] * (agqisocrime:closed_for_business_limit divide l_inside_per))
    Do GET_Business_code_factor
    end
Else
    begin
    L_business_code_FACTOR = 1.00
    end

If agqisocrime:entire_plot = "E" Then
    begin
    Do GET_entire_plot_factor
    end
Else
    begin
    L_entire_plot_FACTOR = 1.00
    end

access agqcrime1, set agqcrime1:app_no   = g_app_no,
                      agqcrime1:prem_no  = g_prem_no,
                      agqcrime1:build_no = g_build_no, generic

l_crime1_terr = agqcrime1:terr

Do GET_TERRITORY_FACTOR
DO GET_RMF_FACTOR

-- Section A Premises coverage [1]
Do APPLY_DISCOUNTS_PREMISES
Do APPLY_AMENDMENTS_PREMISES

agqisocrime:deductible_factor       = l_deductible_factor
agqisocrime:TERRITORY_FACTOR        = L_prem_TERR_FACTOR
agqisocrime:mess_terr_factor        = l_mess_terr_factor
agqisocrime:burglary_terr_factor    = l_burglary_terr_factor
agqisocrime:WATCHPERSON_FACTOR      = L_WATCHPERSON_FACTOR
agqisocrime:GUARD_SERVICE_FACTOR    = L_GUARDSERVICE_FACTOR
agqisocrime:ALARM_SERVICE_FACTOR    = L_ALARM_FACTOR
agqisocrime:SAFE_SERVICE_FACTOR     = L_SAFE_FACTOR
if agqisocrime:business_code ="D" then
    begin
    agqisocrime:each_addl_factor[1] = 1.00
    agqisocrime:each_addl_factor[2] = 1.00
    end
else
    begin
    agqisocrime:each_addl_factor[1] = l_bond_excess_factor1
    agqisocrime:each_addl_factor[2] = l_bond_excess_factor2
    end
agqisocrime:multi_messenger_factor  = L_MULTI_MESSENGER_FACTOR
agqisocrime:business_code_factor    = L_BUSINESS_CODE_FACTOR
agqisocrime:entire_plot_factor      = L_ENTIRE_PLOT_FACTOR
agqisocrime:HOLDUP_ALARMS_FACTOR    = L_HOLDUP_FACTOR
agqisocrime:BULLET_FACTOR           = L_BULLET_FACTOR
agqisocrime:CONVEYANCE_FACTOR       = L_CONVEYANCE_FACTOR
agqisocrime:rmf_total[1]            = l_rmf_factor
agqisocrime:rmf_total[2]            = l_rmf_factor

-- territory multiplier
agqisocrime:net_premium[1]          = (agqisocrime:gross_premium[1] *
                                      l_prem_terr_factor)

-- finding deductible credit
if l_deductible_factor = 0 then
    agqisocrime:deductible_premium[1] = 0
else
    agqisocrime:deductible_premium[1] = ((agqisocrime:gross_premium[1] * l_deductible_factor) divide
                                        (agqisocrime:money_exposure[1] divide l_inside_per))

-- adjusted premium ***************
agqisocrime:ADJUSTED_RATE[1] = agqisocrime:net_premium[1] -
                               agqisocrime:DEDUCTIBLE_Premium[1]

If agqisocrime:ADJUSTED_RATE[1] < L_INSIDE_MINIMUM Then
    begin
    L_INSIDE_MINIMUM_YES = "Y"
    end
else
    l_inside_minimum_Yes = "N"

-- total - inside
l_total_premium = agqisocrime:adjusted_rate[1] *
                  agqisocrime:rmf_total[1]
agqisocrime:total_premium[1] = l_total_premium

-- minimum is calculated without RMF and added after  no discount on minimum premium
if l_inside_minimum_yes = "Y" then
    begin
    l_minimum_total1 = agqisocrime:total_premium[1]
    agqisocrime:minimum_premium[1] = ((l_inside_minimum *
                                     agqisocrime:rmf_total[1]) -
                                     agqisocrime:total_premium[1])
    agqisocrime:final_premium[1] = agqisocrime:minimum_premium[1] +
                                   agqisocrime:total_premium[1]
    end
else
    begin
    agqisocrime:minimum_premium[1] = 0
    agqisocrime:final_premium[1] = agqisocrime:total_premium[1]
    l_inside_minimum = 0
    l_minimum_total1 = 0
    end

-- ******Section B Messenger coverage [2]************
DO APPLY_DISCOUNTS_MESSENGER
-- no amendments messenger

--territory multiplier
agqisocrime:net_premium[2] = (l_ADJUSTED_RATE2 *
                             l_mess_terr_factor)

-- finding deductible credit
if l_deductible_factor = 0 then
    agqisocrime:deductible_premium[2] = 0
else
    agqisocrime:deductible_premium[2] = ((agqisocrime:gross_premium[2] *
                                        l_deductible_factor) divide
                                       (agqisocrime:money_exposure[2] divide
                                       l_outside_per))

-- adjusted premium ***************
agqisocrime:ADJUSTED_RATE[2] = agqisocrime:net_premium[2] -
                               agqisocrime:DEDUCTIBLE_premium[2]
If agqisocrime:ADJUSTED_RATE[2] < L_OUTSIDE_MINIMUM Then
    L_OUTSIDE_MINimum_yes = "Y"
else
    l_outside_minimum_yes = "N"

-- multi messenger
if agqisocrime:no_messengers > 1 then
    begin
    for l_mess = 2 to agqisocrime:no_messengers
        begin
        l_sub = l_mess + 1
        agqisocrime:gross_premium[l_sub] = (agqisocrime:crime_total_1[1] +
                                            agqisocrime:crime_total_1[2] +
                                            agqisocrime:crime_total_1[3] +
                                            agqisocrime:crime_total_1[4])
        DO APPLY_DISCOUNTS_MESSENGER_MULTI
        agqisocrime:net_premium[l_sub] = (l_ADJUSTED_RATE3 *
                                         l_mess_terr_factor)
        agqisocrime:adjusted_rate[l_sub] = agqisocrime:net_premium[l_sub] -
                                           agqisocrime:deductible_premium[2]
        If agqisocrime:ADJUSTED_RATE[l_sub] > L_OUTSIDE_MINIMUM Then
            L_OUTSIDE_MINimum_yes = "Y"
        else
            l_outside_minimum_yes = "N"

        agqisocrime:total_premium[l_sub] = agqisocrime:adjusted_rate[l_sub] * agqisocrime:rmf_total[1]
        agqisocrime:total_premium[l_sub] = agqisocrime:total_premium[l_sub] + l_outside_each_addl
        end
end


-- total - outside
l_total_premium = agqisocrime:adjusted_rate[2] *
                  agqisocrime:rmf_total[2]
agqisocrime:total_premium[2] = l_total_premium

-- minimum is calculated without RMF and added after  no discount on minimum premium
if l_outside_minimum_yes = "Y" then
    begin
    l_minimum_total2 = agqisocrime:total_premium[2]
    agqisocrime:minimum_premium[2] = ((l_outside_minimum *
                                     agqisocrime:rmf_total[2]) -
                                     agqisocrime:total_premium[2])
    agqisocrime:final_premium[2] = agqisocrime:minimum_premium[2] +
                                   agqisocrime:total_premium[2]
    end
else
    begin
    agqisocrime:minimum_premium[2] = 0
    agqisocrime:final_premium[2] = agqisocrime:total_premium[2]
    l_outside_minimum = 0
    l_minimum_total2 = 0
    end

l_premium = agqisocrime:final_premium[1] +
            agqisocrime:final_premium[2]

agqisocrime:crime_premium = l_premium

if agqcrime1:app_no = g_app_no and
   agqcrime1:prem_no = g_prem_no and
   agqcrime1:build_no = g_build_no and
   agqcrime1:sub_code = g_sub_code then
    begin
    change agqcrime1
        begin
        agqcrime1:total_premium = agqisocrime:crime_premium
        agqcrime1:premium = agqcrime1:total_premium
        end
    end

End

Procedure ACCESS_agqEND_DELETE
Begin
