%%                      
Screen
        Name = "agtmt9999g";
        Type = "Detail" ;
        Highlight=Underscore ;
        highlightfield="Yes";
        Padcharacter=Space ;
        Screensize="dynamic";
        Windowtitle="%a     %s Rating Information - Ver. 7.00     %m";
        Domain="agqcontact" ;
        resizeable = yes ;
        iconline = 0 ;

        Global Unsigned Ascii Number G_APP_NO[11] ;
        global unsigned ascii number g_prem_no[4] ;
        global unsigned ascii number g_build_no[4];
        global unsigned ascii number g_line_of_business[4];

        local string l_first_str_location[8],
                     l_last_str_location[8],
                     l_str_location[8],
                     l_first_location[1]="Y",
                     l_first[1],
                     l_add_endorsement[1],
                     l_form_edition[25],
                     l_description[75],
                     l_code[7],
                     l_code_1[7],
                     l_screen[10],
                     l_server_name[100] ,
                     l_screen_name[9],
                     l_username[15] ,
                     l_company_id[10] ;

        Local Unsigned Ascii Number l_first_prem_no[4],
                                    l_first_build_no[4],
                                    l_last_prem_no[4],
                                    l_last_build_no[4],
                                    l_no_locations[2]=0,
                                    l_field_no[4] ,
                                    l_sub_code[4],
                                    l_state[2];

        Access agqcontact,
           Set agqcontact:app_no = G_app_no, generic

        Include "stdkeys7.inc"

Style Definition
        Include "styles.inc"
        Displayonly Field Style {
                                  backgroundcolor="#CCCCCC"
                                }


Functions

        "redisplay" display/noreturn "agtmt9999g" ;

        "change" sequence "chg"   tagged change_fn ;
        "chg" change/hidden ;

        "displayit" sequence "show"  tagged nextscreen_fn;
        "show" continue begin
                        access agqamt, set agqamt:app_no = g_app_no, generic

                        if agqamt:lob_totals[1] <> 0 then
                            l_screen = "agtmt9999h"
                        else
                            l_screen = "agtmt9999j"
                        function = "popup"
                        end ;
        "popup" display/noreturn l_screen ;
        "displayit1"  display "agtmt9999f" tagged back_fn;

        reaccess ;

        include "stdfunc7.inc"

EVENT DEFINITION
        default eventhandler {
                             "REMOVE_SCREEN" { function = "exit" abort () }
                             "reload" { function = "reaccess" abort () }
                             "display_message" { warning "" + event.value }
                             "current_line_of_business" {
                                                          g_line_of_business = val(event.value)
                                                        }
                             }
Menu Definition

