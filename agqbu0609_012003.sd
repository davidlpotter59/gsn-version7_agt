Access agqendorse,
   Set agqendorse:app_no    = l_app_no , generic

Access agqgeneral,
   Set agqgeneral:app_no    = l_app_no , generic

Access agqaddin,
   Set agqaddin:app_no    = l_app_no , generic

Include "swsut100.inc"
I_CODE = "BU0609"
Include "swsut200.inc"

While agqaddin:app_no    = l_app_no
{
