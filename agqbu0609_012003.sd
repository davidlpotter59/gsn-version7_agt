      Writefile(L_FILE_NAME,"%s","following hazards for which the state or political")
      Writefile(L_FILE_NAME,"%s","<NL>")

      Writefile(L_FILE_NAME,"%s",L_ARIAL_9)
      Writefile(L_FILE_NAME,"%s","<033>&a1150h+0V")
