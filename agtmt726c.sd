procedure definition

procedure read_agqend
begin
access agqend, set agqend:app_no = g_app_no, generic

l_scp102 = "N"
l_scp103 = "N"
while agqend:app_no = g_app_no
    begin
    if agqend:code = "SCP102" then
        begin
        l_scp102 = "Y"
        end
    if agqend:code = "SCP103" then
        begin
        l_scp103 = "Y"
        end

   next agqend
   end

end

