procedure change_current_browser (string p_server_name, string p_current_web_page)
BEGIN
l_web_site_address = trun(p_server_name) + trun(p_current_web_page)
setproperty(helper_tg,url,trun(l_web_site_address))

END

procedure check_warning(number l_sub_code_1)
begin
if l_fuel_oil_warning[l_sub_code_1] = 1 then
    l_continue_1 = dialog("WARNING","warning.bmp","Reminder, you cannot bind coverage for an underground oil tank or an abandoned underground oil tank.","OK")

end

procedure check_popup(number l_sub_code_1)
begin
l_popup_complete = 0
access agqapplication, set agqapplication:app_no   = g_app_no,
                           agqapplication:sub_code = l_sub_code_1, generic

if g_line_of_business one of 4 then
    begin
    if agqapplication:sub_code one of 14, 15, 16, 17, 18, 21 and
       agqapplication:remarks <> "" then
        begin
        l_popup_complete = 1
        end

    if agqapplication:sub_code one of 19 and
       agqapplication:no_employees[1] <> 0 or
       agqapplication:no_employees[2] <> 0 then
        l_popup_complete = 1

    if (agqapplication:sub_code = 20 and
       (agqapplication:ul_approved <> 0 or
       agqapplication:employers_name <> "" or
       agqapplication:permit_obtained <> 0)) then
        begin
        l_popup_complete = 1
        end

    if agqapplication:sub_code one of 24, 25, 26 and
       agqapplication:year_replaced_updated <> 0 then
        begin
        l_popup_complete = 1
        end
    end
else
if g_line_of_business one of 34, 44 then
    begin
    if agqapplication:sub_code one of 14, 15, 16, 18, 19 and
       agqapplication:remarks <> "" then
        begin
        l_popup_complete = 1
        end

    if agqapplication:sub_code one of 17 and
       agqapplication:no_employees[1] <> 0 or
       agqapplication:no_employees[2] <> 0 then
        l_popup_complete = 1

    if (agqapplication:sub_code = 20 and
       (agqapplication:fuel_tank[1] <> 0 or
       agqapplication:fuel_tank[2] <> 0 or
       agqapplication:fuel_tank[3] <> 0)) then
        begin
        l_popup_complete = 1
        end

    if agqapplication:sub_code one of 21, 22, 23, 24 and
       agqapplication:year_replaced_updated <> 0 then
        begin
        l_popup_complete = 1
        end

    if (agqapplication:sub_code one of 26 and
       (agqapplication:completion_date <> 0 or
       agqapplication:renovation_value <> 0)) then
        l_popup_complete = 1
    end
else
if g_line_of_business one of 31 then
    begin
    if agqapplication:sub_code one of 14, 15, 16, 17, 18 and
       agqapplication:remarks <> "" then
        begin
        l_popup_complete = 1
        end

    if (agqapplication:sub_code one of 19 and
       (agqapplication:no_employees[1] <> 0 or
       agqapplication:no_employees[2] <> 0)) then
        l_popup_complete = 1

    if (agqapplication:sub_code = 20 and
       (agqapplication:ul_approved <> 0 or
       agqapplication:employers_name <> "" or
       agqapplication:permit_obtained <> 0)) then
        begin
        l_popup_complete = 1
        end

    if agqapplication:sub_code one of 22, 23, 24, 25 and
       agqapplication:year_replaced_updated <> 0 then
        begin
        l_popup_complete = 1
        end
    end
else
if g_line_of_business one of 1 then
    begin
    if agqapplication:sub_code one of 16, 17, 18, 19, 20, 23 and
       agqapplication:remarks <> "" then
        begin
        l_popup_complete = 1
        end

    if (agqapplication:sub_code one of 21 and
       (agqapplication:no_employees[1] <> 0 or
       agqapplication:no_employees[2] <> 0)) then
        l_popup_complete = 1

    if (agqapplication:sub_code = 22 and
       (agqapplication:ul_approved <> 0 or
       agqapplication:employers_name <> "" or
       agqapplication:permit_obtained <> 0)) then
        begin
        l_popup_complete = 1
        end

    if agqapplication:sub_code one of 26 and
       agqapplication:year_replaced_updated <> 0 then
        begin
        l_popup_complete = 1
        end
    end

end

