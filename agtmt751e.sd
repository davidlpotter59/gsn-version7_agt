procedure clear_popup_information
begin
access agqapplication_alias, set agqapplication_alias:app_no = g_app_no, generic

while agqapplication_alias:app_no = g_app_no
    begin
    change agqapplication_alias
        begin
        if agqapplication_alias:screen_name = "agtmt510" and
           agqapplication_alias:answer <> "Y" then
            begin
            agqapplication_alias:hours = ""
            end

        if agqapplication_alias:screen_name = "agtmt511" and
           agqapplication_alias:answer <> "Y" then
            begin
            agqapplication_alias:total_number[1] = 0
            end

        if agqapplication_alias:screen_name one of "agtmt503" and
           agqapplication_alias:answer <> "Y" then
            begin
            agqapplication_alias:remarks = ""
            end

        if agqapplication_alias:screen_name = "agtmt520" and
           agqapplication_alias:answer <> "Y" then
            begin
            agqapplication_alias:fob[1]  = 0
            agqapplication_alias:fob[2]  = 0
            agqapplication_alias:fob[3]  = 0
            agqapplication_alias:fob[4]  = 0
            agqapplication_alias:fob[5]  = 0
            agqapplication_alias:fob[6]  = 0
            agqapplication_alias:remarks = ""
            end

        if agqapplication_alias:screen_name = "agtmt521" and
           agqapplication_alias:answer <> "Y" then
            begin
            agqapplication_alias:total_number[1] = 0
            agqapplication_alias:total_number[2] = 0
            end

        if agqapplication_alias:screen_name one of "agtmt503" and
           agqapplication_alias:answer <> "Y" then
            begin
            agqapplication_alias:remarks = ""
            end

        if agqapplication_alias:screen_name one of "agtmt512" and
           agqapplication_alias:answer <> "Y" then
            begin
            agqapplication_alias:fuel_tank[1] = 0
            agqapplication_alias:fuel_tank[2] = 0
            agqapplication_alias:fuel_tank[3] = 0
            end

        if agqapplication_alias:screen_name one of "agtmt513", "agtmt514", "agtmt515", "agtmt516" and
           agqapplication_alias:answer <> "Y" then
            begin
            agqapplication_alias:year_replaced_updated = 0
            end

        if agqapplication_alias:screen_name one of "agtmt517" and
           agqapplication_alias:answer <> "Y" then
            begin
            agqapplication_alias:completion_date  = 0
            agqapplication_alias:renovation_value = 0
            end

        if agqapplication_alias:screen_name one of "agtmt535" and
           agqapplication_alias:answer <> "Y" then
            begin
            agqapplication_alias:money_overnight = 0
            agqapplication_alias:safe            = 0
            agqapplication_alias:burglar_alarm   = 0
            agqapplication_alias:remarks         = ""
            end

        end

    next agqapplication_alias
    end

end

procedure access_agsapplication
begin
l_sub = 1
while l_sub < 27
    begin
--    l_literal_1[l_sub] = ""
    l_sub_code_1 = 0
    l_sub = l_sub + 1
    end

access agsapplication, set agsapplication:company_id         = agqcontractor:company_id,
                           agsapplication:line_of_business   = agqcontractor:line_of_business, generic


while agsapplication:company_id         = agqcontractor:company_id and
      agsapplication:line_of_business   = agqcontractor:line_of_business
        begin
        if (agqcontractor:eff_date >= agsapplication:reff_date and
           (agqcontractor:eff_date <= agsapplication:rexp_date or
           agsapplication:rexp_date = 0)) then
            begin
            if agsapplication:sub_code >= 50 and
               agsapplication:sub_code <= 62 and
               agsapplication:line_of_business one of 6, 36 then
                begin
                l_sub_code_1                  = agsapplication:sub_code
                l_screen_name[l_sub_code_1]   = agsapplication:screen_name[1]
--                l_screen_name_2[l_sub_code_1] = agsapplication:screen_name[2]
                l_literal[l_sub_code_1]       = trun(agsapplication:literal)
                if agsapplication:sub_code = 50 then
                    begin
                    l_screen_name_50 = agsapplication:screen_name[1]
                    l_literal_50     = agsapplication:literal
                    end
                if agsapplication:sub_code = 51 then
                    begin
                    l_screen_name_51 = agsapplication:screen_name[1]
                    l_literal_51     = agsapplication:literal
                    end
                if agsapplication:sub_code = 52 then
