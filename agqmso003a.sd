Access agqautoquestions, Set agqautoquestions:app_no = l_app_no, approximate

l_auto_sub = 3
if agqautoquestions:app_no = l_app_no then
    begin
    while l_auto_sub < 11
        begin
        if agqautoquestions:question[l_auto_sub] <> "" then
            begin
            If l_max_lines - l_line_counter <= 3 then
                begin
                if agqvehicle:company_id <> "FARMERS" then
                    do fraud_warning
                else
                    Writefile(L_FILE_NAME,"%s%s%s%s%n",
                             L_ARIAL_BOLD_9,
                             "<033>&a5000h7250V",
                             l_arial,
                             "Page ",
                             L_PAGE_NUMBER)
                l_page_number = l_page_number + 1
                Writefile(L_FILE_NAME,"%s","<FF>")
                l_line_counter = 1
                l_next_pos = l_line_counter * 120
                l_remarks_pos = l_next_pos
                do draw_columns
                end

            writefile(l_file_name,"%s","<NL>")
            Writefile(L_FILE_NAME,"%s",l_push)
            l_max_lines        = 70
            L_FONT             = l_arial_9
            L_V_POSITION       = "+0"
            L_H_POSITION       = "100"
            L_CHARACTER_STRING = agqautoquestions:question[l_auto_sub]
            L_MAX_CHAR         = 70
            include "line_spacing.inc"
            l_literal_pos = l_line_counter * 120
            l_literal_counter = l_literal_counter + 1
            Writefile(L_FILE_NAME,"%s",l_pop)

            if l_auto_sub = 3 then
                begin
                Writefile(L_FILE_NAME,"%s",l_arial_bold_9)
                switch(agqautoquestions:run_mvrs)
                  case "N" : l_answer = "NO"
                  default  : l_answer = "YES"
                  end
                if l_answer = "YES" then
                    begin
                    Writefile(L_FILE_NAME,"%s%n%s","<033>&a3285h",l_next_pos,"V")
                    Writefile(L_FILE_NAME,"%s",l_answer)
                    end
                else
                    begin
                    Writefile(L_FILE_NAME,"%s%n%s","<033>&a3310h",l_next_pos,"V")
                    Writefile(L_FILE_NAME,"%s",l_answer)
                    end
                do set_position
                end
            end

        l_auto_sub = l_auto_sub + 1
        end

    end

if agqvehicle:company_id <> "FARMERS" then
    do fraud_warning
