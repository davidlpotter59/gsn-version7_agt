Procedure Definition

Procedure fraud_warning
begin
l_line_counter = 1
Access SCIPSFRAUD, Set SCIPSFRAUD:COMPANY_ID = L_COMPANY_ID, approximate

Writefile(L_FILE_NAME,"%s",l_arial_bold_9)
Writefile(L_FILE_NAME,"%s","<033>&a200h6700V")
Writefile(L_FILE_NAME,"%s","FRAUD WARNING")
Writefile(L_FILE_NAME,"%s",l_arial_bold_7)
Writefile(L_FILE_NAME,"%s","<033>&a3200h+0V")
Writefile(L_FILE_NAME,"%s","APPLICANT'S SIGNATURE")
Writefile(L_FILE_NAME,"%s",l_arial_8)
Writefile(L_FILE_NAME,"%s","<033>&a200h+120V")

l_max_lines        = 60
L_FONT             = l_arial_8
L_V_POSITION       = "+0"
L_H_POSITION       = "200"
L_CHARACTER_STRING = trun(SCIPSFRAUD:FRAUD_WARNING)
L_MAX_CHAR         = 75
include "line_spacing.inc"

Writefile(L_FILE_NAME,"%s","<033>&a3200h6950V<033>*c2037h5v0P")
end

Procedure questions
begin
l_line_counter = 9

Access agqAPPLICATION, Set agqAPPLICATION:app_no = l_app_no, approximate

While agqAPPLICATION:app_no = l_app_no
    begin
    If l_max_lines - l_line_counter <= 3 then
        begin
        if agqgeneral:company_id <> "FARMERS" then
            do fraud_warning
        else
            Writefile(L_FILE_NAME,"%s%s%s%s%n",
                      L_ARIAL_BOLD_9,
                      "<033>&a5000h7250V",
                      l_arial,
                      "Page ",
                      L_PAGE_NUMBER)
        l_page_number = l_page_number + 1
        Writefile(L_FILE_NAME,"%s","<FF>")
        l_line_counter = 1
        l_next_pos = l_line_counter * 120
        l_remarks_pos = l_next_pos
        do draw_columns
        end

    If SFSAPPCONTROL:hide_more_applicant_info = 0 then
        {
        Writefile(L_FILE_NAME,"%s",l_arial_bold_9)
        Writefile(L_FILE_NAME,"%s%n%s","<033>&a3285h",l_next_pos,"V")
        }

    If agqAPPLICATION:ANSWER = "N" and
       agqapplication:dont_print_with_app_questions = 0 then
        {
        Writefile(L_FILE_NAME,"%s",l_arial_bold_9)
        Writefile(L_FILE_NAME,"%s%n%s","<033>&a3310h",l_next_pos,"V")
        Writefile(L_FILE_NAME,"%s","NO")
        }

    If agqAPPLICATION:ANSWER = "Y" and
       agqapplication:dont_print_with_app_questions = 0 then
        {
        Writefile(L_FILE_NAME,"%s",l_arial_bold_9)
        Writefile(L_FILE_NAME,"%s%n%s","<033>&a3285h",l_next_pos,"V")
        Writefile(L_FILE_NAME,"%s","YES")
        }

    if (agqapplication:dont_print_with_app_questions = 0 or
       (agqapplication:dont_print_with_app_questions = 1 and
       sfsappcontrol:hide_more_applicant_info = 0)) then
        {
        if ((l_pp_type = "Y" and
           agqapplication:sub_code one of 14) or
           agqapplication:sub_code not one of 14) then
            begin
            Writefile(L_FILE_NAME,"%s",l_push)
            l_max_lines        = 70
            L_FONT             = l_arial_9
            L_V_POSITION       = "+0"
            L_H_POSITION       = "100"
            L_CHARACTER_STRING = agqAPPLICATION:LITERAL
            L_MAX_CHAR         = 70
            include "line_spacing.inc"
            l_literal_pos = l_line_counter * 120
            l_literal_counter = l_literal_counter + 1
            Writefile(L_FILE_NAME,"%s",l_pop)
            end
        }

    if uppercase(trun(agqapplication:screen_name)) one of "AGTMT584" then
        begin
        l_payment_plan = agqapplication:payment_plan
        access arspayplan, set arspayplan:company_id       = agqvehicle:company_id,
                               arspayplan:line_of_business = agqvehicle:line_of_business,
                               arspayplan:payment_plan     = l_payment_plan, generic
        end

    If uppercase(trun(agqAPPLICATION:SCREEN_NAME)) = "PHONE" then
        {
        If agqAPPLICATION:phone_number <> "" then
            {
            Writefile(L_FILE_NAME,"%s",l_arial_bold_9)
            Writefile(L_FILE_NAME,"%s%n%s","<033>&a3600h",l_next_pos,"V")
            Writefile(L_FILE_NAME,"%s",str(agqapplication:phone_number,"(XXX) XXX-XXXX"))
            do set_position
            }
        else
            {
            Writefile(L_FILE_NAME,"%s",l_arial_bold_9)
            Writefile(L_FILE_NAME,"%s%n%s","<033>&a3600h",l_next_pos,"V")
            Writefile(L_FILE_NAME,"%s",str(agqapplication:contact_phone_no,"(XXX) XXX-XXXX"))
            do set_position
            }
        }

