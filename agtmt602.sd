l_no_trucks   = 0
l_no_vehicles = 0
l_no_pp       = 0
while agqvehicle_alias:app_no = g_app_no
    begin
    if agqvehicle_alias:type = 1 and
       agqvehicle_alias:CLASS_CODE not one of 681, 682, 683, 684, 691, 692, 693, 694 Then
