procedure check_for_warnings_bop
begin
l_wording_type = 1
access agsupdatewording, set agsupdatewording:company_id       = agqgeneral:company_id,
                             agsupdatewording:state            = agqgeneral:state,
                             agsupdatewording:line_of_business = agqgeneral:line_of_business,
                             agsupdatewording:wording_type     = l_wording_type, generic

l_comments_1 = trun(agsupdatewording:comments)

l_wording_type = 2
access agsupdatewording, set agsupdatewording:company_id       = agqgeneral:company_id,
                             agsupdatewording:state            = agqgeneral:state,
                             agsupdatewording:line_of_business = agqgeneral:line_of_business,
                             agsupdatewording:wording_type     = l_wording_type, generic

l_comments_2 = trun(agsupdatewording:comments)

access agqapplication, set agqapplication:app_no = l_app_no, generic

l_first = "Y"
while agqapplication:app_no = l_app_no
        begin
        if agqapplication:highlight_if_yes = 1 and
           agqapplication:answer = "Y" then
            begin
            if l_first = "Y" then
                begin
                l_first = "N"
                l_comments = trun(l_comments_2) +
                             "<NL>" +
                             "<NL>" +
                             trun(agqapplication:literal)
                end
            else
                begin
                l_comments = trun(l_comments) +
                             "<NL>" +
                             trun(agqapplication:literal)
                end
            end

        next agqapplication
        end

if l_comments = "" then
    l_comments = l_comments_1

Writefile(L_FILE_NAME,"%s",l_arial_bold_9)
Writefile(L_FILE_NAME,"%s","<033>&a200h+120V")
l_line_counter = l_line_counter + 1
do set_position

writefile(l_file_name,"%s","Comments")
Writefile(L_FILE_NAME,"%s","<033>&a200h+120V")
        l_max_lines        = 60
        L_FONT             = l_arial_9
        L_V_POSITION       = "+0"
        L_H_POSITION       = "300"
        L_CHARACTER_STRING = l_comments
        L_MAX_CHAR         = 120
        include "line_spacing.inc"
        Writefile(L_FILE_NAME,"%s","<033>&a300h+120V")
        l_line_counter = l_line_counter + 2
        Writefile(L_FILE_NAME,"%s","<033>&a300h+0V")
        l_line_counter = l_line_counter - 1
do set_position
--l_line_counter = l_line_counter + 1
--Writefile(L_FILE_NAME,"%s",l_comments)
end

