Procedure Definition

procedure add_endorsements
begin
l_code        = l_policywide_code[l_sub]
l_vehicle_no  = agqvehicle:vehicle_no
do access_agqend1
if l_add_endorsement = "Y" then
    do add_agqend1
end

procedure delete_endorsements
begin
l_code       = l_policywide_code[l_sub]
l_vehicle_no = agqvehicle:vehicle_no

do access_agqend1
if l_add_endorsement = "N" then
    begin
    do delete_agqend1
    do delete_popup_files
    end

end

procedure delete_popup_files
begin
l_code = l_policywide_code[l_sub]
access agqendorse2, set agqendorse2:app_no      = g_app_no,
                        agqendorse2:vehicle_no  = l_vehicle_no,
                        agqendorse2:code        = l_code,  generic

while agqendorse2:app_no      = g_app_no and
      agqendorse2:vehicle_no  = l_vehicle_no and
      agqendorse2:code        = l_code
        begin
        delete agqendorse2

        next agqendorse2
        end

if l_code one of "CA9937" then
    begin
    access agqgaragekeepers1, set agqgaragekeepers1:app_no   = g_app_no,
                                  agqgaragekeepers1:vehicle_no  = l_vehicle_no, generic

    while agqgaragekeepers1:app_no      = g_app_no and
          agqgaragekeepers1:vehicle_no  = l_vehicle_no
        begin
        delete agqgaragekeepers1

        next agqgaragekeepers1
        end
    end

end

procedure check_popup
begin
l_popup_complete = 0
l_code = l_policywide_code[l_sub]
if l_location_specific[l_sub] = 0 then
    begin
    l_vehicle_no  = 0
    g_vehicle_no  = 0
    end
else
    begin
    l_vehicle_no  = agqvehicle:vehicle_no
    g_vehicle_no  = agqvehicle:vehicle_no
    end

if l_code one of "CA9937" then
    begin
    access agqgaragekeepers1, set agqgaragekeepers1:app_no      = g_app_no,
                                  agqgaragekeepers1:vehicle_no  = l_vehicle_no, generic

    if agqgaragekeepers1:app_no     = g_app_no and
       agqgaragekeepers1:vehicle_no = l_vehicle_no then
        begin
        if (agqgaragekeepers1:limit <> 0 and
           agqgaragekeepers1:class_code <> 0 and
           (agqgaragekeepers1:specified_perils_deductible <> 0 or
           agqgaragekeepers1:comp_deductible <> 0 or
           agqgaragekeepers1:coll_deductible <> 0)) then
            begin
            l_popup_complete = 1
            end
        end
    end

if l_code one of "CA0302" then
    begin
    access agqendorse2, set agqendorse2:app_no      = g_app_no,
                            agqendorse2:vehicle_no  = l_vehicle_no,
                            agqendorse2:code        = l_code, generic

    if agqendorse2:app_no     = g_app_no and
       agqendorse2:vehicle_no = l_vehicle_no then
        begin
        if agqendorse2:deductible[1] <> 0 or
           agqendorse2:deductible[4] <> 0 then
            begin
            l_popup_complete = 1
            end
        end
    end

if l_code one of "CA0444", "CA2303", "CA9940", "CA9947", "LMCDE01" then
    begin
    access agqendorse2, set agqendorse2:app_no      = g_app_no,
                            agqendorse2:vehicle_no  = l_vehicle_no,
                            agqendorse2:code        = l_code, generic

    if agqendorse2:app_no     = g_app_no and
       agqendorse2:vehicle_no = l_vehicle_no then
        begin
        if agqendorse2:description <> "" then
            begin
            l_popup_complete = 1
            end
        end
    end

if l_code one of "CA9928" then
    begin
    access agqendorse2, set agqendorse2:app_no      = g_app_no,
                            agqendorse2:vehicle_no  = l_vehicle_no,
                            agqendorse2:code        = l_code, generic

    if agqendorse2:app_no     = g_app_no and
       agqendorse2:vehicle_no = l_vehicle_no then
        begin
        if agqendorse2:premium <> 0 then
            begin
            l_popup_complete = 1
            end
        end
    end

if l_code one of "CA2048" then
    begin
    access agqaddlinsured, set agqaddlinsured:app_no      = g_app_no,
                               agqaddlinsured:vehicle_no  = l_vehicle_no,
                               agqaddlinsured:code        = l_code, generic

    if agqaddlinsured:app_no     = g_app_no and
       agqaddlinsured:vehicle_no = l_vehicle_no then
        begin
        if agqaddlinsured:name[1] <> "" and
           agqaddlinsured:address[1] <> "" and
           agqaddlinsured:city <> "" and
           agqaddlinsured:str_state <> "" and
           agqaddlinsured:str_zipcode <> "" then
            begin
            l_popup_complete = 1
            end
        end
    end

end

procedure update_endorsements(number l_sub)
begin
if l_value[l_sub] = 1 then
    begin
    if l_location_specific[l_sub] = 0 then
        begin
        l_vehicle_no  = 0
        g_vehicle_no  = 0
        end
    else
        begin
        l_vehicle_no  = agqvehicle:vehicle_no
        g_vehicle_no  = agqvehicle:vehicle_no
        end
    l_code = l_policywide_code[l_sub]
    g_code = l_policywide_code[l_sub]
    do add_endorsements
    if l_popup_screen[l_sub] <> "" then
        begin
        if l_location_specific[l_sub] = 0 then
            begin
            l_vehicle_no  = 0
            g_vehicle_no  = 0
            end
        else
            begin
            l_vehicle_no  = agqvehicle:vehicle_no
            g_vehicle_no  = agqvehicle:vehicle_no
            end
        l_screen = trun(l_popup_screen[l_sub])
        display modal screen l_screen
        end
    end

if l_value[l_sub] = 0 then
    begin
    do remove_blue(l_the_tag[l_sub],"black",l_value[l_sub],l_popup_screen[l_sub])
    do remove_red(l_the_tag[l_sub],"black",l_value[l_sub],l_popup_screen[l_sub])
    if l_location_specific[l_sub] = 0 then
        begin
        l_vehicle_no  = 0
        end
    else
        begin
        l_vehicle_no  = agqvehicle:vehicle_no
        end
    l_code = l_policywide_code[l_sub]
    do delete_endorsements
    end

end

procedure check_for_Red
begin
l_sub = 1
while l_sub < 30
    begin
    if l_value[l_sub] = 1 then
        begin
        l_code = l_policywide_code[l_sub]
        if l_location_specific[l_sub] = 0 then
            begin
            l_vehicle_no  = 0
            g_vehicle_no  = 0
            end
        else
            begin
            l_vehicle_no  = agqvehicle:vehicle_no
            g_vehicle_no  = agqvehicle:vehicle_no
            end
        do check_popup
        if l_popup_complete = 1 then
            do remove_red(l_the_tag[l_sub],"black",l_value[l_sub],l_popup_screen[l_sub])
        end

    l_sub = l_sub + 1
    end

end


