update
if updateok = "NO" then
    begin
    enable(calc_fn)
    disable(accept_fn)
    end

if abort = "YES" then
    begin
    enable(calc_fn)
    disable(accept_fn)
    end

If UPDATEOK = "YES" And
   Abort    = "NO" Then
    Begin
    disable(accept_fn)
    enable(calc_fn)

    access agqnonowned, set agqnonowned:app_no = g_app_no, generic

    if agqnonowned:app_no = g_app_no then
        begin
        change agqnonowned
            begin
            agqnonowned:state        = agqvehicle:state
            agqnonowned:liability    = agqmaster:liability
            switch(agqmaster:if_any)
              case 1  : agqnonowned:if_any = "Y"
              default : agqnonowned:if_any = "N"
              end
            agqnonowned:cost_of_hire = agqmaster:cost_of_hire
            if agqmaster:hired_comp <> 0 then
                begin
                agqnonowned:comp            = "Y"
                agqnonowned:comp_deductible = agqmaster:hired_comp
                end
            else
                begin
                agqnonowned:comp            = "N"
                agqnonowned:comp_deductible = 0
                end
            if agqmaster:hired_coll <> 0 then
                begin
                agqnonowned:coll            = "Y"
                agqnonowned:coll_deductible = agqmaster:hired_coll
                end
            else
                begin
                agqnonowned:coll            = "N"
                agqnonowned:coll_deductible = 0
                end
            switch(agqmaster:nonowned_auto)
              case 1  : agqnonowned:nonowned_auto_applicable = "Y"
              default : agqnonowned:nonowned_auto_applicable = "N"
              end
            agqnonowned:no_employees             = agqmaster:no_employees
            end
        end
    else
        begin
        add agqnonowned
            begin
            agqnonowned:app_no       = g_app_no
            agqnonowned:state        = agqvehicle:state
            agqnonowned:liability    = agqmaster:liability
            switch(agqmaster:if_any)
              case 1  : agqnonowned:if_any = "Y"
              default : agqnonowned:if_any = "N"
              end
            agqnonowned:cost_of_hire = agqmaster:cost_of_hire
            if agqmaster:hired_comp <> 0 then
                begin
                agqnonowned:comp            = "Y"
                agqnonowned:comp_deductible = agqmaster:hired_comp
                end
            else
                begin
                agqnonowned:comp            = "N"
                agqnonowned:comp_deductible = 0
                end
            if agqmaster:hired_coll <> 0 then
                begin
                agqnonowned:coll            = "Y"
                agqnonowned:coll_deductible = agqmaster:hired_coll
                end
            else
                begin
                agqnonowned:coll            = "N"
                agqnonowned:coll_deductible = 0
                end
            switch(agqmaster:nonowned_auto)
              case 1  : agqnonowned:nonowned_auto_applicable = "Y"
              default : agqnonowned:nonowned_auto_applicable = "N"
              end
            agqnonowned:no_employees             = agqmaster:no_employees
            end
        end

    L_RECORD_KEY = "NS"
    l_limit      = agqmaster:um_uim_limit[1]
    l_um_uim     = "UM"
    Access casnonstacked, Set casnonstacked:COMPANY_ID       = agqvehicle:COMPANY_ID,
                              casnonstacked:STATE            = agqvehicle:state,
                              casnonstacked:LINE_OF_BUSINESS = agqvehicle:line_of_business,
                              casnonstacked:um_uim           = l_um_uim,
                              casnonstacked:limit            = l_limit, Generic

    while casnonstacked:RECORD_KEY       = L_RECORD_KEY and
          casnonstacked:COMPANY_ID       = agqvehicle:COMPANY_ID and
          casnonstacked:STATE            = agqvehicle:state and
          casnonstacked:LINE_OF_BUSINESS = agqvehicle:line_of_business and
          casnonstacked:um_uim           = l_um_uim and
          casnonstacked:limit            = l_limit
        begin
        If agqvehicle:EFF_DATE >= casnonstacked:EFF_DATE Then
            Begin
            l_code_1 = casnonstacked:code
            End

        Next casnonstacked
        End

    if l_code_1 <> "" then
        begin
        L_CODE = L_CODE_1
        Do ACCESS_AGQEND1
        If L_ADD_ENDORSEMENT = "Y" And
           agqmaster:um_uim_limit[1] <> 0 and
           agqmaster:non_stacked = "N" Then
            Do ADD_AGQEND1
        If ((L_ADD_ENDORSEMENT = "N" And
           agqmaster:um_uim_limit[1] = 0) or
           (l_add_endorsement = "N" and
           agqmaster:non_stacked <> "N")) Then
            Do DELETE_AGQEND1
        end

    L_RECORD_KEY = "NS"
    l_limit      = agqmaster:um_uim_limit[2]
    l_um_uim     = "UIM"
    Access casnonstacked, Set casnonstacked:COMPANY_ID       = agqvehicle:COMPANY_ID,
                              casnonstacked:STATE            = agqvehicle:state,
                              casnonstacked:LINE_OF_BUSINESS = agqvehicle:line_of_business,
                              casnonstacked:um_uim           = l_um_uim,
                              casnonstacked:limit            = l_limit, Generic

    while casnonstacked:RECORD_KEY       = L_RECORD_KEY and
          casnonstacked:COMPANY_ID       = agqvehicle:COMPANY_ID and
          casnonstacked:STATE            = agqvehicle:state and
          casnonstacked:LINE_OF_BUSINESS = agqvehicle:line_of_business and
          casnonstacked:um_uim           = l_um_uim and
          casnonstacked:limit            = l_limit
        begin
        If agqvehicle:EFF_DATE >= casnonstacked:EFF_DATE Then
            Begin
            l_code_1 = casnonstacked:code
            End

        Next casnonstacked
        End

    if l_code_1 <> "" then
        begin
        L_CODE = L_CODE_1
        Do ACCESS_AGQEND1
        If L_ADD_ENDORSEMENT = "Y" And
           agqmaster:um_uim_limit[2] <> 0 and
           agqmaster:non_stacked = "N" Then
            Do ADD_AGQEND1
        If ((L_ADD_ENDORSEMENT = "N" And
           agqmaster:um_uim_limit[2] = 0) or
           (l_add_endorsement = "N" and
           agqmaster:non_stacked <> "N")) Then
            Do DELETE_AGQEND1
        end

    L_RECORD_KEY = "ST"
    l_limit      = agqmaster:um_uim_limit[1]
    l_um_uim     = "UM"
    Access casstacked, Set casstacked:COMPANY_ID       = agqvehicle:COMPANY_ID,
                           casstacked:STATE            = agqvehicle:state,
                           casstacked:LINE_OF_BUSINESS = agqvehicle:line_of_business,
                           casstacked:um_uim           = l_um_uim,
                           casstacked:limit            = l_limit, Generic

    while casstacked:RECORD_KEY       = L_RECORD_KEY and
          casstacked:COMPANY_ID       = agqvehicle:COMPANY_ID and
          casstacked:STATE            = agqvehicle:state and
          casstacked:LINE_OF_BUSINESS = agqvehicle:line_of_business and
          casstacked:um_uim           = l_um_uim and
          casstacked:limit            = l_limit
        begin
        If agqvehicle:EFF_DATE >= casstacked:EFF_DATE Then
            Begin
            l_code_1 = casstacked:code
            End

        Next casstacked
        End

    if l_code_1 <> "" then
        begin
        L_CODE = L_CODE_1
        Do ACCESS_AGQEND1
        If L_ADD_ENDORSEMENT = "Y" And
           agqmaster:um_uim_limit[1] <> 0 and
           agqmaster:non_stacked = "S" Then
            Do ADD_AGQEND1
        If ((L_ADD_ENDORSEMENT = "N" And
           agqmaster:um_uim_limit[1] = 0) or
           (l_add_endorsement = "N" and
           agqmaster:non_stacked <> "S")) Then
            Do DELETE_AGQEND1
        end

    L_RECORD_KEY = "ST"
    l_limit      = agqmaster:um_uim_limit[2]
    l_um_uim     = "UIM"
    Access casstacked, Set casstacked:COMPANY_ID       = agqvehicle:COMPANY_ID,
                           casstacked:STATE            = agqvehicle:state,
                           casstacked:LINE_OF_BUSINESS = agqvehicle:line_of_business,
                           casstacked:um_uim           = l_um_uim,
                           casstacked:limit            = l_limit, Generic

    while casstacked:RECORD_KEY       = L_RECORD_KEY and
          casstacked:COMPANY_ID       = agqvehicle:COMPANY_ID and
          casstacked:STATE            = agqvehicle:state and
          casstacked:LINE_OF_BUSINESS = agqvehicle:line_of_business and
          casstacked:um_uim           = l_um_uim and
          casstacked:limit            = l_limit
        begin
        If agqvehicle:EFF_DATE >= casstacked:EFF_DATE Then
            Begin
            l_code_1 = casstacked:code
            End

        Next casstacked
        End

    if l_code_1 <> "" then
        begin
        L_CODE = L_CODE_1
        Do ACCESS_AGQEND1
        If L_ADD_ENDORSEMENT = "Y" And
           agqmaster:um_uim_limit[2] <> 0 and
           agqmaster:non_stacked = "S" Then
            Do ADD_AGQEND1
        If ((L_ADD_ENDORSEMENT = "N" And
           agqmaster:um_uim_limit[2] = 0) or
           (l_add_endorsement = "N" and
           agqmaster:non_stacked <> "S")) Then
            Do DELETE_AGQEND1
        end

    l_record_key = "NO"
    l_filler     = ""/raw
    access casnonowned, set casnonowned:company_id       = agqvehicle:company_id,
                            casnonowned:state            = agqvehicle:state,
                            casnonowned:line_of_business = agqvehicle:line_of_business,
                            casnonowned:filler           = l_filler, generic

    l_code_1 = ""
    while casnonowned:record_key       = l_record_key and
          casnonowned:company_id       = agqvehicle:company_id and
          casnonowned:state            = agqvehicle:state and
          casnonowned:line_of_business = agqvehicle:line_of_business and
          casnonowned:filler           = l_filler
            begin
            if agqvehicle:eff_date >= casnonowned:eff_date then
                begin
                l_code_1 = casnonowned:employee_insured_code
                l_code_2 = casnonowned:additional_insured_code

                end

            next casnonowned
            end

    if l_code_1 <> "" then
        begin
        L_CODE = l_code_1
        Do ACCESS_AGQEND1
        If L_ADD_ENDORSEMENT = "Y" And
           agqnonowned:EMPLOYEES_AS_INSUREDS = "Y" Then
            Do ADD_AGQEND1
        If ((L_ADD_ENDORSEMENT = "N" And
           agqnonowned:EMPLOYEES_AS_INSUREDS  = "N") or
           function = "DELETE") Then
            Do DELETE_AGQEND1
        end

    if l_code_2 <> "" then
        begin
        L_CODE = l_code_2
        Do ACCESS_AGQEND1
        If L_ADD_ENDORSEMENT = "Y" And
           agqnonowned:additional_insured = "Y" Then
            Do ADD_AGQEND1
        If ((L_ADD_ENDORSEMENT = "N" And
           agqnonowned:additional_insured = "N") or
           function = "DELETE") Then
            Do DELETE_AGQEND1
        end

    l_record_key = "HI"
    l_filler = ""/raw
    access cashired, set cashired:company_id       = agqvehicle:company_id,
                         cashired:state            = agqvehicle:state,
                         cashired:line_of_business = agqvehicle:line_of_business,
                         cashired:filler           = l_filler, generic

    l_code_1 = ""
    while cashired:record_key       = l_record_key and
          cashired:company_id       = agqvehicle:company_id and
          cashired:state            = agqvehicle:state and
          cashired:line_of_business = agqvehicle:line_of_business and
          cashired:filler           = l_filler
            begin
            if agqvehicle:eff_date >= cashired:eff_date then
                begin
                l_code_1 = cashired:physical_damage_code

                end

            next cashired
            end

    if l_code_1 <> "" then
        begin
        l_code = l_code_1
        Do ACCESS_AGQEND1
        If (L_ADD_ENDORSEMENT = "Y" And
           (agqnonowned:comp  = "Y" or
           agqnonowned:coll   = "Y")) Then
            Do ADD_AGQEND1
        If ((L_ADD_ENDORSEMENT = "N" And
           agqnonowned:comp  = "N" and
           agqnonowned:coll  = "N") or
           function = "DELETE") Then
            Do DELETE_AGQEND1
        end

    end
    function = "redisplay"

screen exit
if l_ca2001 = 1 then
    begin
    access agqvehicle_alias, set agqvehicle_alias:app_no     = agqvehicle:app_no,
                                 agqvehicle_alias:vehicle_no = agqvehicle:vehicle_no, generic

    if agqvehicle_alias:app_no     = agqvehicle:app_no and
       agqvehicle_alias:vehicle_no = agqvehicle:vehicle_no then
        begin
        change agqvehicle_alias
            begin
            agqvehicle_alias:additional_insured = "Y"
            end
        end
    end
else
    begin
    access agqvehicle_alias, set agqvehicle_alias:app_no     = agqvehicle:app_no,
                                 agqvehicle_alias:vehicle_no = agqvehicle:vehicle_no, generic

    if agqvehicle_alias:app_no     = agqvehicle:app_no and
       agqvehicle_alias:vehicle_no = agqvehicle:vehicle_no then
        begin
        change agqvehicle_alias
            begin
            agqvehicle_alias:additional_insured = "N"
            end
        end
    end

