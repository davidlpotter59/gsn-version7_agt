                agqnonowned:employees_as_insureds = "N"
            agqnonowned:nonowned_premium         = 0
            agqnonowned:hired_premium            = 0
            switch(agqmaster:if_any)
              case 1  : agqnonowned:if_any       = "Y"
              default : agqnonowned:if_any       = "N"
              end
            agqnonowned:FUNERAL_MEDICAL_PAYMENTS = agqmaster:funeral_medical_payments
            agqnonowned:state                    = agqvehicle:state
            agqnonowned:liability                = agqmaster:liability
            agqnonowned:mp[1]                    = "  "
            agqnonowned:mp[2]                    = "  "
            agqnonowned:hired_factor             = 0
            switch(agqmaster:nonowned_auto)
              case 1  : agqnonowned:nonowned_auto_applicable = "Y"
