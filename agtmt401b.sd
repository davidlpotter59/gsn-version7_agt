{
%%
      ____________               ____________                    ____________
%%

components

Fields
101     Begin
        if function one of "ADD", "NEW", "CHG", "CHANGE" then
            begin
            disable(calc_fn)
            enable(accept_fn)
            end
        else
            begin
            enable(calc_fn)
            disable(accept_fn)
            end

        if function one of "ADD", "NEW", "CHANGE", "CHG" then
            {
            if l_company_id = "FLEMINGTON" then
                begin
                l_current_web_page = "022webhelp_agtmt401b_scr_policy_opt_dbx_liab_limit.htm"
                do change_current_browser(l_server_name,l_current_web_page)
                end
            }

        access sfscompany,
             set sfscompany:company_id = agqvehicle:company_id, generic

        l_agent_no   = agqvehicle:agent_no
        Access agsdefault_cap,
             Set agsdefault_cap:COMPANY_ID       = agqvehicle:COMPANY_ID,
                 agsdefault_cap:state            = agqvehicle:state,
                 agsdefault_cap:agent_no         = agqvehicle:agent_no,
                 agsdefault_cap:LINE_OF_BUSINESS = agqvehicle:line_of_business, generic

        access sfsliability1_alias,
             set sfsliability1_alias:company_id       = agqvehicle:company_id,
                 sfsliability1_alias:state            = agqvehicle:state,
                 sfsliability1_alias:line_of_business = agqvehicle:line_of_business, generic
        End
        agqmaster:LIABILITY/default/mask="$$,$$$,$$$"/tabstop
        l_limit = agqmaster:liability

        if agqmaster:extend_hired_nonowned = "Y" or
           agqmaster:delete_hired_nonowned = "Y" then
            begin
            agqmaster:um_uim_limit[1] = 0
            agqmaster:um_uim_limit[2] = 0
            end

        access sfsliability1,
           set sfsliability1:company_id       = agqvehicle:company_id,
               sfsliability1:state            = agqvehicle:state,
               sfsliability1:line_of_business = agqvehicle:line_of_business,
               sfsliability1:limit            = l_limit, generic

        if agqmaster:liability <> sfsliability1:limit or
           sfsliability1:allow_agent_to_view = 0 then
            error "Invalid Liability Limit Entered"

        activehelp if sfsliability1_alias:allow_agent_to_view = 1 then
                      sfsliability1_alias:limit
                   else
                       null/width=15 ;
102     Begin
        access casumuim_alias,
           set casumuim_alias:company_id = agqvehicle:company_id,
               casumuim_alias:state = agqvehicle:state,
               casumuim_alias:line_of_business = agqvehicle:line_of_business, generic
        End
        agqmaster:UM_UIM_LIMIT[1]/default/mask="ZZ,ZZZ,ZZZ"/tabstop
        hide if agqmaster:extend_hired_nonowned = "Y"
        hide if agqmaster:delete_hired_nonowned = "Y"
        l_limit = agqmaster:um_uim_limit[1]

        access casumuim,
           set casumuim:company_id       = agqvehicle:company_id,
               casumuim:state            = agqvehicle:state,
               casumuim:line_of_business = agqvehicle:line_of_business,
               casumuim:limit            = l_limit, generic

        if agqmaster:um_uim_limit[1] <> casumuim:limit then                               --i put this in 7/22/2010
          {
            error "Invalid Limit Entered"
          }
        If agqmaster:UM_UIM_LIMIT[1] > agqmaster:LIABILITY Then
          {
            Error "Limit Can Not be Greater Than The Liability Limit Entered"
          }

        agqmaster:um_uim_limit[2] = agqmaster:um_uim_limit[1]

        activehelp if casumuim_alias:allow_agent_to_view = 1 then
                      casumuim_alias:limit
                   else
                       null/width=15 ;
102     agqmaster:um_uim_limit[1]/displayonly/mask="ZZ,ZZZ,ZZZ" ;

103     begin
        if function one of "ADD", "CHANGE", "CHG" then
            agqmaster:UM_UIM_LIMIT[2] = agqmaster:UM_UIM_LIMIT[1]

        access casumuim_alias, set casumuim_alias:company_id = agqvehicle:company_id,
                                   casumuim_alias:state = agqvehicle:state,
                                   casumuim_alias:line_of_business = agqvehicle:line_of_business, generic

        end
        agqmaster:um_uim_limit[2]/displayonly/mask="ZZ,ZZZ,ZZZ"
        hide if agsdefault_cap:different_um_uim one of 0
        l_limit = agqmaster:um_uim_limit[2]
        access casumuim, set casumuim:company_id = agqvehicle:company_id,
                             casumuim:state = agqvehicle:state,
                             casumuim:line_of_business = agqvehicle:line_of_business,
                             casumuim:limit = l_limit, generic

        if agqmaster:um_uim_limit[2] <> casumuim:limit then
            error "Invalid Limited Entered"

        activehelp if casumuim_alias:allow_agent_to_view = 1 then
                       casumuim_alias:limit
                   else
                       null/width=15 ;
103     agqmaster:um_uim_limit[2]/displayonly/mask="ZZ,ZZZ,ZZZ" ;

}

