              end
            agqnonowned:nonowned_partnership     = agqmaster:nonowned_partnership
            agqnonowned:nonowned_auto_service    = agqmaster:nonowned_auto_service
            agqnonowned:cost_of_hire_1           = 0
            agqnonowned:if_any_1                 = "N"
            agqnonowned:expansion                = ""/raw
            end
