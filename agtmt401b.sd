                       if agqvehicle:company_id = "FARMERS" then
                           l_screen = "agtmt401i"
                       else
                           l_screen = "agtmt401f"
                       function = "popup"
                       end ;
        "redisplay" display/noreturn "agtmt401b" ;
        "popup" display l_screen ;
