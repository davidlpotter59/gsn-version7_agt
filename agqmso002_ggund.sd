Procedure Definition

procedure check_for_warnings_scp
begin
l_wording_type = 1
access agsupdatewording, set agsupdatewording:company_id       = agqcontractor:company_id,
                             agsupdatewording:state            = agqcontractor:state,
                             agsupdatewording:line_of_business = agqcontractor:line_of_business,
                             agsupdatewording:wording_type     = l_wording_type, generic

l_comments_1 = trun(agsupdatewording:comments)

l_wording_type = 2
access agsupdatewording, set agsupdatewording:company_id       = agqcontractor:company_id,
                             agsupdatewording:state            = agqcontractor:state,
                             agsupdatewording:line_of_business = agqcontractor:line_of_business,
                             agsupdatewording:wording_type     = l_wording_type, generic

access agqclasscode, set agqclasscode:app_no = l_app_no, generic

l_comments_2 = trun(agsupdatewording:comments)

access agqapplication, set agqapplication:app_no = l_app_no, generic

l_first = "Y"
while agqapplication:app_no = l_app_no
        begin
        if agqapplication:highlight_if_yes = 1 and
           agqapplication:sub_code one of 1, 9 and
           agqapplication:answer = "N" then
            begin
            if l_first = "Y" then
                begin
                l_first = "N"
                l_comments = trun(l_comments_2) +
                             "<NL>" +
                             "<NL>" +
                             trun(agqapplication:literal)
                end
            else
                begin
                l_comments = trun(l_comments) +
                             "<NL>" +
                             trun(agqapplication:literal)
                end
            end

        if agqapplication:highlight_if_yes = 1 and
           agqapplication:sub_code one of 6, 7, 21, 23, 26 and
           agqapplication:answer = "Y" then
            begin
            if l_first = "Y" then
                begin
                l_first = "N"
                l_comments = trun(l_comments_2) +
                             "<NL>" +
                             "<NL>" +
                             trun(agqapplication:literal)
                end
            else
                begin
                l_comments = trun(l_comments) +
                             "<NL>" +
                             trun(agqapplication:literal)
                end
            end

        if agqapplication:highlight_if_yes = 1 and
           agqapplication:sub_code one of 14 and
           agqapplication:sub_contractors_25_percent = "Y" and
           agqapplication:answer = "Y" then
            begin
            if l_first = "Y" then
                begin
                l_first = "N"
                l_comments = trun(l_comments_2) +
                             "<NL>" +
                             "<NL>" +
                             trun(agqapplication:literal)
                end
            else
                begin
                l_comments = trun(l_comments) +
                             "<NL>" +
                             trun(agqapplication:literal)
                end
            end

        if agqapplication:highlight_if_yes = 1 and
           agqapplication:sub_code one of 27 and
           agqclasscode:class_code one of 2980 and
           agqapplication:final_hook_up = "Y" and
           agqapplication:answer = "Y" then
            begin
            if l_first = "Y" then
                begin
                l_first = "N"
                l_comments = trun(l_comments_2) +
                             "<NL>" +
                             "<NL>" +
                             trun(agqapplication:literal)
                end
            else
                begin
                l_comments = trun(l_comments) +
                             "<NL>" +
                             trun(agqapplication:literal)
                end
            end

        if agqapplication:highlight_if_yes = 1 and
           agqapplication:sub_code one of 28 and
           agqclasscode:class_code one of 2980 and
           agqapplication:answer = "Y" then
            begin
            if l_first = "Y" then
                begin
                l_first = "N"
                l_comments = trun(l_comments_2) +
                             "<NL>" +
                             "<NL>" +
                             trun(agqapplication:literal)
                end
            else
                begin
                l_comments = trun(l_comments) +
                             "<NL>" +
                             trun(agqapplication:literal)
                end
            end

        if agqapplication:highlight_if_yes = 1 and
           agqapplication:sub_code one of 29 and
           agqclasscode:class_code one of 1980, 5980 and
           agqapplication:answer = "Y" then
            begin
            if l_first = "Y" then
                begin
                l_first = "N"
                l_comments = trun(l_comments_2) +
                             "<NL>" +
                             "<NL>" +
                             trun(agqapplication:literal)
                end
            else
                begin
                l_comments = trun(l_comments) +
                             "<NL>" +
                             trun(agqapplication:literal)
                end
            end

        if agqapplication:highlight_if_yes = 1 and
           agqapplication:sub_code one of 30 and
           agqclasscode:class_code one of 2980, 2981, 3981, 5981 and
           agqapplication:answer = "Y" then
            begin
            if l_first = "Y" then
                begin
                l_first = "N"
                l_comments = trun(l_comments_2) +
                             "<NL>" +
                             "<NL>" +
                             trun(agqapplication:literal)
                end
            else
                begin
                l_comments = trun(l_comments) +
                             "<NL>" +
                             trun(agqapplication:literal)
                end
            end

        if agqapplication:highlight_if_yes = 1 and
           agqapplication:sub_code one of 31 and
           agqclasscode:class_code one of 2981, 3981, 3984 and
           agqapplication:answer = "Y" then
            begin
            if l_first = "Y" then
                begin
                l_first = "N"
                l_comments = trun(l_comments_2) +
