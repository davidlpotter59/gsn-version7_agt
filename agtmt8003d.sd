panel
properties
        {
        Scrollbarpolicy = asneeded
        Text            = "Robbery"
        }
{
%%

        Limit: __________    Plan: __     Safe/Vault Class: _____
Coverage Form: __

    Coverage           Actual Exposure   Factor      Multiplier
         Basic:           _________     _________     _________
         Add'l:           _________     _________     _________
 Gross Premium:                                       _________

  Gross       Total      Form       Terr       Net     Deductible    Adj                  Final
 Premium      Disc       Mult       Mult     Premium     Credit    Premium       RMF     Premium
 _________  _________  _________  _________  _________  _________  _________  _________  _________
                                                     Balance to Meet Minimum:            _________
                                                               Total Premium:            _________

%%

components

Fields
201     agqisocrime:LIMIT[1]
        if agqisocrime:limit[1] > 0 then
            do GET_PER_AMOUNT ;
202     begin
        if function = "ADD" then
            agqisocrime:plan = 1
        end
        agqisocrime:plan/default
        If agqisocrime:plan <> 1 Then
            Error 1262

        Activehelp CPSisoCRplan_alias:plan,
                   CPSisocrplan_alias:DESC ;
203     Begin
        If FUNCTION one of  "ADD" Then
            agqisocrime:SAFE_CLASS = CPSCRIMEDEFAULTS:SAFE_CLASS
        End
        agqisocrime:SAFE_CLASS/default/uppercase
--        If agqisocrime:SAFE_CLASS <> CPSISOCRSAFES_alias:SAFE_CLASS Then
  --            Error 1262

        agqisocrime:SAFE_CLASS_SUB_CODE = CPSISOCRSAFES:SUB_CODE

        Activehelp CPSISOCRSAFES_ALIAS:SAFE_CLASS/width=2/heading="CLASS",
                   cpsisocrsafes_alias:safe_vault/width=2/heading="TYPE",
                   cpsisocrsafes_alias:doors/width=40,
                   CPSISOCRSAFES_ALIAS:walls/width=75,
                   cpsisocrsafes_alias:labels[1]/width=100,
                   if cpsisocrsafes_alias:labels[2] <> "" then
                      cpsisocrsafes_alias:labels[2]
                   else
                      null,
                   if cpsisocrsafes_alias:labels[3] <> "" then
                      cpsisocrsafes_alias:labels[3] ;
301     agqisocrime:holdup_alarms
        if agqisocrime:holdup_alarms <> 5 and
           agqisocrime:holdup_alarms <> 6 then
            error 1262

         activehelp cpsisocrrobhelp_alias:grouptype/width=2,
                    cpsisocrrobhelp_alias:desc/width=50 ;
601     agqisocrime:money_exposure[1] ;
602     begin
        if function one of "ADD", "CHANGE" then
            begin
            if agqisocrime:money_exposure[1] > 0 then
                begin
                l_crime_type = "R"
                l_safe_class = "Z"
                l_group = agqisocrime:rate_group
                Do GET_ROBBERY
                end
            if l_factor1 = 0 then
                L_FACTOR1 = 1

            agqisocrime:base_factor_inside[1] = l_factor1
            end
        end
        agqisocrime:base_factor_inside[1]/displayonly;
603     begin
        if function one of "ADD", "CHANGE" then
            agqisocrime:crime_total[1] = (agqisocrime:base_factor_inside[1] * (agqisocrime:money_exposure[1] divide l_inside_per))
        end
        agqisocrime:crime_total[1]/displayonly ;
701     begin
        if function one of "ADD", "CHANGE" then
            agqisocrime:excess_exposure[1] = agqisocrime:limit[1] - agqisocrime:money_exposure[1]
        end
        agqisocrime:excess_exposure[1]/default ;
702     begin
        if function one of "ADD", "CHANGE" then
            begin
            if agqisocrime:excess_exposure[1] <> 0 then
                agqisocrime:base_factor_inside[2] = agqisocrime:base_factor_inside[1]
            else
                agqisocrime:base_factor_inside[2] = 1.00
            end
        end
        agqisocrime:base_factor_inside[2]/displayonly ;
703     begin
        if function one of "ADD", "CHANGE" then
            agqisocrime:crime_total[2] = (agqisocrime:base_factor_inside[2] * (agqisocrime:excess_exposure[1] divide l_inside_per))
        end
        agqisocrime:crime_total[2]/displayonly ;
801     begin
        if function one of "ADD", "CHANGE" then
            begin
            agqisocrime:gross_premium[1] = (agqisocrime:crime_total[1] +
                                            agqisocrime:crime_total[2])
            if agqisocrime:gross_premium[1] > 0 then
                begin
                l_safe_class = "Z"
                l_crime_type = "R"
                l_group_type = agqisocrime:holdup_alarms
                Do GET_FORM_MULTI
                agqisocrime:base_factor_inside[4] = l_factor1
                end
            end
        end
        agqisocrime:gross_premium[1]/displayonly ;  --multiplier total
1201    begin
        if function one of "ADD", "CHANGE" then
            do get_totals
        end
        agqisocrime:gross_premium[1]/displayonly ;
1202    begin
            l_deviation = 1.00
        end
        l_deviation/displayonly ;
1203    agqisocrime:base_factor_inside[4]/displayonly ;
1204    agqisocrime:territory_factor/displayonly ;
1205    agqisocrime:net_premium[1]/displayonly ;
1206    agqisocrime:deductible_premium[1]/displayonly ;
1207    agqisocrime:adjusted_rate[1]/displayonly ;
1208    agqisocrime:rmf_total[1]/displayonly ;
1209    agqisocrime:total_premium[1]/displayonly ;
1301    agqisocrime:minimum_premium[1]/displayonly ;
1401    agqisocrime:final_premium[1]/displayonly ;

}

