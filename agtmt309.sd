                                    l_line_of_business[4],
                                    l_remove_form[1],
                                    L_STATE[2],
                                    l_agent_no[4],
                                    l_liability_code[2] ;

        local string l_code[7],
                     l_code_1[7],
                     l_code_2[7],
                     l_system_id[15],
                     l_username[15],
                     l_add_endorsement[1],
                     l_Form_edition[18],
                     L_COMPANY_ID[10] ,
                     l_description[55] ;

        Access agqoperations,
           Set agqoperations:app_NO   = G_app_NO ,
               agqoperations:prem_no  = g_prem_no,
               agqoperations:build_no = g_build_no ;

        Include "stdkeys7.inc"

