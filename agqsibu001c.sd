  Writefile(L_FILE_NAME,"%n%s%s%s%d%s%s",l_app_no,"<033>&a2775h+0V",
                        "Effective:","<033>&a3250h+0V",agqgeneral:EFF_DATE,"<NL>",
                        "<NL>")
